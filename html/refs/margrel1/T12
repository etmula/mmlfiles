<div><span class="kw">theorem </span><a NAME="T12"><span class="comment"><font color="firebrick">:: MARGREL1:12</font></span><br/></a><div class="add"> for <font color="Olive" title="b1">v</font>, <font color="Olive" title="b2">w</font> being   <a href="xboolean.html#V1" title="XBOOLEAN:attr.1">boolean</a>   <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a>  holds <br/> ( ( <font color="Olive" title="b1">v</font> <a href="xboolean.html#K4" title="XBOOLEAN:func.4">'&amp;'</a> <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  implies ( <font color="Olive" title="b1">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  &amp; <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  ) ) &amp; ( <font color="Olive" title="b1">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  &amp; <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  implies <font color="Olive" title="b1">v</font> <a href="xboolean.html#K4" title="XBOOLEAN:func.4">'&amp;'</a> <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  ) &amp; (  not <font color="Olive" title="b1">v</font> <a href="xboolean.html#K4" title="XBOOLEAN:func.4">'&amp;'</a> <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  or <font color="Olive" title="b1">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  or <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  ) &amp; ( ( <font color="Olive" title="b1">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  or <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  ) implies <font color="Olive" title="b1">v</font> <a href="xboolean.html#K4" title="XBOOLEAN:func.4">'&amp;'</a> <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  ) ) <span class="kw">by</span> <span class="lab"><a class="ref" href="xboolean.html#T101" onmouseover="rs('xboolean/T101')" onmouseout="rh()">XBOOLEAN:101</a>, <a class="ref" href="xboolean.html#T140" onmouseover="rs('xboolean/T140')" onmouseout="rh()">XBOOLEAN:140</a></span>;<br/></div></div>
