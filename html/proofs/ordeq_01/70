<span class="kw">hence </span><a NAME="E23:70"/>
( <font color="Maroon" title="c6">f0</font> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <span class="p1"><a href="integra5.html#K3" title="INTEGRA5:func.3">['</a><span class="default"><font color="Maroon" title="c2">a</font>,<font color="Maroon" title="c4">t</font></span><a href="integra5.html#K3" title="INTEGRA5:func.3">']</a></span> is  <a href="fcont_1.html#V1" title="FCONT_1:attr.1">continuous</a>  &amp; <font color="Maroon" title="c6">f0</font> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <span class="p1"><a href="integra5.html#K3" title="INTEGRA5:func.3">['</a><span class="default"><font color="Maroon" title="c2">a</font>,<font color="Maroon" title="c4">t</font></span><a href="integra5.html#K3" title="INTEGRA5:func.3">']</a></span> is  <a href="comseq_2.html#V1" title="COMSEQ_2:attr.1">bounded</a>  &amp; <font color="Maroon" title="c6">f0</font> <a href="integra5.html#R1" title="INTEGRA5:pred.1">is_integrable_on</a> <span class="p1"><a href="integra5.html#K3" title="INTEGRA5:func.3">['</a><span class="default"><font color="Maroon" title="c2">a</font>,<font color="Maroon" title="c4">t</font></span><a href="integra5.html#K3" title="INTEGRA5:func.3">']</a></span> &amp;  <a href="integra5.html#K4" title="INTEGRA5:func.4">integral</a> (<font color="Maroon" title="c6">f0</font>,<font color="Maroon" title="c2">a</font>,<font color="Maroon" title="c4">t</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c3">r</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p4">(<span class="default"><font color="Maroon" title="c4">t</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c2">a</font></span>)</span></span>)</span> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <span class="p3">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span> <a href="newton.html#K2" title="NEWTON:func.2">!</a></span>)</span></span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c5">L</font> )
