<span class="kw">thus </span><a NAME="E20:44"/>
( <span class="p1">(<span class="default"><a href="facirc_1.html#K5" title="FACIRC_1:func.5">Following</a> (<font color="Maroon" title="c4">s</font>,2)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2"><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">&lt;*</a><span class="default"><font color="Maroon" title="c1">x</font>,<font color="Maroon" title="c2">y</font></span><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">*&gt;</a></span>,<a href="facirc_1.html#K3" title="FACIRC_1:func.3">'&amp;'</a></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">a1</font> <a href="margrel1.html#K9" title="MARGREL1:func.9">'&amp;'</a> <font color="Maroon" title="c6">a2</font> &amp; <span class="p1">(<span class="default"><a href="facirc_1.html#K5" title="FACIRC_1:func.5">Following</a> (<font color="Maroon" title="c4">s</font>,2)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2"><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">&lt;*</a><span class="default"><font color="Maroon" title="c2">y</font>,<font color="Maroon" title="c3">c</font></span><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">*&gt;</a></span>,<a href="facirc_1.html#K3" title="FACIRC_1:func.3">'&amp;'</a></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">a2</font> <a href="margrel1.html#K9" title="MARGREL1:func.9">'&amp;'</a> <font color="Maroon" title="c7">a3</font> &amp; <span class="p1">(<span class="default"><a href="facirc_1.html#K5" title="FACIRC_1:func.5">Following</a> (<font color="Maroon" title="c4">s</font>,2)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2"><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">&lt;*</a><span class="default"><font color="Maroon" title="c3">c</font>,<font color="Maroon" title="c1">x</font></span><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">*&gt;</a></span>,<a href="facirc_1.html#K3" title="FACIRC_1:func.3">'&amp;'</a></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c7">a3</font> <a href="margrel1.html#K9" title="MARGREL1:func.9">'&amp;'</a> <font color="Maroon" title="c5">a1</font> )
