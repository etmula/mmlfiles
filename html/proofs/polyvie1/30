<span class="kw">hence </span><a NAME="E5:30"/>
<span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="uproots.html#K7" title="UPROOTS:func.7">BRoots</a> <span class="p3">(<span class="default"><span class="p4"><a href="polynom5.html#K5" title="POLYNOM5:func.5">&lt;%</a><span class="default"><font color="Maroon" title="c3">a</font>,<font color="Maroon" title="c4">b</font></span><a href="polynom5.html#K5" title="POLYNOM5:func.5">%&gt;</a></span> <a href="polynom3.html#K12" title="POLYNOM3:func.12">*'</a> <font color="Maroon" title="c2">p</font></span>)</span></span>)</span> <a href="polyvie1.html#K1" title="POLYVIE1:func.1">(++)</a> <font color="Maroon" title="c7">E</font></span>)</span> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <font color="Maroon" title="c8">P</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="uproots.html#K7" title="UPROOTS:func.7">BRoots</a> <span class="p2">(<span class="default"><span class="p3"><a href="polynom5.html#K5" title="POLYNOM5:func.5">&lt;%</a><span class="default"><font color="Maroon" title="c3">a</font>,<font color="Maroon" title="c4">b</font></span><a href="polynom5.html#K5" title="POLYNOM5:func.5">%&gt;</a></span> <a href="polynom3.html#K12" title="POLYNOM3:func.12">*'</a> <font color="Maroon" title="c2">p</font></span>)</span></span>)</span> <a href="polyvie1.html#K1" title="POLYVIE1:func.1">(++)</a> <span class="p1">(<span class="default"><font color="Maroon" title="c7">E</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <font color="Maroon" title="c8">P</font></span>)</span>
