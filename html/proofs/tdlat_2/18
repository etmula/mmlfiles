 <a href="tdlat_2.html#K1" title="TDLAT_2:func.1">Int</a> <font color="Maroon" title="c2">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">A</font> where <font color="Olive" title="b1">A</font> is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">T</font> :  ex <font color="Olive" title="b2">B</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">T</font> st <br/>( <font color="Olive" title="b1">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="tops_1.html#K1" title="TOPS_1:func.1">Int</a> <font color="Olive" title="b2">B</font> &amp; <font color="Olive" title="b2">B</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> ) </span> } </span> 
