<span class="kw">let </span><font color="Maroon" title="c1">p</font> be   <a href="polyform.html#NM2" title="POLYFORM:NM.2">polyhedron</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c1">p</font> is  <a href="polyform.html#V5" title="POLYFORM:attr.5">simply-connected</a>  &amp;  <a href="polyform.html#K4" title="POLYFORM:func.4">dim</a> <font color="Maroon" title="c1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 2 implies  <a href="polyform.html#K9" title="POLYFORM:func.9">num-vertices</a> <font color="Maroon" title="c1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polyform.html#K10" title="POLYFORM:func.10">num-edges</a> <font color="Maroon" title="c1">p</font> )</span><br/>

<span class="kw">set </span><font color="Maroon" title="c2">s</font> = <span class="p1">(<span class="default"><a href="polyform.html#K8" title="POLYFORM:func.8">num-polytopes</a> (<font color="Maroon" title="c1">p</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>)</span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p1">(<span class="default"><a href="polyform.html#K8" title="POLYFORM:func.8">num-polytopes</a> (<font color="Maroon" title="c1">p</font>,1)</span>)</span>;<br/>
