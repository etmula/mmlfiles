 <a href="polyform.html#K4" title="POLYFORM:func.4">dim</a> <font color="Maroon" title="c1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 2
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="polyform.html#K9" title="POLYFORM:func.9">num-vertices</a> <font color="Maroon" title="c1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polyform.html#K10" title="POLYFORM:func.10">num-edges</a> <font color="Maroon" title="c1">p</font></span><br/>

<a NAME="E4:122"/><span class="kw">then </span><span class="lab"><font color="Green" title="E98">A3</font></span>: 
