<span class="kw">hence </span><a NAME="E21:70"/>
 for <font color="Olive" title="b1">t</font> being    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of  <a href="euclid.html#K1" title="EUCLID:func.1">REAL</a> <font color="Maroon" title="c1">n</font> holds <br/> ( <font color="Olive" title="b1">t</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c3">y</font> iff  for <font color="Olive" title="b2">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> holds <br/><font color="Olive" title="b1">t</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"><a href="rcomp_1.html#K3" title="RCOMP_1:func.3">[.</a><span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c7">f2</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b2">i</font></span>)</span> <a href="srings_5.html#K2" title="SRINGS_5:func.2">`1</a></span>)</span>,<span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c7">f2</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b2">i</font></span>)</span> <a href="srings_5.html#K3" title="SRINGS_5:func.3">`2</a></span>)</span></span><a href="rcomp_1.html#K3" title="RCOMP_1:func.3">.[</a></span> )
