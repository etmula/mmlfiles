<a NAME="E2:51"/><span class="kw">then </span><span class="lab"><font color="Green" title="E25">A6</font></span>: 
 for <font color="Olive" title="b1">y</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font> holds  <font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b1">y</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>2</sub></font>(<font color="Olive" title="b1">y</font>)
 
;<br/>
<span class="kw">thus </span><a NAME="E3:51"/>
<span class="p1"> { <span class="default"> <font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b1">y</font>) where <font color="Olive" title="b1">y</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">y</font>] </span> } </span>  <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <font color="Maroon">H<sub>2</sub></font>(<font color="Olive" title="b1">y</font>) where <font color="Olive" title="b1">y</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">y</font>] </span> } </span> 
