</div><span class="kw">end;</span></div>
<span class="kw">hence </span><a NAME="E3:54_1"/>
 <a href="yellow_0.html#K1" title="YELLOW_0:func.1">"\/"</a> (<span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c2">R</font> <a href="lfuzzy_0.html#K7" title="LFUZZY_0:func.7">.</a> <span class="p4"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Maroon" title="c4">x</font>,<font color="Olive" title="b1">y</font></span><a href="domain_1.html#K1" title="DOMAIN_1:func.1">]</a></span></span>)</span> <a href="lattice3.html#K12" title="LATTICE3:func.12">"/\"</a> <span class="p3">(<span class="default"><font color="Maroon" title="c7">r</font> <a href="lfuzzy_0.html#K9" title="LFUZZY_0:func.9">.</a> <span class="p4"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Olive" title="b1">y</font>,<font color="Maroon" title="c5">z</font></span><a href="domain_1.html#K1" title="DOMAIN_1:func.1">]</a></span></span>)</span></span>)</span> where <font color="Olive" title="b1">y</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font> : verum </span> } </span> ,<span class="p1">(<span class="default"><a href="lfuzzy_0.html#K1" title="LFUZZY_0:func.1">RealPoset</a> <span class="p2"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,1</span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span></span>)</span>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="yellow_0.html#K1" title="YELLOW_0:func.1">"\/"</a> (<span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c2">R</font> <a href="lfuzzy_0.html#K7" title="LFUZZY_0:func.7">.</a> <span class="p4"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Maroon" t