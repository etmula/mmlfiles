 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<a NAME="E2:54"/><span class="kw">then </span><span class="lab"><font color="Green" title="E28">A2</font></span>: 
 for <font color="Olive" title="b1">r</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="lfuzzy_0.html#K4" title="LFUZZY_0:func.4">FuzzyLattice</a> <span class="p2"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c1">X</font>,<font color="Maroon" title="c1">X</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span></span>)</span>  st <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">r</font>] holds <br/><font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b1">r</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>2</sub></font>(<font color="Olive" title="b1">r</font>)
 
;<br/>
<span class="kw">thus </span><a NAME="E3:54"/>
<span class="p1"> { <span class="default"> <font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b1">r</font>) where <font color="Olive" title="b1">r</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="lfuzzy_0.html#K4" title="LFUZZY_0:func.4">FuzzyLattice</a> <span class="p2"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c1">X</font>,<font color="Maroon" title="c1">X</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span></span>)</span> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">r</font>] </span> } </span>  <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <font color="Maroon">H<sub>2</sub></font>(<font color="Olive" title="b1">r</font>) where <font color="Olive" title="b1">r</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="lfuzzy_0.html#K4" title="LFUZZY_0:func.4">FuzzyLattice</a> <span class="p2"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c1">X</font>,<font color="Maroon" title="c1">X</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span></span>)</span> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">r</font>] </span> } </span> 
 <span class="kw">from</span> <span class="lab"><a class="ref" href="fraenkel.html#S6" onmouseover="rs('fraenkel/S6')" onmouseout="rh()">FRAENKEL:sch 6</a>(<a class="txt" href="#E2:54"><span class="lab"><font color="Green" title="E28">A2</font></span></a>);</span> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


