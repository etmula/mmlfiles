<span class="kw">let </span><font color="Maroon" title="c3">x</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c3">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="yellow_0.html#NK3" title="YELLOW_0:NK.3">sup</a> <span class="p1">(<span class="default"><a href="waybel_3.html#K1" title="WAYBEL_3:func.1">waybelow</a> <font color="Maroon" title="c3">x</font></span>)</span></span><br/>

<a NAME="E1:102_2"/>
<font color="Maroon" title="c3">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="yellow_0.html#NK3" title="YELLOW_0:NK.3">sup</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">L</font> <a href="waybel_4.html#K1" title="WAYBEL_4:func.1">-waybelow</a></span>)</span> <a href="waybel_4.html#K5" title="WAYBEL_4:func.5">-below</a> <font color="Maroon" title="c3">x</font></span>)</span>
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:102"><span class="lab"><font color="Green" title="E60">A5</font></span></a>, <a class="ref" href="waybel_4.html#D17" target="_self" onmouseover="rs('waybel_4/D17')" onmouseout="rh()">Def17</a></span>;<br/>
<span class="kw">hence </span><a NAME="E2:102_2"/>
<font color="Maroon" title="c3">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="yellow_0.html#NK3" title="YELLOW_0:NK.3">sup</a> <span class="p1">(<span class="default"><a href="waybel_3.html#K1" title="WAYBEL_3:func.1">waybelow</a> <font color="Maroon" title="c3">x</font></span>)</span>
 <span class="kw">by</span> <span class="lab"><a class="ref" href="waybel_4.html#T40" target="_self" onmouseover="rs('waybel_4/T40')" onmouseout="rh()">Th40</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<a NAME="E4:102"/><span class="kw">then </span>
