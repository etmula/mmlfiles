<span class="p1">(<span class="default"><a href="waybel_4.html#K10" title="WAYBEL_4:func.10">Map2Rel</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <span class="p1">(<span class="default"><a href="waybel_4.html#K9" title="WAYBEL_4:func.9">Rel2Map</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="funct_2.html#R1" title="FUNCT_2:pred.1">=</a>  <a href="partfun1.html#K6" title="PARTFUN1:func.6">id</a> <span class="p1">(<span class="default"><a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <span class="p2">(<span class="default"><a href="waybel_4.html#K9" title="WAYBEL_4:func.9">Rel2Map</a> <font color="Maroon" title="c1">L</font></span>)</span></span>)</span>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:70"><span class="lab"><font color="Green" title="E40">A1</font></span></a>, <a class="txt" href="#E4:70"><span class="lab"><font color="Green" title="E41">A2</font></span></a>, <a class="ref" href="funct_1.html#T2" onmouseover="rs('funct_1/T2')" onmouseout="rh()">FUNCT_1:2</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


