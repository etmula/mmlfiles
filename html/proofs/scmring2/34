<span class="kw">let </span><font color="Maroon" title="c9">x</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <span class="p1">(<span class="default"> the   <a href="scmring1.html#NM1" title="SCMRING1:NM.1">SCM-State</a> of <font color="Maroon" title="c1">R</font> <a href="funct_4.html#K1" title="FUNCT_4:func.1">+*</a> <span class="p2">(<span class="default"><a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> <a href="funcop_1.html#K17" title="FUNCOP_1:func.17">.--&gt;</a> <span class="p3">(<span class="default"><font color="Maroon" title="c3">i1</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span></span>)</span> implies <span class="p1">(<span class="default"> the   <a href="scmring1.html#NM1" title="SCMRING1:NM.1">SCM-State</a> of <font color="Maroon" title="c1">R</font> <a href="funct_4.html#K1" title="FUNCT_4:func.1">+*</a> <span class="p2">(<span class="default"><a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> <a href="funcop_1.html#K17" title="FUNCOP_1:func.17">.--&gt;</a> <span class="p3">(<span class="default"><font color="Maroon" title="c3">i1</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c9">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1">(<span class="default"><a href="memstr_0.html#K2" title="MEMSTR_0:func.2">the_Values_of</a> <span class="p2">(<span class="default"><a href="scmring2.html#K1" title="SCMRING2:func.1">SCM</a> <font color=