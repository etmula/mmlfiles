<span class="kw">thus </span><a NAME="E12:12"/>
(  <a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Maroon" title="c3">g</font></span>)</span>,<font color="Maroon" title="c1">A</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<font color="Maroon" title="c2">f</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1">(<span class="default"><a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<font color="Maroon" title="c3">g</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> &amp;  <a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="valued_1.html#K47" title="VALUED_1:func.47">-</a> <font color="Maroon" title="c3">g</font></span>)</span>,<font color="Maroon" title="c1">A</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<font color="Maroon" title="c2">f</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p1">(<span class="default"><a href="integra5.html#K2" title="INTEGRA5:func.2">integral</a> (<font color="Maroon" title="c3">g</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> )
