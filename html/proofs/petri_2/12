 for <font color="Olive" title="b1">x</font> being    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a>   st <font color="Olive" title="b1">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="petri_2.html#K2" title="PETRI_2:func.2">thin_cylinders</a> (<font color="Maroon" title="c2">A2</font>,<font color="Maroon" title="c4">B2</font>) holds <br/> ex <font color="Olive" title="b2">Bo2</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <font color="Maroon" title="c4">B2</font> ex <font color="Olive" title="b3">Bo1</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <font color="Maroon" title="c3">B1</font> ex <font color="Olive" title="b4">yo1</font> being   <a href="funct_2.html#NM1" title="FUNCT_2:NM.1">Function</a> of <font color="Olive" title="b3">Bo1</font>,<font color="Maroon" title="c1">A1</font> ex <font color="Olive" title="b5">yo2</font> being   <a href="funct_2.html#NM1" title="FUNCT_2:NM.1">Function</a> of <font color="Olive" title="b2">Bo2</font>,<font color="Maroon" title="c2">A2</font> st <br/>( <font color="Olive" title="b3">Bo1</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  &amp; <font color="Olive" title="b2">Bo2</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  &amp; <font color="Olive" title="b3">Bo1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">B1</font> <a href="subset_1.html#K9" title="SUBSET_1:func.9">/\</a> <font color="Olive" title="b2">Bo2</font></span>)</span> <a href="subset_1.html#K8" title="SUBSET_1:func.8">/\</a> <span class="p1">(<span class="default"><font color="Olive" title="b5">yo2</font> <a href="relat_1.html#K8" title="RELAT_1:func.8">"</a> <font color="Maroon" title="c1">A1</font></span>)</span> &amp; <font color="Olive" title="b4">yo1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b5">yo2</font> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <font color="Olive" title="b3">Bo1</font> &amp; <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="petri_2.html#K1" title="PETRI_2:func.1">cylinder0</a> (<font color="Maroon" title="c2">A2</font>,<font color="Maroon" title="c4">B2</font>,<font color="Olive" title="b2">Bo2</font>,<font color="Olive" title="b5">yo2</font>) &amp; <font color="Maroon" title="c5">G</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="petri_2.html#K1" title="PETRI_2:func.1">cylinder0</a> (<font color="Maroon" title="c1">A1</font>,<font color="Maroon" title="c3">B1</font>,<font color="Olive" title="b3">Bo1</font>,<font color="Olive" title="b4">yo1</font>) )
