<span class="kw">hence </span><a NAME="E6:59_1_1"/>
<span class="p1"><a href="tarski.html#K2" title="TARSKI:func.2">{</a><span class="default"><span class="p2"> { <span class="default"> <font color="Olive" title="b1">x</font> where <font color="Olive" title="b1">x</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">Y</font> : <font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  </span> } </span> ,<span class="p2"> { <span class="default"> <font color="Olive" title="b1">x9</font> where <font color="Olive" title="b1">x9</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">Y</font> : <font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">x9</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  </span> } </span> </span><a href="tarski.html#K2" title="TARSKI:func.2">}</a></span> <a href="subset_1.html#K6" title="SUBSET_1:func.6">\</a> <span class="p1"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span> is    <a href="eqrel_1.html#M1" title="EQREL_1:mode.1">a_partition</a> of <font color="Maroon" title="c1">Y</font>
