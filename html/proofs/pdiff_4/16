<span class="kw">end;</span></div>

<span class="kw">given </span><font color="Maroon" title="c7">x1</font>, <font color="Maroon" title="c8">y1</font>, <font color="Maroon" title="c9">z1</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a><span class="kw"> such that </span><a NAME="E4:16"/><span class="lab"><font color="Green" title="E18">A2</font></span>: 
( <font color="Maroon" title="c5">u</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">&lt;*</a><span class="default"><font color="Maroon" title="c7">x1</font>,<font color="Maroon" title="c8">y1</font>,<font color="Maroon" title="c9">z1</font></span><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">*&gt;</a></span> &amp;  ex <font color="Olive" title="b1">N</font> being    <a href="rcomp_1.html#M1" title="RCOMP_1:mode.1">Neighbourhood</a> of <font color="Maroon" title="c7">x1</font> st <br/>( <font color="Olive" title="b1">N</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><a href="pdiff_2.html#K1" title="PDIFF_2:func.1">SVF1</a> (1,<font color="Maroon" title="c6">f</font>,<font color="Maroon" title="c5">u</font>)</span>)</span> &amp;  ex <font color="Olive" title="b2">L</font> being   <a href="fdiff_1.html#NM2" title="FDIFF_1:NM.2">LinearFunc</a> ex <font color="Olive" title="b3">R</font> being   <a href="fdiff_1.html#NM1" title="FDIFF_1:NM.1">RestFunc</a> st <br/>( <font color="Maroon" title="c4">r</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">L</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> 1 &amp; (  for <font color="Olive" title="b4">x</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st <font color="Olive" title="b4">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Olive" title="b1">N</font> holds <br/><span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="pdiff_2.html#K1" title="PDIFF_2:func.1">SVF1</a> (1,<font color="Maroon" title="c6">f</font>,<font color="Maroon" title="c5">u</font>)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b4">x</font></span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="pdiff_2.html#K1" title="PDIFF_2:func.1">SVF1</a> (1,<font color="Maroon" title="c6">f</font>,<font color="Maroon" title="c5">u</font>)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">x1</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Olive" title="b2">L</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b4">x</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c7">x1</font></span>)</span></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1">(<span class="default"><font color="Olive" title="b3">R</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b4">x</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c7">x1</font></span>)</span></span>)</span> ) ) ) )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c4">r</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="pdiff_1.html#K10" title="PDIFF_1:func.10">partdiff</a> (<font color="Maroon" title="c6">f</font>,<font color="Maroon" title="c5">u</font>,1)</span><br/>

