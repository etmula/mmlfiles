<span class="kw">hence </span><a NAME="E22:12"/>
( ( <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  implies <font color="Maroon" title="c1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="rsspace3.html#K3" title="RSSPACE3:func.3">l1_Space</a> ) &amp; ( <font color="Maroon" title="c1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="rsspace3.html#K3" title="RSSPACE3:func.3">l1_Space</a> implies <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  ) &amp;  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> &amp; <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">x</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c2">y</font></span>)</span></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c2">y</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> &amp; <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c3">a</font> <a href="rlvect_1.html#K1" title="RLVECT_1:func.1">*</a> <font color="Maroon" title="c1">x</font></span>)</span></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c3">a</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> )
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E14:12"><span class="lab"><font color="Green" title="E20">A11</font></span></a>, <a class="txt" href="#E12:12"><span class="lab"><font color="Green" title="E18">A8</font></span></a>, <a class="txt" href="#E1:12"><span class="lab"><font color="Green" title="E11">A1</font></span></a>, <a class="txt" href="#E20:12"><span class="lab"><font color="Green" title="E24">A17</font></span></a>, <a class="txt" href="#E15:12"><span class="lab"><font color="Green" title="E21">A14</font></span></a>, <a class="txt" href="#E13:12"><span class="lab"><font color="Green" title="E19">A10</font></span></a>, <a class="txt" href="#E7:12"><span class="lab"><font color="Green" title="E16">A6</font></span></a>, <a class="txt" href="#E11:12"><span class="lab"><font color="Green" title="E17">A7</font></span></a>, <a class="ref" href="series_1.html#T7" onmouseover="rs('series_1/T7')" onmouseout="rh()">SERIES_1:7</a>, <a class="ref" href="series_1.html#T18" onmouseover="rs('series_1/T18')" onmouseout="rh()">SERIES_1:18</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


