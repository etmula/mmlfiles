( <font color="Maroon" title="c5">M4</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  &amp; <font color="Maroon" title="c3">M1</font> <a href="matrix_3.html#K3" title="MATRIX_3:func.3">+</a> <span class="p1">(<span class="default"><a href="matrix_1.html#K3" title="MATRIX_1:func.3">1.</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c5">M4</font> <a href="matrix_6.html#K1" title="MATRIX_6:func.1">~</a></span>)</span> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <span class="p2">(<span class="default"><font color="Maroon" title="c4">M2</font> <a href="matrix_3.html#K3" title="MATRIX_3:func.3">+</a> <span class="p3">(<span class="default"><a href="matrix_1.html#K3" title="MATRIX_1:func.3">1.</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>)</span>)</span></span>)</span></span>)</span> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Maroon" title="c5">M4</font> )
