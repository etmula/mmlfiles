<span class="kw">let </span><font color="Maroon" title="c4">k</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c4">k</font>] implies <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1] )</span><br/>

