 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">T</font> is  <a href="bintree1.html#V1" title="BINTREE1:attr.1">binary</a> </span><br/>

<a NAME="E2:8"/><span class="kw">then </span>
 for <font color="Olive" title="b1">t</font> being    <a href="trees_1.html#M1" title="TREES_1:mode.1">Element</a> of <font color="Maroon" title="c1">T</font>  st  not <font color="Olive" title="b1">t</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="trees_1.html#K3" title="TREES_1:func.3">Leaves</a> <font color="Maroon" title="c1">T</font> holds <br/> <a href="trees_2.html#K1" title="TREES_2:func.1">succ</a> <font color="Olive" title="b1">t</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="domain_1.html#K7" title="DOMAIN_1:func.7">{</a><span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b1">t</font> <a href="finseq_1.html#K8" title="FINSEQ_1:func.8">^</a> <span class="p3"><a href="binarith.html#K10" title="BINARITH:func.10">&lt;*</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="binarith.html#K10" title="BINARITH:func.10">*&gt;</a></span></span>)</span>,<span class="p2">(<span class="default"><font color="Olive" title="b1">t</font> <a href="finseq_1.html#K8" title="FINSEQ_1:func.8">^</a> <span class="p3"><a href="binarith.html#K10" title="BINARITH:func.10">&lt;*</a><span class="default">1</span><a href="binarith.html#K10" title="BINARITH:func.10">*&gt;</a></span></span>)</span></span><a href="domain_1.html#K7" title="DOMAIN_1:func.7">}</a></span>
 
