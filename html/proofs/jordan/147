<span class="kw">set </span><font color="Maroon" title="c26">F</font> = <span class="p1"> { <span class="default"> <font color="Olive" title="b1">B</font> where <font color="Olive" title="b1">B</font> is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="euclid.html#K15" title="EUCLID:func.15">TOP-REAL</a> 2</span>)</span> : <font color="Olive" title="b1">B</font> <a href="jordan2c.html#R1" title="JORDAN2C:pred.1">is_inside_component_of</a> <font color="Maroon" title="c22">C</font> </span> } </span> ;<br/>
<span class="kw">let </span><font color="Maroon" title="c27">q</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="tarski.html#D3" onmouseover="rs('tarski/D3')" onmouseout="rh()">TARSKI:def 3</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  not <font color="Maroon" title="c27">q</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="jordan2c.html#K1" title="JORDAN2C:func.1">BDD</a> <font color="Maroon" title="c22">C</font> or <font color="Maroon" title="c27">q</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c25">Ux</font> )</span><br/>

