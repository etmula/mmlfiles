 for <font color="Olive">b<sub>1</sub></font> being    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a>  holds <br/> ( <font color="Olive">b<sub>1</sub></font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="exchsort.html#K6" title="EXCHSORT:func.6">inversions</a> <font color="Maroon" title="c2">R</font> iff <font color="Olive">b<sub>1</sub></font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <span class="p2"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Olive" title="b2">a</font>,<font color="Olive" title="b3">b</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> where <font color="Olive" title="b2">a</font>, <font color="Olive" title="b3">b</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c2">R</font> : ( <font color="Olive" title="b2">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b3">b</font> &amp; <font color="Maroon" title="c2">R</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b2">a</font> <a href="orders_2.html#NR4" title="ORDERS_2:NR.4">&gt;</a> <font color="Maroon" title="c2">R</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b3">b</font> ) </span> } </span>  )
