(  <a href="laplace.html#K2" title="LAPLACE:func.2">Delete</a> (<span class="p1">(<span class="default"><a href="matrix13.html#K7" title="MATRIX13:func.7">EqSegm</a> (<font color="Maroon" title="c2">M</font>,<font color="Maroon" title="c3">P1</font>,<font color="Maroon" title="c4">Q1</font>)</span>)</span>,<font color="Maroon" title="c5">i</font>,<font color="Maroon" title="c6">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix13.html#K7" title="MATRIX13:func.7">EqSegm</a> (<font color="Maroon" title="c2">M</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c3">P1</font> <a href="subset_1.html#K7" title="SUBSET_1:func.7">\</a> <span class="p2"><a href="matrix13.html#K3" title="MATRIX13:func.3">{</a><span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="matrix13.html#K5" title="MATRIX13:func.5">Sgm</a> <font color="Maroon" title="c3">P1</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c5">i</font></span>)</span></span><a href="matrix13.html#K3" title="MATRIX13:func.3">}</a></span></span>)</span>,<span class="p1">(<span class="default"><font color="Maroon" title="c4">Q1</font> <a href="subset_1.html#K7" title="SUBSET_1:func.7">\</a> <span class="p2"><a href="matrix13.html#K3" title="MATRIX13:func.3">{</a><span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="matrix13.html#K5" title="MATRIX13:func.5">Sgm</a> <font color="Maroon" title="c4">Q1</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">j</font></span>)</span></span><a href="matrix13.html#K3" title="MATRIX13:func.3">}</a></span></span>)</span>) &amp;  <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">P1</font> <a href="subset_1.html#K7" title="SUBSET_1:func.7">\<