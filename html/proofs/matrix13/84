 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c5">m</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1]</span><br/>

<span class="kw">let </span><font color="Maroon" title="c6">k</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c6">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">m</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 &amp; <font color="Maroon" title="c6">k</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c1">n</font> implies  ex <font color="Olive" title="b1">aM</font> being   <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of <font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c2">K</font> st <br/>(  <a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <font color="Olive" title="b1">aM</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="group_1.html#K4" title="GROUP_1:func.4">power</a> <font color="Maroon" title="c2">K</font></span>)</span> <a href="laplace.html#K1" title="LAPLACE:func.1">.</a> (<font color="Maroon" title="c3">a</font>,<font color="Maroon" title="c6">k</font>)</span>)</span> <a href="group_1.html#K8" title="GROUP_1:func.8">*</a> <span class="p1">(<span class="default"><a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <font color="Maroon" title="c4">M</font></span>)</span> &amp; (  for <font color="Olive" title="b2">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> holds <br/>( ( <font color="Olive" title="b2">i</font> <a href="xxreal_