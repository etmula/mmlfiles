<span class="kw">hence </span><a NAME="E25:20"/>
( <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c9">S</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="nat_1.html#K3" title="NAT_1:func.3">min*</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">N</font> where <font color="Olive" title="b1">N</font> is   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> : 2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><a href="afinsq_2.html#K7" title="AFINSQ_2:func.7">Sum</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">p</font> <a href="relat_1.html#K5" title="RELAT_1:func.5">|</a> <font color="Olive" title="b1">N</font></span>)</span></span>)</span> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a> <font color="Olive" title="b1">N</font> </span> } </span>  &amp; <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c9">S</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c1">p</font> &amp; <font color="Maroon" title="c9">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="afinsq_2.html#K7" title="AFINSQ_2:func.7">Sum</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">p</font> <a href="relat_1.html#K5" title="RELAT_1:func.5">|</a> <span class="p2">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c9">S</font></span>)</span></span>)</span> &amp; <font color="Maroon" title="c1">p</font> <a href="recdef_1.html#K1" title="RECDEF_1:func.1">.</a> <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c9">S</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 )
