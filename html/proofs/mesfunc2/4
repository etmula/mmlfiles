<font color="Maroon" title="c7">A</font> <a href="subset_1.html#K9" title="SUBSET_1:func.9">/\</a> <span class="p1">(<span class="default"><a href="mesfunc1.html#K14" title="MESFUNC1:func.14">less_dom</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="mesfunc1.html#K3" title="MESFUNC1:func.3">+</a> <font color="Maroon" title="c4">g</font></span>)</span>,<font color="Maroon" title="c6">r</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="tarski.html#K3" title="TARSKI:func.3">union</a> <span class="p1">(<span class="default"><a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Maroon" title="c5">F</font></span>)</span>
