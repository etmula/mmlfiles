<span class="kw">let </span><font color="Maroon" title="c1">n</font> be   <a href="ordinal1.html#V7" title="ORDINAL1:attr.7">natural</a>   <a href="ordinal1.html#NM2" title="ORDINAL1:NM.2">number</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  not <font color="Maroon" title="c1">n</font> is  <a href="zfmisc_1.html#V1" title="ZFMISC_1:attr.1">trivial</a>  implies <font color="Maroon" title="c1">n</font> is 2 <a href="ec_pf_2.html#V1" title="EC_PF_2:attr.1">_or_greater</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:40_2_1"/><span class="lab"><font color="Green" title="E23">A1</font></span>: 
 not <font color="Maroon" title="c1">n</font> is  <a href="zfmisc_1.html#V1" title="ZFMISC_1:attr.1">trivial</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">n</font> is 2 <a href="ec_pf_2.html#V1" title="EC_PF_2:attr.1">_or_greater</a> </span><br/>

<a NAME="E2:40_2_1"/>
( <font color="Maroon" title="c1">n</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> 1 implies  not  not <font color="Maroon" title="c1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp;  ...  &amp;  not <font color="Maroon" title="c1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 )
 
;<br/>
<a NAME="E3:40_2_1"/><span class="kw">then </span>
<font color="Maroon" title="c1">n</font> <a href="xxreal_0.html#NR2" title="XXREAL_0:NR.2">&gt;=</a> 1 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:40_2_1"><span class="lab"><font color="Green" title="E23">A1</font></span></a>, <a class="ref" href="nat_1.html#T13" onmouseover="rs('nat_1/T13')" onmouseout="rh()">NAT_1:13</a></span>;<br/>
<span class="kw">hence </span><a NAME="E4:40_2_1"/>
