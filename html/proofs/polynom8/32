<span class="p1">(<span class="default"><a href="polynom8.html#K2" title="POLYNOM8:func.2">pow</a> (<font color="Maroon" title="c3">x</font>,<font color="Maroon" title="c2">j</font>)</span>)</span> <a href="group_1.html#K8" title="GROUP_1:func.8">*</a> <span class="p1">(<span class="default"><a href="polynom8.html#K2" title="POLYNOM8:func.2">pow</a> (<font color="Maroon" title="c3">x</font>,<span class="p2">(<span class="default"><a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> 1</span>)</span>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polynom8.html#K2" title="POLYNOM8:func.2">pow</a> (<font color="Maroon" title="c3">x</font>,<span class="p1">(<span class="default"><a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> <span class="p2">(<span class="default">1 <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c2">j</font></span>)</span></span>)</span>)
 
