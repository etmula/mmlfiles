> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> holds <br/>( ( <font color="Olive" title="b2">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">j</font> implies <span class="p1">(<span class="default"><font color="Olive" title="b1">A</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b3">j</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  ) &amp; ( <font color="Olive" title="b2">i</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> <font color="Olive" title="b3">j</font> implies <span class="p1">(<span class="default"><font color="Olive" title="b1">A</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b3">j</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K6" title="MARGREL1:func.6">FALSE</a>  ) ) ) ) )
