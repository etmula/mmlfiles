, <font color="Olive" title="b7">xr</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b6">xl</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">f2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">tl</font> &amp; <font color="Olive" title="b7">xr</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">f2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b3">tr</font> holds <br/><font color="Maroon" title="c11">f2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">nt</font> <a href="scm_comp.html#K2" title="SCM_COMP:func.2">-tree</a> (<font color="Olive" title="b2">tl</font>,<font color="Olive" title="b3">tr</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>3</sub></font>(<font color="Olive" title="b1">nt</font>,<font color="Olive" title="b4">rtl</font>,<font color="Olive" title="b5">rtr</font>,<font color="Olive" title="b6">xl</font>,<font color="Olive" title="b7">xr</font>) ) )
