(  ex <font color="Olive" title="b1">s9</font> being   <a href="msualg_1.html#NM1" title="MSUALG_1:NM.1">SortSymbol</a> of <font color="Maroon" title="c1">S</font> ex <font color="Olive" title="b2">x</font>, <font color="Olive" title="b3">y</font> being   <a href="msualg_6.html#NM1" title="MSUALG_6:NM.1">Element</a> of <font color="Maroon" title="c2">A</font>,<font color="Olive" title="b1">s9</font> ex <font color="Olive" title="b4">t</font> being    <a href="msualg_6.html#M2" title="MSUALG_6:mode.2">Translation</a> of <font color="Maroon" title="c2">A</font>,<font color="Olive" title="b1">s9</font>,<font color="Maroon" title="c4">s</font> st <br/>(  <a href="msualg_6.html#K3" title="MSUALG_6:func.3">TranslationRel</a> <font color="Maroon" title="c1">S</font> <a href="rewrite1.html#R1" title="REWRITE1:pred.1">reduces</a> <font color="Olive" title="b1">s9</font>,<font color="Maroon" title="c4">s</font> &amp; <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Olive" title="b2">x</font>,<font color="Olive" title="b3">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c3">P</font> <a href="msualg_4.html#K1" title="MSUALG_4:func.1">.</a> <font color="Olive" title="b1">s9</font> &amp; <font color="Maroon" title="c5">a</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">t</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">x</font> &amp; <font color="Maroon" title="c6">b</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">t</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b3">y</font> ) implies <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c5">a</font>,<font color="Maroon" title="c6">b</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <span class="p1">(<span class="default"><a href="msualg_6.html#K6" title="MSUALG_6:func.6">InvCl</a> <font color="Maroon" title="c3">P</font></span>)</span> <a href="msualg_4.html#K1" title="MSUALG_4:func.1">.</a> <font color="Maroon" title="c4">s</font> )
