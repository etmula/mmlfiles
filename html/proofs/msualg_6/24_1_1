<a NAME="E12:24_1_1"/>
<font color="Maroon" title="c8">p</font> is  <a href="funcop_1.html#V1" title="FUNCOP_1:attr.1">Function-yielding</a> 
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="24_1_1_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c9">j</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="funcop_1.html#D6" onmouseover="rs('funcop_1/D6')" onmouseout="rh()">FUNCOP_1:def 6</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  not <font color="Maroon" title="c9">j</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="xtuple_0.html#K9" title="XTUPLE_0:func.9">proj1</a> <font color="Maroon" title="c8">p</font> or <font color="Maroon" title="c8">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c9">j</font> is    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:24_1_1_2"/>
<font color="Maroon" title="c9">j</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c8">p</font>
