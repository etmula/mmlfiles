<span class="kw">hence </span><a NAME="E11:63_1_1"/>
( ( <font color="Maroon" title="c3">n</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Maroon" title="c7">k</font> &amp; <font color="Maroon" title="c7">k</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> 2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font> implies ( ( <font color="Maroon" title="c2">f</font> <a href="graphsp.html#R1" title="GRAPHSP:pred.1">hasBetterPathAt</a> <font color="Maroon" title="c3">n</font>,<font color="Maroon" title="c7">k</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> <font color="Maroon" title="c3">n</font> implies <font color="Maroon" title="c6">F</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">f</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c3">n</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p3">(<span class="default">3 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font></span>)</span></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> ) &amp; (  not <font color="Maroon" title="c2">f</font> <a href="graphsp.html#R1" title="GRAPHSP:pred.1">hasBetterPathAt</a> <font color="Maroon" title="c3">n</font>,<font color="Maroon" title="c7">k</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> <font color="Maroon" title="c3">n</font> implies <font color="Maroon" title="c6">F</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">k</font> ) ) ) &amp; ( 2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Maroon" title="c7">k</font> &amp; <font color="Maroon" title="c7">k</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> 3 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font> implies ( ( <font color="Maroon" title="c2">f</font> <a href="graphsp.html#R1" title="GRAPHSP:pred.1">hasBetterPathAt</a> <font color="Maroon" title="c3">n</font>,<font color="Maroon" title="c7">k</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font></span>)</span> implies <font color="Maroon" title="c6">F</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="graphsp.html#K16" title="GRAPHSP:func.16">newpathcost</a> (<font color="Maroon" title="c2">f</font>,<font color="Maroon" title="c3">n</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c7">k</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> <span class="p2">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font></span>)</span></span>)</span>) ) &amp; (  not <font color="Maroon" title="c2">f</font> <a href="graphsp.html#R1" title="GRAPHSP:pred.1">hasBetterPathAt</a> <font color="Maroon" title="c3">n</font>,<font color="Maroon" title="c7">k</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font