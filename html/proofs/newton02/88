( 4 <a href="int_1.html#R1" title="INT_1:pred.1">divides</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">a</font> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <font color="Maroon" title="c3">m</font></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">b</font> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <font color="Maroon" title="c3">m</font></span>)</span> iff 4 <a href="int_1.html#R1" title="INT_1:pred.1">divides</a> <font color="Maroon" title="c1">a</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">b</font> )
