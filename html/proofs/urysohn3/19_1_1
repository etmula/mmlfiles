 for <font color="Olive" title="b1">p</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <font color="Maroon" title="c1">T</font> holds <br/> ( (  <a href="urysohn3.html#K3" title="URYSOHN3:func.3">Rainbow</a> (<font color="Olive" title="b1">p</font>,<font color="Maroon" title="c4">R</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  implies <font color="Maroon" title="c5">F</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  ) &amp; (  for <font color="Olive" title="b2">S</font> being   non  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">empty</a>  <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <a href="numbers.html#K6" title="NUMBERS:func.6">ExtREAL</a>  st <font color="Olive" title="b2">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="urysohn3.html#K3" title="URYSOHN3:func.3">Rainbow</a> (<font color="Olive" title="b1">p</font>,<font color="Maroon" title="c4">R</font>) holds <br/><font color="Maroon" title="c5">F</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="supinf_2.html#K8" title="SUPINF_2:func.8">sup</a> <font color="Olive" title="b2">S</font> ) )
