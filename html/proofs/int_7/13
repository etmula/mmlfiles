<span class="kw">assume </span><a NAME="E1:13_2"/>
 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  not <font color="Maroon" title="c2">b</font> is  <a href="int_7.html#V1" title="INT_7:attr.1">prime-factorization-like</a>  or  not  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> 1 or  not <font color="Maroon" title="c3">a</font> <a href="int_1.html#R1" title="INT_1:pred.1">divides</a>  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> or  not  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="wsierp_1.html#K2" title="WSIERP_1:func.2">Product</a> <font color="Maroon" title="c1">f</font> or  not <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">b</font> <a href="funct_1.html#NK3" title="FUNCT_1:NK.3">*</a> <span class="p1">(<span class="default"><a href="finseq_1.html#K20" title="FINSEQ_1:func.20">canFS</a> <span class="p2">(<span class="default"><a href="pre_poly.html#K22" title="PRE_POLY:func.22">support</a> <font color="Maroon" title="c2">b</font></span>)</span></span>)</span> or <font color="Maroon" title="c3">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="pre_poly.html#K22" title="PRE_POLY:func.22">support</a> <font color="Maroon" title="c2">b</font> )</span><br/>

<a NAME="E2:13_2"/><span class="kw">then </span>
<font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="pre_poly.html#K2" title="PRE_POLY:func.2">&lt;*&gt;</a> <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>
 
;<br/>
<span class="kw">hence </span><a NAME="E3:13_2"/>
(  not <font color="Maroon" title="c2">b</font> is  <a href="int_7.html#V1" title="INT_7:attr.1">prime-factorization-like</a>  or  not  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> 1 or  not <font color="Maroon" title="c3">a</font> <a href="int_1.html#R1" title="INT_1:pred.1">divides</a>  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> or  not  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Maroon" title="c2">b</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="wsierp_1.html#K2" title="WSIERP_1:func.2">Product</a> <font color="Maroon" title="c1">f</font> or  not <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">b</font> <a href="funct_1.html#NK3" title="FUNCT_1:NK.3">*</a> <span class="p1">(<span class="default"><a href="finseq_1.html#K20" title="FINSEQ_1:func.20">canFS</a> <span class="p2">(<span class="default"><a href="pre_poly.html#K22" title="PRE_POLY:func.22">support</a> <font color="Maroon" title="c2">b</font></span>)</span></span>)</span> or <font color="Maroon" title="c3">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="pre_poly.html#K22" title="PRE_POLY:func.22">support</a> <font color="Maroon" title="c2">b</font> )
