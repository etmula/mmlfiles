<span class="kw">let </span><font color="Maroon" title="c1">f</font> be    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">b</font> being   <a href="pre_poly.html#NM2" title="PRE_POLY:NM.2">bag</a> of  <a href="newton.html#K8" title="NEWTON:func.8">SetPrimes</a> <br/>  for <font color="Olive" title="b2">a</font> being   <a href="newton.html#NM1" title="NEWTON:NM.1">Prime</a>  st  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; <font color="Olive" title="b1">b</font> is  <a href="int_7.html#V1" title="INT_7:attr.1">prime-factorization-like</a>  &amp;  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Olive" title="b1">b</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> 1 &amp; <font color="Olive" title="b2">a</font> <a href="int_1.html#R1" title="INT_1:pred.1">divides</a>  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Olive" title="b1">b</font> &amp;  <a href="nat_3.html#K8" title="NAT_3:func.8">Product</a> <font color="Olive" title="b1">b</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="wsierp_1.html#K2" title="WSIERP_1:func.2">Product</a> <font color="Maroon" title="c1">f</font> &amp; <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">b</font> <a href="funct_1.html#NK3" title="FUNCT_1:NK.3">*</a> <span class="p1">(<span class="default"><a href="finseq_1.html#K20" title="FINSEQ_1:func.20">canFS</a> <span class="p2">(<span