( <span class="p1">(<span class="default"><a href="taylor_2.html#K1" title="TAYLOR_2:func.1">Maclaurin</a> (<span class="p2">(<span class="default"><a href="taylor_1.html#K1" title="TAYLOR_1:func.1">#Z</a> <font color="Maroon" title="c1">n</font></span>)</span>,<span class="p2"><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">].</a><span class="default"><span class="p3">(<span class="default"><a href="real_1.html#K1" title="REAL_1:func.1">-</a> <font color="Maroon" title="c3">r</font></span>)</span>,<font color="Maroon" title="c3">r</font></span><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">.[</a></span>,<font color="Maroon" title="c4">x</font>)</span>)</span> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c2">m</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; <span class="p1">(<span class="default"><a href="taylor_2.html#K1" title="TAYLOR_2:func.1">Maclaurin</a> (<span class="p2">(<span class="default"><a href="taylor_1.html#K1" title="TAYLOR_1:func.1">#Z</a> <font color="Maroon" title="c1">n</font></span>)</span>,<span class="p2"><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">].</a><span class="default"><span class="p3">(<span class="default"><a href="real_1.html#K1" title="REAL_1:func.1">-</a> <font color="Maroon" title="c3">r</font></span>)</span>,<font color="Maroon" title="c3">r</font></span><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">.[</a></span>,<font color="Maroon" title="c4">x</font>)</span>)</span> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">x</font> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <font color="Maroon" title="c1">n</font> )
