<span class="kw">assume </span><a NAME="E1:32_1"/>
<font color="Maroon" title="c3">B</font> is    <a href="pencil_2.html#M1" title="PENCIL_2:mode.1">Segre-Coset</a> of <font color="Maroon" title="c2">A</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  ex <font color="Olive" title="b1">W</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="pencil_1.html#K5" title="PENCIL_1:func.5">Segre_Product</a> <font color="Maroon" title="c2">A</font></span>)</span> st <br/>(  not <font color="Olive" title="b1">W</font> is  <a href="zfmisc_1.html#V1" title="ZFMISC_1:attr.1">trivial</a>  &amp; <font color="Olive" title="b1">W</font> is  <a href="pencil_1.html#V2" title="PENCIL_1:attr.2">strong</a>  &amp; <font color="Olive" title="b1">W</font> is  <a href="pencil_1.html#V1" title="PENCIL_1:attr.1">closed_under_lines</a>  &amp; <font color="Maroon" title="c3">B</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="tarski.html#K3" title="TARSKI:func.3">union</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b2">Y</font> where <font color="Olive" title="b2">Y</font> is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="pencil_1.html#K5" title="PENCIL_1:func.5">Segre_Product</a> <font color="Maroon" title="c2">A</font></span>)</span> : (  not <font color="Olive" title="b2">Y</font> is  <a href="zfmisc_1.html#V1" title="ZFMISC_1:attr.1">trivial</a>  &amp; <font color="Olive" title="b2">Y</font> is  <a href="pencil_1.html#V2" title="PENCIL_1:attr.2">strong</a>  &amp; <font color="Olive" title="b2">Y</font> is  <a href="pencil_1.html#V1" title="PENCIL_1:attr.1">closed_under_lines</a>  &amp; <font color="Olive" title="b1">W</font>,<font color="Olive" title="b2">Y</font> <a href="pencil_2.html#R1" title="PENCIL_2:pred.1">are_joinable</a>  ) </span> } </span>  )</span><br/>

<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c4">BB</font> = <font color="Maroon" title="c3">B</font> as    <a href="pencil_2.html#M1" title="PENCIL_2:mode.1">Segre-Coset</a> of <font color="Maroon" title="c2">A</font> ;<br/>
