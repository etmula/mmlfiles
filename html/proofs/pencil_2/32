<span class="kw">given </span><font color="Maroon" title="c4">W</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="pencil_1.html#K5" title="PENCIL_1:func.5">Segre_Product</a> <font color="Maroon" title="c2">A</font></span>)</span><span class="kw"> such that </span><a NAME="E4:32"/><span class="lab"><font color="Green" title="E21">A18</font></span>: 
(  not <font color="Maroon" title="c4">W</font> is  <a href="zfmisc_1.html#V1" title="ZFMISC_1:attr.1">trivial</a>  &amp; <font color="Maroon" title="c4">W</font> is  <a href="pencil_1.html#V2" title="PENCIL_1:attr.2">strong</a>  &amp; <font color="Maroon" title="c4">W</font> is  <a href="pencil_1.html#V1" title="PENCIL_1:attr.1">closed_under_lines</a>  )
 <span class="kw">and </span><br/><a NAME="E5:32"/><span class="lab"><font color="Green" title="E22">A19</font></span>: 
