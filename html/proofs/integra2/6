<span class="kw">let </span><font color="Maroon" title="c4">p</font> be    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">q</font> being   <a href="valued_0.html#V8" title="VALUED_0:attr.8">non-increasing</a>   <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>   st  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c4">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; <font color="Maroon" title="c4">p</font>,<font color="Olive" title="b1">q</font> <a href="classes1.html#R2" title="CLASSES1:pred.2">are_fiberwise_equipotent</a>  holds <br/><font color="Maroon" title="c4">p</font>, <a href="finseq_5.html#K4" title="FINSEQ_5:func.4">Rev</a> <font color="Olive" title="b1">q</font> <a href="classes1.html#R2" title="CLASSES1:pred.2">are_fiberwise_equipotent</a> </span><br/><span class="kw">let </span><font color="Maroon" title="c5">q</font> be   <a href="valued_0.html#V8" title="VALUED_0:attr.8">non-increasing</a>   <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c4">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:fun