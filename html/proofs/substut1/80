<a NAME="E4:80"/><span class="kw">then </span><span class="lab"><font color="Green" title="E43">A2</font></span>: 
 for <font color="Olive" title="b1">S</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="substut1.html#K16" title="SUBSTUT1:func.16">QC-Sub-WFF</a> <font color="Maroon" title="c1">A</font><br/>  for <font color="Olive" title="b2">d1</font>, <font color="Olive" title="b3">d2</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="qc_lang1.html#K9" title="QC_LANG1:func.9">QC-WFF</a> <font color="Maroon" title="c1">A</font> holds <br/> ( ( <font color="Olive" title="b1">S</font> is <font color="Maroon" title="c1">A</font> <a href="substut1.html#V2" title="SUBSTUT1:attr.2">-Sub_VERUM</a>  implies <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="cqc_lang.html#K5" title="CQC_LANG:func.5">VERUM</a> <font color="Maroon" title="c1">A</font> ) &amp; ( <font color="Olive" title="b1">S</font> is  <a href="substut1.html#V4" title="SUBSTUT1:attr.4">Sub_atomic</a>  implies <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>3</sub></font>(<font color="Olive" title="b1">S</font>) ) &amp; ( <font color="Olive" title="b1">S</font> is  <a href="substut1.html#V5" title="SUBSTUT1:attr.5">Sub_negative</a>  &amp; <font color="Olive" title="b2">d1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="substut1.html#K27" title="SUBSTUT1:func.27">Sub_the_argument_of</a> <font color="Olive" title="b1">S</font></span>)</span> implies <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>2</sub></font>(<font color="Olive" title="b2">d1</font>) ) &amp; ( <font color="Olive" title="b1">S</font> is  <a href="substut1.html#V6" title="SUBSTUT1:attr.6">Sub_conjunctive</a>  &amp; <font color="Olive" title="b2">d1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="substut1.html#K28" title="SUBSTUT1:func.28">Sub_the_left_argument_of</a> <font color="Olive" title="b1">S</font></span>)</span> &amp; <font color="Olive" title="b3">d2</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="substut1.html#K29" title="SUBSTUT1:func.29">Sub_the_right_argument_of</a> <font color="Olive" title="b1">S</font></span>)</span> implies <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b2">d1</font>,<font color="Olive" title="b3">d2</font>) ) &amp; ( <font color="Olive" title="b1">S</font> is  <a href="substut1.html#V7" title="SUBSTUT1:attr.7">Sub_universal</a>  &amp; <font color="Olive" title="b2">d1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="substut1.html#K31" title="SUBSTUT1:func.31">Sub_the_scope_of</a> <font color="Olive" title="b1">S</font></span>)</span> implies <font color="Maroon" title="c3">F2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">S</font> <a href="hidden.html#