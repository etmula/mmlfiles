</div><span class="kw">end;</span></div>

<span class="kw">thus </span><a NAME="E7:15"/>
( <font color="Maroon" title="c2">p</font> is  <a href="qc_lang1.html#V5" title="QC_LANG1:attr.5">universal</a>  implies  <a href="substut1.html#K9" title="SUBSTUT1:func.9">Bound_Vars</a> <font color="Maroon" title="c2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="substut1.html#K9" title="SUBSTUT1:func.9">Bound_Vars</a> <span class="p2">(<span class="default"><a href="qc_lang1.html#K22" title="QC_LANG1:func.22">the_scope_of</a> <font color="Maroon" title="c2">p</font></span>)</span></span>)</span> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <span class="p1"><a href="domain_1.html#K6" title="DOMAIN_1:func.6">{</a><span class="default"><span class="p2">(<span class="default"><a href="qc_lang1.html#K21" title="QC_LANG1:func.21">bound_in</a> <font color="Maroon" title="c2">p</font></span>)</span></span><a href="domain_1.html#K6" title="DOMAIN_1:func.6">}</a></span> )
  <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="15_5"><span class="kw">proof </span></a><div class="add">

<span class="kw">assume </span><a NAME="E1:15_5"/><span class="lab"><font color="Green" title="E6">A5</font></span>: 
<font color="Maroon" title="c2">p</font> is  <a href="qc_lang1.html#V5" title="QC_LANG1:attr.5">universal</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="substut1.html#K9" title="SUBSTUT1:func.9">Bound_Vars</a> <font color="Maroon" title="c2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="substut1.html#K9" title="SUBSTUT1:func.9">Bound_Vars</a> <span class="p2">(<span class="default"><a href="qc_lang1.html#K22" title="QC_LANG1:func.22">the_scope_of</a> <font color="Maroon" title="c2">p</font></span>)</span></span>)</span> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <span class="p1"><a href="domain_1.html#K6" title="DOMAIN_1:func.6">{</a><span class="default"><span class="p2">(<span class="default"><a href="qc_lang1.html#K21" title="QC_LANG1:func.21">bound_in</a> <font color="Maroon" title="c2">p</font></span>)</span></span><a href="domain_1.html#K6" title="DOMAIN_1:func.6">}</a></span></span><br/>

<span class="kw">thus </span><a NAME="E2:15_5"/>
<font color="Maroon">H<sub>2</sub></font>(<font color="Maroon" title="c2">p</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>5</sub></font>(<font color="Maroon" title="c2">p</font>,<font color="Maroon">H<sub>2</sub></font>( <a href="qc_lang1.html#K22" title="QC_LANG1:func.22">the_scope_of</a> <font color="Maroon" title="c2">p</font>))
 <span class="kw">from</span> <span class="lab"><a class="ref" href="qc_lang3.html#S7" onmouseover="rs('qc_lang3/S7')" onmouseout="rh()">QC_LANG3:sch 7</a>(<a class="txt" href="#E1:15"><span class="lab"><font color="Green" title="E5">A1</font></span></a>, <a class="txt" href="#E1:15_5"><span class="lab"><font color="Green" title="E6">A5</font></span></a>);</span> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>


