} </span>  or <font color="Olive" title="b2">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b4">n</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b3">A</font> or <span class="p1">(<span class="default"><font color="Olive" title="b2">p</font> <a href="finseq_3.html#K1" title="FINSEQ_3:func.1">-</a> <font color="Olive" title="b3">A</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b4">n</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p2">(<span class="default"><a href="card_1.html#K4" title="CARD_1:func.4">card</a> <font color="Olive" title="b5">B</font></span>)</span></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b4">n</font> )
