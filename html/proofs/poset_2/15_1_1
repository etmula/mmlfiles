<span class="kw">let </span><font color="Maroon" title="c5">x</font>, <font color="Maroon" title="c6">y</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c5">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">S</font> &amp; <font color="Maroon" title="c6">y</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">S</font> &amp; <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c5">x</font>,<font color="Maroon" title="c6">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="orders_2.html#U1" title="ORDERS_2:sel.1">InternalRel</a> of <font color="Maroon" title="c1">S</font> &amp; <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c6">y</font>,<font color="Maroon" title="c5">x</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="orders_2.html#U1" title="ORDERS_2:sel.1">InternalRel</a> of <font color="Maroon" title="c1">S</font> implies <font color="Maroon" title="c5">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">y</font> )</span><br/>



<span class="kw">assume </span><a NAME="E1:15_1_1_3_1"/><span class="lab"><font color="Green" title="E10">B1</font></span>: 
( <font color="Maroon" title="c5">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">S</font> &amp; <font color="Maroon" title="c6">y</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">S</font> &amp; <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c5">x</font>,<font color="Maroon" title="c6">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="orders_2.html#U1" title="ORDERS_2:sel.1">InternalRel</a> of <font color="Maroon" title="c1">S</font> &amp; <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c6">y</font>,<font color="Maroon" title="c5">x</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  the <a href="orders_2.html#U1" title="ORDERS_2:sel.1">InternalRel</a> of <font color="Maroon" title="c1">S</font> )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">y</font></span><br/>

<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c7">x</font> = <font color="Maroon" title="c5">x</font>, <font color="Maroon" title="c8">y</font> = <font color="Maroon" title="c6">y</font> as   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">S</font> ;<br/>
