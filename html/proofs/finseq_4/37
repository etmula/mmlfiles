  <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">x</font> <a href="finseq_4.html#K4" title="FINSEQ_4:func.4">..</a> <font color="Maroon" title="c1">p</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c4">k</font> implies <span class="p1">(<span class="default"><font color="Maroon" title="c1">p</font> <a href="finseq_3.html#K1" title="FINSEQ_3:func.1">-</a> <span class="p2"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><font color="Maroon" title="c2">x</font></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> )</span><div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="37_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">assume </span><a NAME="E1:37_2"/><span class="lab"><font color="Green" title="E28">A8</font></span>: 
