 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c2">x</font> <a href="finseq_4.html#K4" title="FINSEQ_4:func.4">..</a> <font color="Maroon" title="c1">p</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c4">k</font></span><br/>

<span class="kw">assume </span><a NAME="E2:37_2"/>
 not <font color="Maroon" title="c2">x</font> <a href="finseq_4.html#K4" title="FINSEQ_4:func.4">..</a> <font color="Maroon" title="c1">p</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c4">k</font>
