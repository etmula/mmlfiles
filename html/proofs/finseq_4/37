<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:37_1"/><span class="lab"><font color="Green" title="E28">A6</font></span>: 
<span class="p1">(<span class="default"><font color="Maroon" title="c1">p</font> <a href="finseq_3.html#K1" title="FINSEQ_3:func.1">-</a> <span class="p2"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><font color="Maroon" title="c2">x</font></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">k</font>
 <span class="kw">and </span><br/><a NAME="E2:37_1"/><span class="lab"><font color="Green" title="E29">A7</font></span>: 
