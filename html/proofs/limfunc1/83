<span class="kw">hence </span><a NAME="E4:83"/>
 <a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c3">f</font> is  <a href="limfunc1.html#V3" title="LIMFUNC1:attr.3">convergent_in+infty</a> 
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:83"><span class="lab"><font color="Green" title="E48">A2</font></span></a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="limfunc1.html#K4" title="LIMFUNC1:func.4">lim_in+infty</a> <span class="p1">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c3">f</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><a href="limfunc1.html#K4" title="LIMFUNC1:func.4">lim_in+infty</a> <font color="Maroon" title="c3">f</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span></span><br/>

<span class="kw">hence </span><a NAME="E5:83"/>
 <a href="limfunc1.html#K4" title="LIMFUNC1:func.4">lim_in+infty</a> <span class="p1">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c3">f</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><a href="limfunc1.html#K4" title="LIMFUNC1:func.4">lim_in+infty</a> <font color="Maroon" title="c3">f</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span>
