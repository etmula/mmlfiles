<span class="kw">let </span><font color="Maroon" title="c4">i</font>, <font color="Maroon" title="c5">j</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c4">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c3">M</font> &amp; <font color="Maroon" title="c5">j</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <span class="p1">(<span class="default"><a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<font color="Maroon" title="c3">M</font>,<font color="Maroon" title="c4">i</font>)</span>)</span> implies <span class="p1">(<span class="default"><a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<font color="Maroon" title="c3">M</font>,<font color="Maroon" title="c4">i</font>)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c5">j</font> <a href="xxreal_0.html#NR2" title="XXREAL_0:NR.2">&gt;=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  )</span><br/>



<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:81_3"/><span class="lab"><font color="Green" title="E65">A7</font></span>: 
