<span class="kw">let </span><font color="Maroon" title="c4">f</font>, <font color="Maroon" title="c5">g</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  the <a href="struct_0.html#U4" title="STRUCT_0:sel.4">carrier'</a> of <font color="Maroon" title="c1">C</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c5">g</font>,<font color="Maroon" title="c4">f</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a>  the <a href="cat_1.html#U1" title="CAT_1:sel.1">Comp</a> of <font color="Maroon" title="c1">C</font> implies <font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">g</font> <a href="cat_1.html#K1" title="CAT_1:func.1">(*)</a> <font color="Maroon" title="c4">f</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c5">g</font></span>)</span> <a href="cat_1.html#K1" title="CAT_1:func.1">(*)</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">f</font></span>)</span> )</span><br/>

<span class="kw">assume </span><a NAME="E6:74"/>
