 for <font color="Olive" title="b1">f</font> being   <a href="cat_1.html#NM2" title="CAT_1:NM.2">Morphism</a> of <font color="Maroon" title="c1">C</font> holds <br/> ( <font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="cat_1.html#K4" title="CAT_1:func.4">id</a> <span class="p2">(<span class="default"><a href="graph_1.html#K3" title="GRAPH_1:func.3">dom</a> <font color="Olive" title="b1">f</font></span>)</span></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="cat_1.html#K4" title="CAT_1:func.4">id</a> <span class="p1">(<span class="default"><a href="graph_1.html#K3" title="GRAPH_1:func.3">dom</a> <span class="p2">(<span class="default"><font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">f</font></span>)</span></span>)</span> &amp; <font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="cat_1.html#K4" title="CAT_1:func.4">id</a> <span class="p2">(<span class="default"><a href="graph_1.html#K4" title="GRAPH_1:func.4">cod</a> <font color="Olive" title="b1">f</font></span>)</span></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="cat_1.html#K4" title="CAT_1:func.4">id</a> <span class="p1">(<span class="default"><a href="graph_1.html#K4" title="GRAPH_1:func.4">cod</a> <span class="p2">(<span class="default"><font color="Maroon" title="c3">T</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">f</font></span>)</span></span>)</span> )
 
