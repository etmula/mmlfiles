 for <font color="Olive" title="b1">a</font>, <font color="Olive" title="b2">b</font> being   <a href="cat_1.html#NM1" title="CAT_1:NM.1">Object</a> of <font color="Maroon" title="c1">A</font>  st  <a href="cat_1.html#K2" title="CAT_1:func.2">Hom</a> (<font color="Olive" title="b1">a</font>,<font color="Olive" title="b2">b</font>) <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  holds <br/> for <font color="Olive" title="b3">f</font> being    <a href="cat_1.html#M1" title="CAT_1:mode.1">Morphism</a> of <font color="Olive" title="b1">a</font>,<font color="Olive" title="b2">b</font> holds  <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c7">t2</font> <a href="nattra_1.html#K3" title="NATTRA_1:func.3">`*`</a> <font color="Maroon" title="c6">t1</font></span>)</span> <a href="nattra_1.html#K2" title="NATTRA_1:func.2">.</a> <font color="Olive" title="b2">b</font></span>)</span> <a href="cat_1.html#K5" title="CAT_1:func.5">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">F</font> <a href="cat_3.html#K9" title="CAT_3:func.9">/.</a> <font color="Olive" title="b3">f</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">F2</font> <a href="cat_3.html#K9" title="CAT_3:func.9">/.</a> <font color="Olive" title="b3">f</font></span>)</span> <a href="cat_1.html#K5" title="CAT_1:func.5">*</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c7">t2</font> <a href="nattra_1.html#K3" title="NATTRA_1:func.3">`*`</a> <font color="Maroon" title="c6">t1</font></span>)</span> <a href="nattra_1.html#K2" title="NATTRA_1:func.2">.</a> <font color="Olive" title="b1">a</font></span>)</span>
