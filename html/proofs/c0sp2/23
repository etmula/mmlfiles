( <font color="Maroon" title="c3">W</font> is  <a href="rlvect_1.html#V5" title="RLVECT_1:attr.5">vector-distributive</a>  &amp; <font color="Maroon" title="c3">W</font> is  <a href="rlvect_1.html#V6" title="RLVECT_1:attr.6">scalar-distributive</a>  &amp; <font color="Maroon" title="c3">W</font> is  <a href="rlvect_1.html#V7" title="RLVECT_1:attr.7">scalar-associative</a>  &amp; <font color="Maroon" title="c3">W</font> is  <a href="funcsdom.html#V2" title="FUNCSDOM:attr.2">vector-associative</a>  )
 
