 ex <font color="Olive">b<sub>1</sub></font> being   <a href="eqrel_1.html#NM2" title="EQREL_1:NM.2">Equivalence_Relation</a> of <font color="Maroon" title="c1">Y</font> st <br/> for <font color="Olive" title="b2">x1</font>, <font color="Olive" title="b3">x2</font> being    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a>  holds <br/> ( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Olive" title="b2">x1</font>,<font color="Olive" title="b3">x2</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Olive">b<sub>1</sub></font> iff  ex <font color="Olive" title="b4">A</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <font color="Maroon" title="c1">Y</font> st <br/>( <font color="Olive" title="b4">A</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">PA</font> &amp; <font color="Olive" title="b2">x1</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Olive" title="b4">A</font> &amp; <font color="Olive" title="b3">x2</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Olive" title="b4">A</font> ) )
