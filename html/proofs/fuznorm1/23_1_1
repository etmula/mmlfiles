 <span class="kw">by</span> <span class="lab"><a class="ref" href="fuznorm1.html#D6" target="_self" onmouseover="rs('fuznorm1/D6')" onmouseout="rh()">MinDef</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<a NAME="E4:23_1_1"/>
 for <font color="Olive" title="b1">a</font> being    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <span class="p1"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,1</span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span> holds  <a href="fuznorm1.html#K1" title="FUZNORM1:func.1">minnorm</a> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (<font color="Olive" title="b1">a</font>,1) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">a</font>
 
