<span class="kw">let </span><font color="Maroon" title="c2">a</font>, <font color="Maroon" title="c3">b</font>, <font color="Maroon" title="c4">c</font>, <font color="Maroon" title="c5">d</font> be    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <span class="p1"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,1</span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">a</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c4">c</font> &amp; <font color="Maroon" title="c3">b</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c5">d</font> implies <a href="fuznorm1.html#K1" title="FUZNORM1:func.1">minnorm</a> <a href="binop_1.html#K4" title="BINOP_1:func.4">.</a> (<font color="Maroon" title="c2">a</font>,<font color="Maroon" title="c3">b</font>) <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <a href="fuznorm1.html#K1" title="FUZNORM1:func.1">minnorm</a> <a href="binop_1.html#K4" title="BINOP_1:func.4">.</a> (<font color="Maroon" title="c4">c</font>,<font color="Maroon" title="c5">d</font>) )</span><br/>

