( <a href="sin_cos.html#K16" title="SIN_COS:func.16">sin</a> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="sincos10.html#K5" title="SINCOS10:func.5">arcsec1</a> <font color="Maroon" title="c1">r</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="square_1.html#K3" title="SQUARE_1:func.3">sqrt</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c1">r</font> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> 1</span>)</span></span>)</span> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <font color="Maroon" title="c1">r</font> &amp; <a href="sin_cos.html#K18" title="SIN_COS:func.18">cos</a> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="sincos10.html#K5" title="SINCOS10:func.5">arcsec1</a> <font color="Maroon" title="c1">r</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <font color="Maroon" title="c1">r</font> )
