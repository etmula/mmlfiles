<div><span class="kw">hereby </span> <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="waybel27.html#D2" target="_self" onmouseover="rs('waybel27/D2')" onmouseout="rh()">WAYBEL27:def 2</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">f</font> being   <a href="funct_1.html#NM1" title="FUNCT_1:NM.1">Function</a>  st <font color="Olive" title="b1">f</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c7">G</font> holds <br/><font color="Maroon" title="c7">G</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="funct_5.html#K1" title="FUNCT_5:func.1">curry</a> <font color="Olive" title="b1">f</font></span>
<div class="add"><span class="kw">let </span><font color="Maroon" title="c8">x</font> be    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c8">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c7">G</font> implies ( <font color="Maroon" title="c8">x</font> is   <a href="funct_1.html#NM1" title="FUNCT_1:NM.1">Function</a> &amp;  <a href="xtuple_0.html#K9" title="XTUPLE_0:func.9">proj1</a> <font color="Maroon" title="c8">x</font> is   <a href="relat_1.html#NM1" title="RELAT_1:NM.1">Relation</a> ) )</span><br/><span class="kw">assume </span><a NAME="E1:64_4_1"/>
<font color="Maroon" title="c8">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c7">G</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c8">x</font> is   <a href="funct_1.html#NM1" title="FUNCT_1:NM.1">Function</a> &amp;  <a href="xtuple_0.html#K9" title="XTUPLE_0:func.9">proj1</a> <font color="Maroon" title="c8">x</font> is   <a href="relat_1.html#NM1" title="RELAT_1:NM.1">Relation</a> )</span><br/><span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c9">f</font> = <font color="Maroon" title="c8">x</font> as   <a href="struct_0.html#NM6" title="STRUCT_0:NM.6">Function</a> of <span class="p1"><a href="yellow_3.html#K3" title="YELLOW_3:func.3">[:</a><span class="default"><font color="Maroon" title="c1">R</font>,<font color="Maroon" title="c2">S</font></span><a href="yellow_3.html#K3" title="YELLOW_3:func.3">:]</a></span>,<font color="Maroon" title="c3">T</font> <span class="kw">by</span> <span class="lab"><a class="ref" href="waybel27.html#D4" target="_self" onmouseover="rs('waybel27/D4')" onmouseout="rh()">Def4</a></span>;<br/> <a href="xtuple_0.html#K9" title="XTUPLE_0:func.9">proj1</a> <font color="Maroon" title="c9">f</font> = 
