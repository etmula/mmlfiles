<span class="kw">hence </span><a NAME="E15:18_1_1"/>
(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c12">N</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <span class="p2"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Olive" title="b1">a</font>,<font color="Olive" title="b2">f</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> where <font color="Olive" title="b1">a</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font>, <font color="Olive" title="b2">f</font> is    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <font color="Maroon" title="c3">F</font> : <font color="Olive" title="b1">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b2">f</font> </span> } </span>  &amp; (  for <font color="Olive" title="b1">i</font>, <font color="Olive" title="b2">j</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c12">N</font> holds <br/> ( <font color="Olive" title="b1">i</font> <a href="orders_2.html#R1" title="ORDERS_2:pred.1">&lt;=</a> <font color="Olive" title="b2">j</font> iff <font color="Olive" title="b2">j</font> <a href="xfamily.html#K2" title="XFAMILY:func.2">`2</a>  <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Olive" title="b1">i</font> <a href="xfamily.html#K2" title="XFAMILY:func.2">`2</a>  ) ) &amp; (  for <font color="Olive" title="b1">i</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c12">N</font> holds  <font color="Maroon" title="c12">N</font> <a href="waybel_0.html#K2" title="WAYBEL_0:func.2">.</a> <font color="Olive" title="b1">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">i</font> <a href="xfamily.html#K1" title="XFAMILY:func.1">`1</a>  ) )
