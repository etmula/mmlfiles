<span class="kw">let </span><font color="Maroon" title="c1">X</font> be   <a href="bhsp_1.html#NM1" title="BHSP_1:NM.1">RealUnitarySpace</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">Y</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">X</font> holds <br/> ( <font color="Olive" title="b1">Y</font> is  <a href="dualsp04.html#V1" title="DUALSP04:attr.1">open</a>  iff <font color="Olive" title="b1">Y</font> <a href="subset_1.html#K3" title="SUBSET_1:func.3">`</a>  is  <a href="dualsp04.html#V2" title="DUALSP04:attr.2">closed</a>  )</span><br/><span class="kw">let </span><font color="Maroon" title="c2">Y</font> be   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">X</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">Y</font> is  <a href="dualsp04.html#V1" title="DUALSP04:attr.1">open</a>  iff <font color="Maroon" title="c2">Y</font> <a href="subset_1.html#K3" title="SUBSET_1:func.3">`</a>  is  <a href="dualsp04.html#V2" title="DUALSP04:attr.2">closed</a>  )</span><br/>



