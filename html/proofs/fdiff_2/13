<span class="kw">assume </span><a NAME="E1:13_1"/><span class="lab"><font color="Green" title="E12">A1</font></span>: 
<font color="Maroon" title="c2">f</font> <a href="fdiff_1.html#R1" title="FDIFF_1:pred.1">is_differentiable_in</a> <font color="Maroon" title="c1">x0</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  ex <font color="Olive" title="b1">N</font> being    <a href="rcomp_1.html#M1" title="RCOMP_1:mode.1">Neighbourhood</a> of <font color="Maroon" title="c1">x0</font> st <font color="Olive" title="b1">N</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c2">f</font> &amp; (  for <font color="Olive" title="b1">h</font> being   <a href="ordinal1.html#V9" title="ORDINAL1:attr.9">non-zero</a>   <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="fdiff_1.html#V1" title="FDIFF_1:attr.1">-convergent</a>  <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a><br/>  for <font color="Olive" title="b2">c</font> being  <a href="funct_1.html#V3" title="FUNCT_1:attr.3">V8</a>() <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a>  st  <a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Olive" title="b2">c</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><font color="Maroon" title="c1">x0</font></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span> &amp;  <a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">h</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Olive" title="b2">c</font></span>)</span> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c2">f</font> holds <br/><span class="p1">(<span class="default"><font color="Olive" title="b1">h</font> <a href="valued_1.html#K37" title="VALUED_1:func.37">"</a></span>)</span> <a href="valued_1.html#K20" title="VALUED_1:func.20">(#)</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="funct_2.html#K8" title="FUNCT_2:func.8">/*</a> <span class="p3">(<span class="default"><font color="Olive" title="b1">h</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Olive" title="b2">c</font></span>)</span></span>)</span> <a href="valued_1.html#K47" title="VALUED_1:func.47">-</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="funct_2.html#K8" title="FUNCT_2:func.8">/*</a> <font color="Olive" title="b2">c</font></span>)</span></span>)</span> is  <a href="comseq_2.html#V2" title="COMSEQ_2:attr.2">convergent</a>  ) )</span><br/>

<span class="kw">then </span><span class="kw">consider </span><font color="Maroon" title="c3">N</font> being    <a href="rcomp_1.html#M1" title="RCOMP_1:mode.1">Neighbourhood</a> of <font color="Maroon" title="c1">x0</font><span class="kw"> such that </span><br/><a NAME="E3:13_1"/><span class="lab"><font color="Green" title="E13">A2</font></span>: 
<font color="Maroon" title="c3">N</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c2">f</font>
 <span class="kw">and </span><br/><a NAME="E4:13_1"/>
 ex <font color="Olive" title="b1">L</font> being   <a href="fdiff_1.html#NM2" title="FDIFF_1:NM.2">LinearFunc</a> ex <font color="Olive" title="b2">R</font> being   <a href="fdiff_1.html#NM1" title="FDIFF_1:NM.1">RestFunc</a> st <br/> for <font color="Olive" title="b3">g</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st <font color="Olive" title="b3">g</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c3">N</font> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b3">g</font></span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c1">x0</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">L</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b3">g</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c1">x0</font></span>)</span></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1">(<span class="default"><font color="Olive" title="b2">R</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b3">g</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c1">x0</font></span>)</span></span>)</span>
 ;<br/>
<span class="kw">thus </span><a NAME="E5:13_1"/>
