<span class="kw">let </span><font color="Maroon" title="c1">x0</font> be   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">f</font> being   <a href="partfun1.html#NM1" title="PARTFUN1:NM.1">PartFunc</a> of <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> holds <br/> ( <font color="Olive" title="b1">f</font> <a href="fdiff_1.html#R1" title="FDIFF_1:pred.1">is_differentiable_in</a> <font color="Maroon" title="c1">x0</font> iff (  ex <font color="Olive" title="b2">N</font> being    <a href="rcomp_1.html#M1" title="RCOMP_1:mode.1">Neighbourhood</a> of <font color="Maroon" title="c1">x0</font> st <font color="Olive" title="b2">N</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Olive" title="b1">f</font> &amp; (  for <font color="Olive" title="b2">h</font> being   <a href="ordinal1.html#V9" title="ORDINAL1:attr.9">non-zero</a>   <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="fdiff_1.html#V1" title="FDIFF_1:attr.1">-convergent</a>  <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a><br/>  for <font color="Olive" title="b3">c</font> being  <a href="funct_1.html#V3" title="FUNCT_1:attr.3">V8</a>() <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a>  st  <a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Olive" title="b3">c</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><font color="Maroon" title="c1">x0</font></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span> &amp;  <a href="relset_1.html#K2"