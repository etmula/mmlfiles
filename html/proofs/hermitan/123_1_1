<span class="kw">let </span><font color="Maroon" title="c6">v</font> be   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <span class="p1">(<span class="default"><a href="vectsp10.html#K6" title="VECTSP10:func.6">VectQuot</a> (<font color="Maroon" title="c1">V</font>,<span class="p2">(<span class="default"><a href="bilinear.html#K13" title="BILINEAR:func.13">LKer</a> <font color="Maroon" title="c2">f</font></span>)</span>)</span>)</span>; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="hermitan.html#D7" target="_self" onmouseover="rs('hermitan/D7')" onmouseout="rh()">HERMITAN:def 7</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="complex1.html#K3" title="COMPLEX1:func.3">Re</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">sc</font> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<font color="Maroon" title="c6">v</font>,<font color="Maroon" title="c6">v</font>)</span>)</span></span><br/>

<span class="kw">reconsider </span><font color="Maroon" title="c7">A</font> = <font color="Maroon" title="c6">v</font> as   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <span class="p1">(<span class="default"><a href="vectsp10.html#K6" title="VECTSP10:func.6">VectQuot</a> (<font color="Maroon" title="c1">V</font>,<span class="p2">(<span class="default"><a href="bilinear.html#K14" title="BILINEAR:func.14">RKer</a> <span class="p3">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="hermitan.html#K3" title="HERMITAN:func.3">*'</a></span>)</span></span>)</span>)</span>)</span> <span class="kw">by</span> <span class="lab"><a class="ref" href="hermitan.html#T56" target="_self" onmouseover="rs('hermitan/T56')" onmouseout="rh()">Th56</a></span>;<br/>
<span class="kw">consider </span><font color="Maroon" title="c8">va</font> being   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <font color="Maroon" title="c1">V</font><span class="kw"> such that </span><br/><a NAME="E3:123_1_1_4"/><span class="lab"><font color="Green" title="E62">A15</font></span>: 
<font color="Maroon" title="c6">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c8">va</font> <a href="vectsp_4.html#K3" title="VECTSP_4:func.3">+</a> <span class="p1">(<span class="default"><a href="bilinear.html#K13" title="BILINEAR:func.13">LKer</a> <font color="Maroon" title="c2">f</font></span>)</span>
 <span class="kw">by</span> <span class="lab"><a class="ref" href="vectsp10.html#T22" onmouseover="rs('vectsp10/T22')" onmouseout="rh()">VECTSP10:22</a></span>;<br/>
<a NAME="E4:123_1_1_4"/>
<font color="Maroon" title="c7">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c8">va</font> <a href="vectsp_4.html#K3" title="VECTSP_4:func.3">+</a> <span class="p1">(<span class="default"><a href="bilinear.html#K14" title="BILINEAR:func.14">RKer</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="hermitan.html#K3" title="HERMITAN:func.3">*'</a></span>)</span></span>)</span>
 
