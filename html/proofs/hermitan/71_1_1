( <span class="p1">(<span class="default"><a href="bilinear.html#K9" title="BILINEAR:func.9">FormFunctional</a> (<font color="Maroon" title="c2">f</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="hermitan.html#K1" title="HERMITAN:func.1">*'</a></span>)</span>)</span>)</span> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<font color="Maroon" title="c4">v</font>,<font color="Maroon" title="c4">v</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">[**</a><span class="default"><span class="p2">(<span class="default"><span class="p3"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p4">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">v</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">**]</a></span> &amp; <span class="p1">(<span class="default"><a href="bilinear.html#K9" title="BILINEAR:func.9">FormFunctional</a> (<font color="Maroon" title="c2">f</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="hermitan.html#K1" title="HERMITAN:func.1">*'</a></span>)</span>)</span>)</span> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<font color="Maroon" title="c4">v</font>,<font color="Maroon" title="c4">v</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="complex1.html#K3" title="COMPLEX1:func.3">Re</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="bilinear.html#K9" title="BILINEAR:func.9">FormFunctional</a> (<font color="Maroon