<span class="kw">hence </span><a NAME="E11:88"/>
(  <a href="complex1.html#K3" title="COMPLEX1:func.3">Re</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c2">v1</font> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default"><span class="p5"><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">[**</a><span class="default"><font color="Maroon" title="c5">r</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">**]</a></span> <a href="group_1.html#K8" title="GROUP_1:func.8">*</a> <font color="Maroon" title="c6">a</font></span>)</span> <a href="vectsp_1.html#K3" title="VECTSP_1:func.3">*</a> <font color="Maroon" title="c3">w</font></span>)</span></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon" title="c2">v1</font> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default"><span class="p5"><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">[**</a><span class="default"><font color="Maroon" title="c5">r</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">**]</a></span> <a href="group_1.html#K8" title="GROUP_1:func.8">*</a> <font color="Maroon" title="c6">a</font></span>)</span> <a href="vectsp_1.html#K3" title="VECTSP_1:func.3">*</a> <font color="Maroon" title="c3">w</font></span>)</span></span>)</span>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="hermitan.html#K4" title="HERMITAN:func.4">signnorm</a> (<font color="Maroon" title="c4">f</font>,<font color="Maroon" title="c2">v1</font>)</span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0: