 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">a1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">a2</font></span><br/>

<span class="kw">reconsider </span><font color="Maroon" title="c9">b</font> =  <a href="afinsq_1.html#K2" title="AFINSQ_1:func.2">dom</a> <font color="Maroon" title="c1">A</font> as   <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  <a href="ordinal1.html#NM3" title="ORDINAL1:NM.3">Ordinal</a> ;<br/>
<a NAME="E10:71_1_1"/><span class="lab"><font color="Green" title="E51">A5</font></span>: 
 for <font color="Olive" title="b1">c</font> being   <a href="ordinal1.html#NM3" title="ORDINAL1:NM.3">Ordinal</a>  st  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Olive" title="b1">c</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c9">b</font> holds <br/><font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Olive" title="b1">c</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>1</sub></font>(<font color="Olive" title="b1">c</font>,<font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">c</font>)
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="71_1_1_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c10">c</font> be   <a href="ordinal1.html#NM3" title="ORDINAL1:NM.3">Ordinal</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c10">c</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c9">b</font> implies <font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c10">c</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>1</sub></font>(<font color="Maroon" title="c10">c</font>,<font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c10">c</font>) )</span><br/>

<span class="kw">assume </span><a NAME="E1:71_1_1_2"/>
 <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c10">c</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c9">b</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c10">c</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>1</sub></font>(<font color="Maroon" title="c10">c</font>,<font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c10">c</font>)</span><br/>

<a NAME="E2:71_1_1_2"/><span class="kw">then </span><span class="lab"><font color="Green" title="E52">A6</font></span>: 
<font color="Maroon" title="c10">c</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c9">b</font>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="ordinal3.html#T3" onmouseover="rs('ordinal3/T3')" onmouseout="rh()">ORDINAL3:3</a></span>;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c11">n</font> = <font color="Maroon" title="c10">c</font> as    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="ordinal1.html#K4" title="ORDINAL1:func.4">omega</a>  ;<br/>
<a NAME="E4:71_1_1_2"/><span class="lab"><font color="Green" title="E53">A7</font></span>: 
 <a href="card_1.html#K5" title="CARD_1:func.5">Segm</a> <span class="p1">(<span class="default"><font color="Maroon" title="c11">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <span class="p1">(<span class="default"><a href="card_1.html#K5" title="CARD_1:func.5">Segm</a> <font color="Maroon" title="c11">n</font></span>)</span>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="nat_1.html#T38" onmouseover="rs('nat_1/T38')" onmouseout="rh()">NAT_1:38</a></span>;<br/>
<a NAME="E5:71_1_1_2"/>
<font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c11">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">H<sub>1</sub></font>(<font color="Maroon" title="c11">n</font>,<font color="Maroon" title="c7">f1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">n</font>)
 
