( <font color="Maroon" title="c6">r</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp;  <a href="metric_1.html#K8" title="METRIC_1:func.8">Ball</a> (<font color="Maroon" title="c4">p</font>,<font color="Maroon" title="c6">r</font>) <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c3">P1</font> )
