 for <font color="Olive" title="b1">p</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <a href="topmetr.html#K3" title="TOPMETR:func.3">R^1</a><br/>  for <font color="Olive" title="b2">r</font> being   <a href="xxreal_0.html#V2" title="XXREAL_0:attr.2">positive</a>  <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  ex <font color="Olive" title="b3">W</font> being   <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">open</a>  <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">T</font> st <br/>( <font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">p</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b3">W</font> &amp; <font color="Olive" title="b3">W</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c2">f</font> <a href="relset_1.html#K7" title="RELSET_1:func.7">.:</a> <span class="p1"><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">].</a><span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b1">p</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Olive" title="b2">r</font></span>)</span>,<span class="p2">(<span class="default"><font color="Olive" title="b1">p</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Olive" title="b2">r</font></span>)</span></span><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">.[</a></span> )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c2">f</font> is  <a href="t_0topsp.html#V1" title="T_0TOPSP:attr.1">open</a> </span><br/>

