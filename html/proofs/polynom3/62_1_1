<span class="kw">assume </span><a NAME="E1:62_1_1_4"/><span class="lab"><font color="Green" title="E38">A10</font></span>: 
( <font color="Maroon" title="c8">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c10">p</font> &amp; <font color="Maroon" title="c9">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">q</font> )
