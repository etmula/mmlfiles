 for <font color="Olive" title="b1">x</font>, <font color="Olive" title="b2">y</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c2">Ca</font> holds  <font color="Maroon">S<sub>2</sub></font>[<font color="Olive" title="b1">x</font>,<font color="Olive" title="b2">y</font>,<font color="Maroon" title="c6">Mu</font> <a href="binop_1.html#K4" title="BINOP_1:func.4">.</a> (<font color="Olive" title="b1">x</font>,<font color="Olive" title="b2">y</font>)]
