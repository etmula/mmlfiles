 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>

<span class="kw">thus </span><a NAME="E2:62_1_1_3"/>
( <font color="Maroon" title="c8">x</font> is   <a href="polynom3.html#NM1" title="POLYNOM3:NM.1">Polynomial</a> of <font color="Maroon" title="c1">L</font> implies <font color="Maroon" title="c8">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c7">P</font> )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>

<span class="kw">thus </span><a NAME="E14:62_1_1"/>
 for <font color="Olive" title="b1">x</font>, <font color="Olive" title="b2">y</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c7">P</font><br/>  for <font color="Olive" title="b3">p</font>, <font color="Olive" title="b4">q</font> being   <a href="struct_0.html#NM9" title="STRUCT_0:NM.9">sequence</a> of <font color="Maroon" title="c1">L</font>  st <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">p</font> &amp; <font color="Olive" title="b2">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">q</font> holds <br/><font color="Olive" title="b1">x</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Olive" title="b2">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">p</font> <a href="normsp_1.html#K1" title="NORMSP_1:func.1">+</a> <font color="Olive" title="b4">q</font>
  <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( (  for <font color="Olive" title="b1">x</font>, <font color="Olive" title="b2">y</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c7">P</font><br/>  for <font color="Olive" title="b3">p</font>, <font color="Olive" title="b4">q</font> being   <a href="struct_0.html#NM9" title="STRUCT_0:NM.9">sequence</a> of <font color="Maroon" title="c1">L</font>  st <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">p</font> &amp; <font color="Olive" title="b2">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">q</font> holds <br/><font color="Olive" title="b1">x</font> <a href="algstr_0.html#K6" title="ALGSTR_0:func.6">*</a> <font color="Olive" title="b2">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">p</font> <a href="polynom3.html#K11" title="POLYNOM3:func.11">*'</a> <font color="Olive" title="b4">q</font> ) &amp;  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c7">P</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polynom3.html#K9" title="POLYNOM3:func.9">0_.</a> <font color="Maroon" title="c1">L</font> &amp;  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <font color="Maroon" title="c7">P</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polynom3.html#K10" title="POLYNOM3:func.10">1_.</a> <font color="Maroon" title="c1">L</font> )</span><div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="62_1_1_4"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c8">x</font>, <font color="Maroon" title="c9">y</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c7">P</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">p</font>, <font color="Olive" title="b2">q</font> being   <a href="struct_0.html#NM9" title="STRUCT_0:NM.9">sequence</a> of <font color="Maroon" title="c1">L</font>  st <font color="Maroon" title="c8">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">p</font> &amp; <font color="Maroon" title="c9">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">q</font> holds <br/><font color="Maroon" title="c8">x</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c9">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">p</font> <a href="normsp_1.html#K1" title="NORMSP_1:func.1">+</a> <font color="Olive" title="b2">q</font></span><br/><span class="kw">let </span><font color="Maroon" title="c10">p</font>, <font color="Maroon" title="c11">q</font> be   <a href="struct_0.html#NM9" title="STRUCT_0:NM.9">sequence</a> of <font color="Maroon" title="c1">L</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c8">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c10">p</font> &amp; <font color="Maroon" title="c9">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">q</font> implies