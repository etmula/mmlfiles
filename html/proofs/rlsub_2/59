(  <a href="lattices.html#G3" title="LATTICES:aggr.3">LattStr</a>(# <span class="p1">(<span class="default"><a href="rlsub_2.html#K3" title="RLSUB_2:func.3">Subspaces</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K5" title="RLSUB_2:func.5">SubJoin</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K6" title="RLSUB_2:func.6">SubMeet</a> <font color="Maroon" title="c1">V</font></span>)</span> #) is  <a href="lattices.html#V4" title="LATTICES:attr.4">join-commutative</a>  &amp;  <a href="lattices.html#G3" title="LATTICES:aggr.3">LattStr</a>(# <span class="p1">(<span class="default"><a href="rlsub_2.html#K3" title="RLSUB_2:func.3">Subspaces</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K5" title="RLSUB_2:func.5">SubJoin</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K6" title="RLSUB_2:func.6">SubMeet</a> <font color="Maroon" title="c1">V</font></span>)</span> #) is  <a href="lattices.html#V5" title="LATTICES:attr.5">join-associative</a>  &amp;  <a href="lattices.html#G3" title="LATTICES:aggr.3">LattStr</a>(# <span class="p1">(<span class="default"><a href="rlsub_2.html#K3" title="RLSUB_2:func.3">Subspaces</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K5" title="RLSUB_2:func.5">SubJoin</a> <font color="Maroon" title="c1">V</font></span>)</span>,<span class="p1">(<span class="default"><a href="rlsub_2.html#K6" title="RLSUB_2:func.6">SubMeet</a> <font color="Maroon" title="c1">V</font></span>)</span> #) is  <a href="lattices.html#V8" title="LATTICES:attr.8">meet-absorbing</a>  &amp;  <a href="lattices.html#G3" title="LATTICES:aggr.3">LattStr</a>(# <span class="p1">(<span class="default"><a href="rlsub_2.ht