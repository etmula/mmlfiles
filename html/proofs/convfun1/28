 for <font color="Olive" title="b1">x1</font>, <font color="Olive" title="b2">x2</font> being   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c1">X</font><br/>  for <font color="Olive" title="b3">p</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Olive" title="b3">p</font> &amp; <font color="Olive" title="b3">p</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> 1 holds <br/><font color="Maroon" title="c2">f</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b3">p</font> <a href="rlvect_1.html#K1" title="RLVECT_1:func.1">*</a> <font color="Olive" title="b1">x1</font></span>)</span> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default">1 <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Olive" title="b3">p</font></span>)</span> <a href="rlvect_1.html#K1" title="RLVECT_1:func.1">*</a> <font color="Olive" title="b2">x2</font></span>)</span></span>)</span> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <span class="p1">(<span class="default"><font color="Olive" title="b3">p</font> <a href="xxreal_3.html#K4" title="XXREAL_3:func.4">*</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x1</font></span>)</span></span>)</span> <a href="xxreal_3.html#K1" title="XXREAL_3:func.1">+</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default">1 <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Olive" title="b3">p</font></span>)</span> <a href="xxreal_3.html#K4" title="