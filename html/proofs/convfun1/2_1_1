<span class="kw">thus </span><a NAME="E5:2_1_1"/>
 for <font color="Olive" title="b1">x</font> being   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c1">X</font><br/>  for <font color="Olive" title="b2">y</font> being   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c2">Y</font>  st <font color="Maroon" title="c5">z</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Olive" title="b1">x</font>,<font color="Olive" title="b2">y</font></span><a href="domain_1.html#K1" title="DOMAIN_1:func.1">]</a></span> holds <br/><font color="Maroon" title="c3">g</font> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (<font color="Maroon" title="c4">a</font>,<font color="Maroon" title="c5">z</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"> the <a href="rlvect_1.html#U1" title="RLVECT_1:sel.1">Mult</a> of <font color="Maroon" title="c1">X</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c4">a</font>,<font color="Olive" title="b1">x</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span></span>)</span>,<span class="p2">(<span class="default"> the <a href="rlvect_1.html#U1" title="RLVECT_1:sel.1">Mult</a> of <font color="Maroon" title="c2">Y</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c4">a</font>,<font color="Olive" title="b2">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span></span>)</span></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span>
