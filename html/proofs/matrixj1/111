<span class="kw">let </span><font color="Maroon" title="c6">R1</font>, <font color="Maroon" title="c7">R2</font> be   <a href="matrixj1.html#NM4" title="MATRIXJ1:NM.4">FinSequence_of_Square-Matrix</a> of <font color="Maroon" title="c1">K</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">a1</font>, <font color="Olive" title="b2">a2</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">K</font>  st  <a href="matrixj1.html#K10" title="MATRIXJ1:func.10">Len</a> <font color="Maroon" title="c6">R1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrixj1.html#K10" title="MATRIXJ1:func.10">Len</a> <font color="Maroon" title="c7">R2</font> &amp;  <a href="matrixj1.html#K11" title="MATRIXJ1:func.11">Width</a> <font color="Maroon" title="c6">R1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrixj1.html#K11" title="MATRIXJ1:func.11">Width</a> <font color="Maroon" title="c7">R2</font> &amp;  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c6">R1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  holds <br/><span class="p1">(<span class="default"><a href="matrixj1.html#K20" title="MATRIXJ1:func.20">block_diagonal</a> (<font color="Maroon" title="c6">R1</font>,<font color="Olive" title="b1">a1</font>)</span>)</span> <a href="matrix_3.html#K3" title="MATRIX_3:func.3">+</a> <span class="p1">(<span class="default"><a href="matrixj1.html#K20" title="MATRIXJ1:func.20">block_diagonal</a> (<font color="Maroon" title="c7">R2</font>,<font color="Olive" title="b2">a2</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrixj1.html#K20" title="