( <font color="Maroon" title="c4">k</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">f</font> <a href="nat_1.html#K7" title="NAT_1:func.7">^\</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span> &amp; <span class="p1">(<span class="default"><font color="Maroon" title="c1">f</font> <a href="nat_1.html#K7" title="NAT_1:func.7">^\</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">k</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  )
 ;<br/>
<a NAME="E6:9"/>
<font color="Maroon" title="c1">f</font> <a href="nat_1.html#K7" title="NAT_1:func.7">^\</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> is  <a href="series_1.html#V1" title="SERIES_1:attr.1">summable</a> 
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="series_1.html#T12" onmouseover="rs('series_1/T12')" onmouseout="rh()">SERIES_1:12</a></span>;<br/>
<span class="kw">hence </span><a NAME="E7:9"/>
 <a href="series_1.html#K4" title="SERIES_1:func.4">Sum</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">f</font> <a href="nat_1.html#K7" title="NAT_1:func.7">^\</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> 
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E5:9"><span class="lab"><font color="Green" title="E7">A6</font></span></a>, <a class="ref" href="rsspace2.html#T3" onmouseover="rs('rsspace2/T3')" onmouseout="rh()">RSSPACE2:3</a>, <a class="txt" href="#E2:9"><span class="lab"><font color="Green" title="E6">A2</font></span></a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


