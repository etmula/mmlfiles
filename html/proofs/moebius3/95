 <a href="moebius3.html#K11" title="MOEBIUS3:func.11">ReciProducts</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="moebius3.html#K11" title="MOEBIUS3:func.11">ReciProducts</a> <font color="Maroon" title="c1">n</font></span>)</span> <a href="xboole_0.html#K2" title="XBOOLE_0:func.2">\/</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default">1 <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p3">(<span class="default"><a href="rvsum_1.html#K19" title="RVSUM_1:func.19">Product</a> <span class="p4">(<span class="default"><a href="finseq_1.html#K14" title="FINSEQ_1:func.14">Sgm</a> <font color="Olive" title="b1">X</font></span>)</span></span>)</span></span>)</span> where <font color="Olive" title="b1">X</font> is   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="moebius3.html#K4" title="MOEBIUS3:func.4">SetPrimes</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span> : <font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b1">X</font> </span> } </span> 
