<span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <span class="p3">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Maroon" title="c5">g</font></span>)</span></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> <font color="Maroon" title="c3">k</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">x</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default">2 <a href="power.html#K2" title="POWER:func.2">to_power</a> <font color="Maroon" title="c3">k</font></span>)</span> <a href="valued_1.html#K26" title="VALUED_1:func.26">(#)</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c4">f</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> <font color="Maroon" title="c3">k</font></span>)</span> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c5">g</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> <font color="Maroon" title="c3">k</font></span>)</span></span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">x</font>
