( <font color="Maroon" title="c7">f</font> is  <a href="pre_topc.html#V5" title="PRE_TOPC:attr.5">continuous</a>  &amp; (  for <font color="Olive" title="b1">t</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <a href="topmetr.html#K5" title="TOPMETR:func.5">I[01]</a> holds <br/> ( <font color="Maroon" title="c7">f</font> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (<font color="Olive" title="b1">t</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">P</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">t</font> &amp; <font color="Maroon" title="c7">f</font> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (<font color="Olive" title="b1">t</font>,1) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">P</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">t</font> &amp; <font color="Maroon" title="c7">f</font> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,<font color="Olive" title="b1">t</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">a</font> &amp; <font color="Maroon" title="c7">f</font> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (1,<font color="Olive" title="b1">t</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">b</font> ) ) )
