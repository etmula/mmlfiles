( <font color="Maroon" title="c2">A</font> is  <a href="vectsp_7.html#V1" title="VECTSP_7:attr.1">linearly-independent</a>  &amp; (  for <font color="Olive" title="b1">v</font> being   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <font color="Maroon" title="c1">V</font>  ex <font color="Olive" title="b2">a</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="int_3.html#K1" title="INT_3:func.1">INT.Ring</a> st <br/>( <font color="Olive" title="b2">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  &amp; <font color="Olive" title="b2">a</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; <font color="Olive" title="b2">a</font> <a href="vectsp_1.html#K3" title="VECTSP_1:func.3">*</a> <font color="Olive" title="b1">v</font> <a href="struct_0.html#R1" title="STRUCT_0:pred.1">in</a>  <a href="vectsp_7.html#K1" title="VECTSP_7:func.1">Lin</a> <font color="Maroon" title="c2">A</font> ) ) )
