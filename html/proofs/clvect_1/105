<span class="kw">let </span><font color="Maroon" title="c1">V</font> be   <a href="clvect_1.html#NM2" title="CLVECT_1:NM.2">ComplexLinearSpace</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">V</font> is    <a href="clvect_1.html#M2" title="CLVECT_1:mode.2">Coset</a> of  <a href="clvect_1.html#K4" title="CLVECT_1:func.4">(Omega).</a> <font color="Maroon" title="c1">V</font></span><br/>

<span class="kw">set </span><font color="Maroon" title="c2">v</font> =  the   <a href="clvect_1.html#NM1" title="CLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c1">V</font>;<br/>
<a NAME="E1:105"/>
(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">V</font> is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">V</font> iff  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">V</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">V</font> )
 
;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c3">A</font> =  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">V</font> as   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">V</font> ;<br/>
<a NAME="E3:105"/>
<font color="Maroon" title="c3">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the   <a href="clvect_1.html#NM1" title="CLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c1">V</font> <a href="clvect_1.html#K5" title="CLVECT_1:func.5">+</a> <span class="p1">(<span class="default"><a href="clvect_1.html#K4" title="CLVECT_1:func.4">(Omega).</a> <font color="Maroon" title="c1">V</font></span>)</span>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="clvect_1.html#T65" target="_self" onmouseover="rs('clvect_1/T65')" onmouseout="rh()">Th65</a></span>;<br/>
<span class="kw">hence </span><a NAME="E4:105"/>
