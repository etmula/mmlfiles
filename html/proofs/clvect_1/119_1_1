<span class="kw">let </span><font color="Maroon" title="c4">u</font>, <font color="Maroon" title="c5">v</font>, <font color="Maroon" title="c6">w</font> be   <a href="clvect_1.html#NM1" title="CLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c3">X</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <span class="p1">(<span class="default"><font color="Maroon" title="c4">u</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c5">v</font></span>)</span> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c6">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">u</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">v</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c6">w</font></span>)</span></span><br/>

<span class="kw">reconsider </span><font color="Maroon" title="c7">u9</font> = <font color="Maroon" title="c4">u</font>, <font color="Maroon" title="c8">v9</font> = <font color="Maroon" title="c5">v</font>, <font color="Maroon" title="c9">w9</font> = <font color="Maroon" title="c6">w</font> as   <a href="clvect_1.html#NM1" title="CLVECT_1:NM.1">VECTOR</a> of <span class="p1">(<span class="default"><a href="clvect_1.html#K3" title="CLVECT_1:func.3">(0).</a>  the   <a href="clvect_1.html#NM2" title="CLVECT_1:NM.2">ComplexLinearSpace</a></span>)</span> ;<br/>
