<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="6_3"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c6">Z</font> be   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <font color="Maroon" title="c1">Omega</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c6">Z</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="kolmog01.html#K1" title="KOLMOG01:func.1">Indep</a> (<font color="Maroon" title="c4">B</font>,<font color="Maroon" title="c3">P</font>) implies <font color="Maroon" title="c6">Z</font> <a href="subset_1.html#K3" title="SUBSET_1:func.3">`</a>  <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="kolmog01.html#K1" title="KOLMOG01:func.1">Indep</a> (<font color="Maroon" title="c4">B</font>,<font color="Maroon" title="c3">P</font>) )</span><br/>

<span class="kw">assume </span><a NAME="E1:6_3"/><span class="lab"><font color="Green" title="E10">A6</font></span>: 
<font color="Maroon" title="c6">Z</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="kolmog01.html#K1" title="KOLMOG01:func.1">Indep</a> (<font color="Maroon" title="c4">B</font>,<font color="Maroon" title="c3">P</font>)
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c6">Z</font> <a href="subset_1.html#K3" title="SUBSET_1:func.3">`</a>  <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="kolmog01.html#K1" title="KOLMOG01:func.1">Indep</a> (<font color="Maroon" title="c4">B</font>,<font color="Maroon" title="c3">P</font>)</span><br/>

