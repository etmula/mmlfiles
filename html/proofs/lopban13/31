<span class="kw">hence </span><a NAME="E11:31"/>
( <font color="Maroon" title="c8">L</font> <a href="nfcont_1.html#R3" title="NFCONT_1:pred.3">is_continuous_on</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <span class="p1"><a href="prvect_3.html#K7" title="PRVECT_3:func.7">[:</a><span class="default"><span class="p2">(<span class="default"><a href="lopban_1.html#K16" title="LOPBAN_1:func.16">R_NormSpace_of_BoundedLinearOperators</a> (<font color="Maroon" title="c1">X</font>,<font color="Maroon" title="c2">Y</font>)</span>)</span>,<span class="p2">(<span class="default"><a href="lopban_1.html#K16" title="LOPBAN_1:func.16">R_NormSpace_of_BoundedLinearOperators</a> (<font color="Maroon" title="c2">Y</font>,<font color="Maroon" title="c3">Z</font>)</span>)</span></span><a href="prvect_3.html#K7" title="PRVECT_3:func.7">:]</a></span> &amp; (  for <font color="Olive" title="b1">u</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="lopban_1.html#K16" title="LOPBAN_1:func.16">R_NormSpace_of_BoundedLinearOperators</a> (<font color="Maroon" title="c1">X</font>,<font color="Maroon" title="c2">Y</font>)</span>)</span><br/>  for <font color="Olive" title="b2">v</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="lopban_1.html#K16" title="LOPBAN_1:func.16">R_NormSpace_of_BoundedLinearOperators</a> (<font color="Maroon" title="c2">Y</font>,<font color="Maroon" title="c3">Z</font>)</span>)</span> holds  <font color="Maroon" title="c8">L</font> <a href="lopban_8.html#K2" title="LOPBAN_8:func.2">.</a> (<font color="Olive" title="b1">u</font>,<font color="Olive" title="b2">v</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">v</font> <a href="ndiff_2.html#K2" title="NDIFF_2:func.2">*</a> <font color="Olive" title="b1">u</font> ) )
