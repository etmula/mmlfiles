<span class="kw">let </span><font color="Maroon" title="c9">v</font> be   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="lopban_1.html#K16" title="LOPBAN_1:func.16">R_NormSpace_of_BoundedLinearOperators</a> (<font color="Maroon" title="c2">Y</font>,<font color="Maroon" title="c1">X</font>)</span>)</span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">v</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="lopban13.html#K2" title="LOPBAN13:func.2">InvertibleOperators</a> (<font color="Maroon" title="c2">Y</font>,<font color="Maroon" title="c1">X</font>) implies <font color="Maroon" title="c8">L</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c9">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="lopban13.html#K1" title="LOPBAN13:func.1">Inv</a> <font color="Maroon" title="c9">v</font> )</span><br/>

