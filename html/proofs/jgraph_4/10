<span class="kw">hence </span><a NAME="E30:10"/>
 ex <font color="Olive" title="b1">g</font> being   <a href="struct_0.html#NM6" title="STRUCT_0:NM.6">Function</a> of <font color="Maroon" title="c1">X</font>,<a href="topmetr.html#K3" title="TOPMETR:func.3">R^1</a> st <br/>( (  for <font color="Olive" title="b2">p</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <font color="Maroon" title="c1">X</font><br/>  for <font color="Olive" title="b3">r1</font>, <font color="Olive" title="b4">r2</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st <font color="Maroon" title="c2">f1</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b3">r1</font> &amp; <font color="Maroon" title="c3">f2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">r2</font> holds <br/><font color="Olive" title="b1">g</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b4">r2</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> <span class="p2">(<span class="default"><a href="square_1.html#K3" title="SQUARE_1:func.3">sqrt</a> <span class="p3"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p4">(<span class="default">1 <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p5">(<span class="default"><span class="p0">(<span class="default"><span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b3">r1</font> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <font color="Olive" title="b4">r2</font></span>)</span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c4">a</font></span>)</span> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <font color="Maroon" title="c5">b</font></span>)</span> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span></span>)</span></span>)</span> ) &amp; <font color="Olive" title="b1">g</font> is  <a href="pre_topc.html#V5" title="PRE_TOPC:attr.5">continuous</a>  )
