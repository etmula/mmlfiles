<span class="kw">let </span><font color="Maroon" title="c3">i</font>, <font color="Maroon" title="c4">j</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c3">i</font>,<font color="Maroon" title="c4">j</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="matrix_0.html#K2" title="MATRIX_0:func.2">Indices</a> <span class="p1">(<span class="default"><a href="matrixr1.html#K9" title="MATRIXR1:func.9">ColVec2Mx</a> <font color="Maroon" title="c1">x1</font></span>)</span> implies <span class="p1">(<span class="default"><a href="matrixr1.html#K9" title="MATRIXR1:func.9">ColVec2Mx</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">x1</font> <a href="rvsum_1.html#K8" title="RVSUM_1:func.8">-</a> <font color="Maroon" title="c2">x2</font></span>)</span></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c3">i</font>,<font color="Maroon" title="c4">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="matrixr1.html#K9" title="MATRIXR1:func.9">ColVec2Mx</a> <font color="Maroon" title="c1">x1</font></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c3">i</font>,<font color="Maroon" title="c4">j</font>)</span>)</span> <a href="real_1.html#K5" title="REAL_1:func.5">-</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="matrixr1.html#K9" title="MATRIXR1:func.9">ColVec2Mx</a> <font color="Ma