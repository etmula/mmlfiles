<span class="kw">let </span><font color="Maroon" title="c1">n</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">A</font> being   <a href="matrix_0.html#NM3" title="MATRIX_0:NM.3">Matrix</a> of <font color="Maroon" title="c1">n</font>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>  st <font color="Olive" title="b1">A</font> is  <a href="matrixr2.html#V1" title="MATRIXR2:attr.1">invertible</a>  holds <br/> <a href="matrixr2.html#K7" title="MATRIXR2:func.7">Inv</a> <span class="p1">(<span class="default"><a href="matrixr2.html#K7" title="MATRIXR2:func.7">Inv</a> <font color="Olive" title="b1">A</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">A</font></span><br/><span class="kw">let </span><font color="Maroon" title="c2">A</font> be   <a href="matrix_0.html#NM3" title="MATRIX_0:NM.3">Matrix</a> of <font color="Maroon" title="c1">n</font>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">A</font> is  <a href="matrixr2.html#V1" title="MATRIXR2:attr.1">invertible</a>  implies  <a href="matrixr2.html#K7" title="MATRIXR2:func.7">Inv</a> <span class="p1">(<span class="default"><a href="matrixr2.html#K7" title="MATRIXR2:func.7">Inv</a> <font color="Maroon" title="c2">A</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">A</font> )</span><br/>



