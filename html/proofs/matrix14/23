<span class="kw">thus </span><a NAME="E2:23"/>
(  ex <font color="Olive" title="b1">B</font> being   <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of <font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c2">K</font> st <br/>( <font color="Olive" title="b1">B</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Maroon" title="c3">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix_1.html#K3" title="MATRIX_1:func.3">1.</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>) &amp; <font color="Maroon" title="c3">A</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Olive" title="b1">B</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix_1.html#K3" title="MATRIX_1:func.3">1.</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>) ) implies <font color="Maroon" title="c3">A</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  )
