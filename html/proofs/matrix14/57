<span class="kw">hence </span><a NAME="E5:57"/>
( (  for <font color="Olive" title="b1">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">i</font> &amp; <font color="Olive" title="b1">i</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c1">n</font> holds <br/>( <span class="p1">(<span class="default"><font color="Maroon" title="c3">A</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <span class="p2">(<span class="default"><a href="matrix14.html#K4" title="MATRIX14:func.4">SwapDiagonal</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c4">j0</font>)</span>)</span></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">i</font>,<font color="Maroon" title="c4">j0</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">A</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">i</font>,1) &amp; <span class="p1">(<span class="default"><font color="Maroon" title="c3">A</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <span class="p2">(<span class="default"><a href="matrix14.html#K4" title="MATRIX14:func.4">SwapDiagonal</a> (<font color="Maroon" title="c2">K</font>,<font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c4">j0</font>)</span>)</span></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">i</font>,1) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">A</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">i</font>,<font color="Maroon" title="c4">j0</font>) ) ) &amp; (  for <font color="Olive" title="b1">i</font>, <font colo