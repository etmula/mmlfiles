<a NAME="E7:63"/><span class="kw">then </span>
 for <font color="Olive" title="b1">x</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">S</font><br/>  for <font color="Olive" title="b2">y</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c2">T</font> holds <br/> ( <font color="Olive" title="b2">y</font> <a href="waybel_3.html#NR2" title="WAYBEL_3:NR.2">&lt;&lt;</a> <font color="Maroon" title="c3">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">x</font> iff  ex <font color="Olive" title="b3">w</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">S</font> st <br/>( <font color="Olive" title="b3">w</font> <a href="waybel_3.html#NR2" title="WAYBEL_3:NR.2">&lt;&lt;</a> <font color="Olive" title="b1">x</font> &amp; <font color="Olive" title="b2">y</font> <a href="waybel_3.html#NR2" title="WAYBEL_3:NR.2">&lt;&lt;</a> <font color="Maroon" title="c3">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b3">w</font> ) )
 
