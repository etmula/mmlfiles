( (  for <font color="Olive" title="b1">x</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font>  st <font color="Olive" title="b1">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="valued_1.html#K32" title="VALUED_1:func.32">-</a> <font color="Maroon" title="c2">f</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font> ) &amp; (  for <font color="Olive" title="b1">x</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font>  st <font color="Olive" title="b1">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c2">f</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font> ) )
 
;<br/>
<span class="kw">hence </span><a NAME="E9:7"/>
( <font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2 <a href="relset_1.html#R2" title="RELSET_1:pred.2">=</a> <span class="p1">(<span class="default"><a href="valued_1.html#K32" title="VALUED_1:func.32">-</a> <font color="Maroon" title="c2">f</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2 &amp; <font color="Maroon" title="c2">f</font> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2 <a href="relset_1.html#R2" title="RELSET_1:pred.2">=</a> <span class="p1">(<span class="default"><a href="valued_1.html#K56" title="VALUED_1:func.56">abs</a> <font color="Maroon" title="c2">f</font></span>)</span> <a href="mesfun6c.html#K2" title="MESFUN6C:func.2">to_power</a> 2 )
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:7"><span class="lab"><font color="Green" title="E6">A1</font></span></a>, <a class="txt" href="#E6:7"><span class="lab"><font color="Green" title="E8">A3</font></span></a>, <a class="ref" href="partfun1.html#T5" onmouseover="rs('partfun1/T5')" onmouseout="rh()">PARTFUN1:5</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


