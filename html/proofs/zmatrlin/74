( <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c7">i</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 or <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c7">i</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> <span class="p1">(<span class="default"><a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> 1</span>)</span> )
