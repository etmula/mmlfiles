( ( <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V1" title="GLIB_001:attr.1">closed</a>  implies <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V1" title="GLIB_001:attr.1">closed</a>  ) &amp; ( <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V1" title="GLIB_001:attr.1">closed</a>  implies <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V1" title="GLIB_001:attr.1">closed</a>  ) &amp; ( <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V2" title="GLIB_001:attr.2">directed</a>  implies <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V2" title="GLIB_001:attr.2">directed</a>  ) &amp; ( <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V2" title="GLIB_001:attr.2">directed</a>  implies <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V2" title="GLIB_001:attr.2">directed</a>  ) &amp; ( <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V3" title="GLIB_001:attr.3">trivial</a>  implies <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V3" title="GLIB_001:attr.3">trivial</a>  ) &amp; ( <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V3" title="GLIB_001:attr.3">trivial</a>  implies <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V3" title="GLIB_001:attr.3">trivial</a>  ) &amp; ( <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V4" title="GLIB_001:attr.4">Trail-like</a>  implies <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V4" title="GLIB_001:attr.4">Trail-like</a>  ) &amp; ( <font color="Maroon" title="c4">W2</font> is  <a href="glib_001.html#V4" title="GLIB_001:attr.4">Trail-like</a>  implies <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V4" title="GLIB_001:attr.4">Trail-like</a>  ) &amp; ( <font color="Maroon" title="c3">W1</font> is  <a href="glib_001.html#V5" title="