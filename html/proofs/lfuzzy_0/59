<span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c4">R</font> <a href="fuzzy_4.html#K3" title="FUZZY_4:func.3">(#)</a> <font color="Maroon" title="c5">S</font></span>)</span> <a href="lfuzzy_0.html#K8" title="LFUZZY_0:func.8">.</a> (<font color="Maroon" title="c6">x</font>,<font color="Maroon" title="c7">z</font>)</span>)</span> <a href="lattice3.html#K12" title="LATTICE3:func.12">"/\"</a> <font color="Maroon" title="c8">a</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="yellow_0.html#K1" title="YELLOW_0:func.1">"\/"</a> (<span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><font color="Maroon" title="c4">R</font> <a href="lfuzzy_0.html#K8" title="LFUZZY_0:func.8">.</a> (<font color="Maroon" title="c6">x</font>,<font color="Olive" title="b1">y</font>)</span>)</span> <a href="lattice3.html#K12" title="LATTICE3:func.12">"/\"</a> <span class="p4">(<span class="default"><font color="Maroon" title="c5">S</font> <a href="lfuzzy_0.html#K8" title="LFUZZY_0:func.8">.</a> (<font color="Olive" title="b1">y</font>,<font color="Maroon" title="c7">z</font>)</span>)</span></span>)</span> <a href="lattice3.html#K12" title="LATTICE3:func.12">"/\"</a> <font color="Maroon" title="c8">a</font></span>)</span> where <font color="Olive" title="b1">y</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c2">Y</font> : verum </span> } </span> ,<span class="p1">(<span class="default"><a href="lfuzzy_0.html#K1" title="LFUZZY_0:func.1">RealPoset</a> <span class="p2"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,1</span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span></span>)</span>)
