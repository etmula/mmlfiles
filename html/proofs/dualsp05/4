<span class="kw">let </span><font color="Maroon" title="c6">V</font> be   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c6">V</font> &amp; <font color="Maroon" title="c6">V</font> is  <a href="rcomp_1.html#V3" title="RCOMP_1:attr.3">open</a>  implies  ex <font color="Olive" title="b1">W</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">X</font> st <br/>( <font color="Maroon" title="c4">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b1">W</font> &amp; <font color="Olive" title="b1">W</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">open</a>  &amp; <font color="Maroon" title="c2">f</font> <a href="relset_1.html#K7" title="RELSET_1:func.7">.:</a> <font color="Olive" title="b1">W</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c6">V</font> ) )</span><br/>

