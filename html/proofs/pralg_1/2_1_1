<font color="Maroon" title="c11">y9</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">s</font> where <font color="Olive" title="b1">s</font> is    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c1">A</font>,<font color="Maroon" title="c2">B</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> <a href="finseq_2.html#K3" title="FINSEQ_2:func.3">*</a>  :  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Olive" title="b1">s</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K18" title="MARGREL1:func.18">arity</a> <font color="Maroon" title="c3">f1</font> </span> } </span> 
 
