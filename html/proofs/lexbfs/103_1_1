( (  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">L</font> <a href="lexbfs.html#K7" title="LEXBFS:func.7">`1</a></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="glib_000.html#K6" title="GLIB_000:func.6">the_Vertices_of</a> <font color="Maroon" title="c1">G</font> implies  ex <font color="Olive">b<sub>1</sub></font> being   <a href="glib_000.html#NM10" title="GLIB_000:NM.10">Vertex</a> of <font color="Maroon" title="c1">G</font> st <font color="Olive">b<sub>1</sub></font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="glib_000.html#K6" title="GLIB_000:func.6">the_Vertices_of</a> <font color="Maroon" title="c1">G</font> ) &amp; (  not  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">L</font> <a href="lexbfs.html#K7" title="LEXBFS:func.7">`1</a></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="glib_000.html#K6" title="GLIB_000:func.6">the_Vertices_of</a> <font color="Maroon" title="c1">G</font> implies  ex <font color="Olive">b<sub>1</sub></font> being   <a href="glib_000.html#NM10" title="GLIB_000:NM.10">Vertex</a> of <font color="Maroon" title="c1">G</font> ex <font color="Olive" title="b2">S</font> being   non  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">empty</a>   <a href="membered.html#V6" title="MEMBERED:attr.6">natural-membered</a>   <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>   <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a>  ex <font color="Olive" title="b3">F</font> being   <a href="funct_1.html#NM1" title="FUNCT_1:NM.1">Function</a> st <br/>( <font color="Olive" title="b2">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="xtuple_0.html#K10" title="XTUPLE_0:func.10">rng</a> <font color="Olive"