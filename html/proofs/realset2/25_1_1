<span class="kw">let </span><font color="Maroon" title="c19">v</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="algstr_0.html#G2" title="ALGSTR_0:aggr.2">addLoopStr</a>(# <font color="Maroon" title="c15">B</font>,<font color="Maroon" title="c16">P</font>,<font color="Maroon" title="c17">e</font> #); <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="rlvect_1.html#D4" onmouseover="rs('rlvect_1/D4')" onmouseout="rh()">RLVECT_1:def 4</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c19">v</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <span class="p1">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="algstr_0.html#G2" title="ALGSTR_0:aggr.2">addLoopStr</a>(# <font color="Maroon" title="c15">B</font>,<font color="Maroon" title="c16">P</font>,<font color="Maroon" title="c17">e</font> #)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c19">v</font></span><br/>

