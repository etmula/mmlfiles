<span class="kw">let </span><font color="Maroon" title="c2">n</font>, <font color="Maroon" title="c3">m</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c3">m</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Maroon" title="c2">n</font> implies <font color="Maroon" title="c1">seq</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Maroon" title="c3">m</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a> <font color="Maroon" title="c1">seq</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Maroon" title="c2">n</font> )</span><br/>

