( (  for <font color="Olive" title="b1">y</font> being   <a href="qc_lang1.html#NM5" title="QC_LANG1:NM.5">bound_QC-variable</a> of <font color="Maroon" title="c1">Al</font>  st <font color="Olive" title="b1">y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c9">vS1</font> holds <br/> not <font color="Olive" title="b1">y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="qc_lang1.html#K24" title="QC_LANG1:func.24">still_not-bound_in</a> <span class="p1">(<span class="default"><a href="cqc_lang.html#K11" title="CQC_LANG:func.11">All</a> (<font color="Maroon" title="c2">x</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c5">S</font> <a href="sublemma.html#K2" title="SUBLEMMA:func.2">`1</a></span>)</span>)</span>)</span> ) &amp; (  for <font color="Olive" title="b1">y</font> being   <a href="qc_lang1.html#NM5" title="QC_LANG1:NM.5">bound_QC-variable</a> of <font color="Maroon" title="c1">Al</font>  st <font color="Olive" title="b1">y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c10">vS2</font> holds <br/><font color="Maroon" title="c10">vS2</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c8">v</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">y</font> ) &amp;  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><a href="sublemma.html#K13" title="SUBLEMMA:func.13">NEx_Val</a> (<font color="Maroon" title="c8">v</font>,<font color="Maroon" title="c5">S</font>,<font color="Maroon" title="c2">x</font>,<font color="Maroon" title="c6">xSQ</font>)</span>)</span> <a href="xboole_0.html#R1" title="XBOOLE_0:pred.1">misses</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c10">vS2</font> )
 <span class="kw">and </span><br/><a NAME="E8:107"/><span class="lab"><font color="Green" title="E90">A4</font></span>: 
<font color="Maroon" title="c8">v</font> <a href="sublemma.html#K1" title="SUBLEMMA:func.1">.</a> <span class="p1">(<span class="default"><a href="sublemma.html#K3" title="SUBLEMMA:func.3">Val_S</a> (<font color="Maroon" title="c8">v</font>,<span class="p2">(<span class="default"><a href="sublemma.html#K9" title="SUBLEMMA:func.9">CQCSub_All</a> (<span class="p3"><a href="sublemma.html#K7" title="SUBLEMMA:func.7">[</a><span class="default"><font color="Maroon" title="c5">S</font>,<font color="Maroon" title="c2">x</font></span><a href="sublemma.html#K7" title="SUBLEMMA:func.7">]</a></span>,<font color="Maroon" title="c6">xSQ</font>)</span>)</span>)</span>)</span> <a href="funct_2.html#R2" title="FUNCT_2:pred.2">=</a> <font color="Maroon" title="c8">v</font> <a href="sublemma.html#K1" title="SUBLEMMA:func.1">.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="sublemma.html#K13" title="SUBLEMMA:func.13">NEx_Val</a> (<font color="Maroon" title="c8">v</font>,<font color="Maroon" title="c5">S</font>,<font color="Maroon" title="c2">x</font>,<font color="Maroon" title="c6">xSQ</font>)</span>)</span> <a href="sublemma.html#K14" title="SUBLEMMA:func.14">+*</a> <font color="Maroon" title="c9">vS1</font></span>)</span> <a href="sublemma.html#K14" title="SUBLEMMA:func.14">+*</a> <font color="Maroon" title="c10">vS2</font></span>)</span>
