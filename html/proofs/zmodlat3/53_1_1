<span class="kw">thus </span><a NAME="E22:53_1_1"/>
(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c15">L1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="zmodlat3.html#K5" title="ZMODLAT3:func.5">DualSet</a> <font color="Maroon" title="c1">L</font> &amp;  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c15">L1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <span class="p1">(<span class="default"><a href="zmodul08.html#K2" title="ZMODUL08:func.2">DivisibleMod</a> <font color="Maroon" title="c1">L</font></span>)</span> &amp;  the <a href="algstr_0.html#U1" title="ALGSTR_0:sel.1">addF</a> of <font color="Maroon" title="c15">L1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the <a href="algstr_0.html#U1" title="ALGSTR_0:sel.1">addF</a> of <span class="p1">(<span class="default"><a href="zmodul08.html#K2" title="ZMODUL08:func.2">DivisibleMod</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="realset1.html#K1" title="REALSET1:func.1">||</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c15">L1</font> &amp;  the <a href="vectsp_1.html#U1" title="VECTSP_1:sel.1">lmult</a> of <font color="Maroon" title="c15">L1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the <a href="vectsp_1.html#U1" title="VECTSP_1:sel.1">lmult</a> of <span class="p1">(<span class="default"><a href="zmodul08.html#K2" title="ZMODUL08:func.2">DivisibleMod</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"> the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <a href="int_3.html#K1" title="INT_3:func.1">INT.Ring</a>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c15">L1</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> &amp;  the <a href="zmodlat1.html#U1" title="ZMODLAT1:sel.1">scalar</a> of <font color="Maroon" title="c15">L1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="zmodlat2.html#K2" title="ZMODLAT2:func.2">ScProductDM</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="realset1.html#K1" title="REALSET1:func.1">||</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c15">L1</font> )
