<span class="kw">let </span><font color="Maroon" title="c2">V</font> be   <a href="bhsp_1.html#NM1" title="BHSP_1:NM.1">RealUnitarySpace</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">V</font> is    <a href="rusub_1.html#M2" title="RUSUB_1:mode.2">Coset</a> of  <a href="rusub_1.html#K2" title="RUSUB_1:func.2">(Omega).</a> <font color="Maroon" title="c2">V</font></span><br/>

<span class="kw">set </span><font color="Maroon" title="c3">v</font> =  the   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c2">V</font>;<br/>
<a NAME="E1:68"/>
(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">V</font> is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c2">V</font> iff  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">V</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">V</font> )
 
;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c4">A</font> =  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">V</font> as   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c2">V</font> ;<br/>
<a NAME="E3:68"/>
<font color="Maroon" title="c4">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <font color="Maroon" title="c2">V</font> <a href="rusub_1.html#K3" title="RUSUB_1:func.3">+</a> <span class="p1">(<span class="default"><a href="rusub_1.html#K2" title="RUSUB_1:func.2">(Omega).</a> <font color="Maroon" title="c2">V</font></span>)</span>
 
