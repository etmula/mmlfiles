; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c14">K</font> is  <a href="prob_1.html#V2" title="PROB_1:attr.2">non-ascending</a>  &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c14">K</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Olive" title="b1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c8">P</font> <a href="measur11.html#K17" title="MEASUR11:func.17">.</a> <font color="Olive" title="b1">n</font></span>)</span>,<font color="Maroon" title="c9">x</font>)</span>)</span> <a href="measure1.html#K2" title="MEASURE1:func.2">/\</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c7">V</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> ) &amp;  <a href="setlim_1.html#NK7" title="SETLIM_1:NK.7">lim</a> <font color="Maroon" title="c14">K</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c6">E</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> <a href="measure1.html#K2" title="MEASURE1:func.2">/\</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c7">V</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> )</span><br/>

<span class="kw">thus </span><a NAME="E16:138"/>
( <font color="Maroon" title="c14">K</font> is  <a href="prob_1.html#V2" title="PROB_1:attr.2">non-ascending</a>  &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c14">K</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Olive" title="b1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c8">P</font> <a href="measur11.html#K17" title="MEASUR11:func.17">.</a> <font color="Olive" title="b1">n</font></span>)</span>,<font color="Maroon" title="c9">x</font>)</span>)</span> <a href="measure1.html#K2" title="MEASURE1:func.2">/\</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c7">V</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> ) &amp;  <a href="setlim_1.html#NK7" title="SETLIM_1:NK.7">lim</a> <font color="Maroon" title="c14">K</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c6">E</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> <a href="measure1.html#K2" title="MEASURE1:func.2">/\</a> <span class="p1">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c7">V</font>,<font color="Maroon" title="c9">x</font>)</span>)</span> )
