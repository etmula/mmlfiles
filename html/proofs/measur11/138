<span class="kw">let </span><font color="Maroon" title="c12">n</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c11">G</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Maroon" title="c12">n</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c3">S1</font></span><br/>

<a NAME="E1:138_1"/>
