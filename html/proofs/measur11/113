<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:113"/><span class="lab"><font color="Green" title="E79">A1</font></span>: 
<font color="Maroon" title="c3">K</font> is   <a href="prob_3.html#NM2" title="PROB_3:NM.2">MonotoneClass</a> of <font color="Maroon" title="c1">X</font>
