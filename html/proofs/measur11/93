<a NAME="E1:93"/>
 for <font color="Olive" title="b1">x</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X1</font> holds  <span class="p1">(<span class="default"><a href="mesfunc9.html#K6" title="MESFUNC9:func.6">ProjMap2</a> (<span class="p2">(<span class="default"><a href="measur11.html#K1" title="MEASUR11:func.1">chi</a> (<span class="p3">(<span class="default"><font color="Maroon" title="c7">E</font> <a href="subset_1.html#K8" title="SUBSET_1:func.8">/\</a> <span class="p4"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c10">U</font>,<font color="Maroon" title="c2">X2</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span></span>)</span>,<span class="p3"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c1">X1</font>,<font color="Maroon" title="c2">X2</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span>)</span>)</span>,<font color="Maroon" title="c9">y</font>)</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="measur11.html#K1" title="MEASUR11:func.1">chi</a> (<span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="measur11.html#K10" title="MEASUR11:func.10">Measurable-Y-section</a> (<font color="Maroon" title="c7">E</font>,<font color="Maroon" title="c9">y</font>)</span>)</span> <a href="measure1.html#K2" title="MEASURE1:func.2">/\</a> <font color="Maroon" title="c10">U</font></span>)</span>,<font color="Maroon" title="c1">X1</font>)</span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">x</font>
 
