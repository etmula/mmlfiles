<span class="kw">let </span><font color="Maroon" title="c4">i</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c4">i</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <span class="p1">(<span class="default">(<font color="Maroon" title="c1">a</font>,<font color="Maroon" title="c2">b</font>) <a href="newton04.html#K4" title="NEWTON04:func.4">Subnomial</a> <span class="p2">(<span class="default"><font color="Maroon" title="c3">n</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span> implies <span class="p1">(<span class="default">(<font color="Maroon" title="c1">a</font>,<font color="Maroon" title="c2">b</font>) <a href="newton04.html#K4" title="NEWTON04:func.4">Subnomial</a> <span class="p2">(<span class="default"><font color="Maroon" title="c3">n</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2"><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">&lt;*</a><span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c1">a</font> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <span class="p4">(<span class="default"><font color="Maroon" title="c3">n</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span></span>)</span></span><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">*&gt;</a></span> <a href="finseq_1.html#K7" title="FINSEQ_1:func.7">^</a> <span class="p2">(<span class="default"><