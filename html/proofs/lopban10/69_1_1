<span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c3">u</font> <a href="lopban10.html#K9" title="LOPBAN10:func.9">.</a> <span class="p3">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <span class="p4">(<span class="default"><a href="prvect_2.html#K14" title="PRVECT_2:func.14">product</a> <font color="Maroon" title="c1">X</font></span>)</span></span>)</span></span>)</span></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <span class="p1"> { <span class="default"> <span class="p2"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c3">u</font> <a href="lopban10.html#K9" title="LOPBAN10:func.9">.</a> <font color="Olive" title="b1">t</font></span>)</span></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> where <font color="Olive" title="b1">t</font> is   <a href="rlvect_1.html#NM1" title="RLVECT_1:NM.1">VECTOR</a> of <span class="p1">(<span class="default"><a href="prvect_2.html#K14" title="PRVECT_2:func.14">product</a> <font color="Maroon" title="c1">X</font></span>)</span> :  for <font color="Olive" title="b2">i</font> being    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c1">X</font> holds  <span class="p1"><a href="normsp_0.html#K2" title="NORMSP_0:func.2">||.</a><span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b1">t</font> <a href="lopban10.html#K10" title="LOPBAN10:func.10">.</a> <font color="Olive" title="b2">i</font></span>)</span></span><a href="normsp_0.html#K2" title="NORMSP_0:func.2">.||</a></span> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> 1 </span> } </s