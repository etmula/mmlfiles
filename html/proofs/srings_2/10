( <font color="Maroon" title="c5">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">y</font> &amp; <font color="Maroon" title="c6">y</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  )
 ;<br/>
<span class="kw">thus </span><a NAME="E4:10_2_1"/>
<font color="Maroon" title="c5">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c4">SD</font>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:10_2"><span class="lab"><font color="Green" title="E6">A4</font></span></a>, <a class="txt" href="#E3:10_2_1"><span class="lab"><font color="Green" title="E8">A6</font></span></a>, <a class="ref" href="finsub_1.html#D5" onmouseover="rs('finsub_1/D5')" onmouseout="rh()">FINSUB_1:def 5</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<span class="kw">hence </span><a NAME="E4:10_2"/>
