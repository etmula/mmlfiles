( (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">n</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c2">p</font> holds <br/> ex <font color="Olive" title="b2">d</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="ec_pf_1.html#NK2" title="EC_PF_1:NK.2">GF</a> <font color="Maroon" title="c2">p</font></span>)</span> st <br/>( <font color="Olive" title="b2">d</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">n</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> 1 &amp; <font color="Maroon" title="c6">F</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="ec_pf_1.html#K2" title="EC_PF_1:func.2">Lege_p</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Olive" title="b2">d</font> <a href="binom.html#K2" title="BINOM:func.2">|^</a> 3</span>)</span> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <span class="p3">(<span class="default"><font color="Maroon" title="c3">a</font> <a href="group_1.html#K8" title="GROUP_1:func.8">*</a> <font color="Olive" title="b2">d</font></span>)</span></span>)</span> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <font color="Maroon" title="c4">b</font></span>)</span> ) ) &amp;  <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <span class="p1">(<span class="default"><a href="eqrel_1.html#K8" title="EQREL_1:func.8">Class</a> <span class="p2">(<span class="default"><a href="ec_pf_1.html#K8" title="EC_PF_1:func.8">R_EllCur</a> (<font color="Maroon" title="c3">a</font>,<font color="Maroon" title="c4">b</font>,<font color="Maroon" title="c2">p</font>)</span>)</span></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default">1 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">p</font></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1">(<span class="default"><a href="gr_cy_1.html#K1" title="GR_CY_1:func.1">Sum</a> <font color="Maroon" title="c6">F</font></span>)</span> )
