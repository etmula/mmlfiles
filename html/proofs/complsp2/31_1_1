<span class="p1">(<span class="default"><a href="xcmplx_0.html#K4" title="XCMPLX_0:func.4">-</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default">1 <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> 2</span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <a href="complex1.html#K7" title="COMPLEX1:func.7">&lt;i&gt;</a></span>)</span></span>)</span> <a href="seq_4.html#K10" title="SEQ_4:func.10">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">z</font> <a href="seq_4.html#K8" title="SEQ_4:func.8">-</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">z</font> <a href="complsp2.html#K1" title="COMPLSP2:func.1">*'</a></span>)</span></span>)</span> is    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> 
