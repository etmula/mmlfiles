<span class="kw">let </span><font color="Maroon" title="c1">t</font> be   <a href="grzlog_1.html#NM2" title="GRZLOG_1:NM.2">GRZ-formula</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="grzlog_1.html#K34" title="GRZLOG_1:func.34">LD-EqClassOf</a> <font color="Maroon" title="c1">t</font> is  <a href="grzlog_1.html#V11" title="GRZLOG_1:attr.11">LD-provable</a>  implies <font color="Maroon" title="c1">t</font> is  <a href="grzlog_1.html#V10" title="GRZLOG_1:attr.10">LD-provable</a>  )</span><br/>

<span class="kw">set </span><font color="Maroon" title="c2">x</font> =  <a href="grzlog_1.html#K34" title="GRZLOG_1:func.34">LD-EqClassOf</a> <font color="Maroon" title="c1">t</font>;<br/>
<span class="kw">assume </span><a NAME="E1:89"/>
 <a href="grzlog_1.html#K34" title="GRZLOG_1:func.34">LD-EqClassOf</a> <font color="Maroon" title="c1">t</font> is  <a href="grzlog_1.html#V11" title="GRZLOG_1:attr.11">LD-provable</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">t</font> is  <a href="grzlog_1.html#V10" title="GRZLOG_1:attr.10">LD-provable</a> </span><br/>

<span class="kw">then </span><span class="kw">consider </span><font color="Maroon" title="c3">u</font> being   <a href="grzlog_1.html#NM2" title="GRZLOG_1:NM.2">GRZ-formula</a><span class="kw"> such that </span><br/><a NAME="E3:89"/><span class="lab"><font color="Green" title="E45">A1</font></span>: 
 <a href="grzlog_1.html#K34" title="GRZLOG_1:func.34">LD-EqClassOf</a> <font color="Maroon" title="c3">u</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="grzlog_1.html#K34" title="GRZLOG_1:func.34">LD-EqClassOf</a> <font color="Maroon" title="c1">t</font>
 <span class="kw">and </span><br/><a NAME="E4:89"/><span class="lab"><font color="Green" title="E46">A2</font></span>: 
<font color="Maroon" title="c3">u</font> is  <a href="grzlog_1.html#V10" title="GRZLOG_1:attr.10">LD-provable</a> 
 ;<br/>
<a NAME="E5:89"/>
<font color="Maroon" title="c3">u</font> <a href="grzlog_1.html#R4" title="GRZLOG_1:pred.4">LD-=</a> <font color="Maroon" title="c1">t</font>
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:89"><span class="lab"><font color="Green" title="E45">A1</font></span></a>, <a class="ref" href="grzlog_1.html#T32" target="_self" onmouseover="rs('grzlog_1/T32')" onmouseout="rh()">Th80</a></span>;<br/>
<a NAME="E6:89"/><span class="kw">then </span>
<font color="Maroon" title="c3">u</font> <a href="grzlog_1.html#K17" title="GRZLOG_1:func.17">'='</a> <font color="Maroon" title="c1">t</font> is  <a href="grzlog_1.html#V10" title="GRZLOG_1:attr.10">LD-provable</a> 
 
;<br/>
