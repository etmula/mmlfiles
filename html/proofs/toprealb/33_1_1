<span class="kw">let </span><font color="Maroon" title="c9">S</font> be    <a href="pre_topc.html#M1" title="PRE_TOPC:mode.1">SubSpace</a> of  <a href="euclid.html#K15" title="EUCLID:func.15">TOP-REAL</a> 2; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">S</font> is  <a href="toprealb.html#V1" title="TOPREALB:attr.1">being_simple_closed_curve</a>  implies (  not <font color="Maroon" title="c9">S</font> is  <a href="struct_0.html#V2" title="STRUCT_0:attr.2">empty</a>  &amp; <font color="Maroon" title="c9">S</font> is  <a href="borsuk_2.html#V1" title="BORSUK_2:attr.1">pathwise_connected</a>  &amp; <font color="Maroon" title="c9">S</font> is  <a href="compts_1.html#V1" title="COMPTS_1:attr.1">compact</a>  ) )</span><br/>

<span class="kw">assume </span><a NAME="E1:33_1_1"/><span class="lab"><font color="Green" title="E20">A1</font></span>: 
 the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c9">S</font> is   <a href="topreal2.html#NM1" title="TOPREAL2:NM.1">Simple_closed_curve</a>
 ; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="toprealb.html#D5" target="_self" onmouseover="rs('toprealb/D5')" onmouseout="rh()">TOPREALB:def 5</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  not <font color="Maroon" title="c9">S</font> is  <a href="struct_0.html#V2" title="STRUCT_0:attr.2">empty</a>  &amp; <font color="Maroon" title="c9">S</font> is  <a href="borsuk_2.html#V1" title="BORSUK_2:attr.1">pathwise_connected</a>  &amp; <font color="Maroon" title="c9">S</font> is  <a href="compts_1.html#V1" title="COMPTS_1:attr.1">compact</a>  )</span><br/>

<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c10">A</font> =  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c9">S</font> as   <a href="topreal2.html#NM1" title="TOPREAL2:NM.1">Simple_closed_curve</a> ;<br/>
<a NAME="E3:33_1_1"/>
 not <font color="Maroon" title="c10">A</font> is  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">empty</a> 
 
;<br/>
<span class="kw">hence </span><a NAME="E4:33_1_1"/>
 not  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c9">S</font> is  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">empty</a> 
 ; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="struct_0.html#D1" onmouseover="rs('struct_0/D1')" onmouseout="rh()">STRUCT_0:def 1</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">S</font> is  <a href="borsuk_2.html#V1" title="BORSUK_2:attr.1">pathwise_connected</a>  &amp; <font color="Maroon" title="c9">S</font> is  <a href="compts_1.html#V1" title="COMPTS_1:attr.1">compact</a>  )</span><br/>

<span class="kw">thus </span><a NAME="E5:33_1_1"/>
<font color="Maroon" title="c9">S</font> is  <a href="borsuk_2.html#V1" title="BORSUK_2:attr.1">pathwise_connected</a> 
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:33_1_1"><span class="lab"><font color="Green" title="E20">A1</font></span></a>, <a class="ref" href="topalg_3.html#T10" onmouseover="rs('topalg_3/T10')" onmouseout="rh()">TOPALG_3:10</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c9">S</font> is  <a href="compts_1.html#V1" title="COMPTS_1:attr.1">compact</a> </span><br/>

