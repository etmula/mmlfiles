<span class="kw">hence </span><a NAME="E4:39_1_1"/>
( <font color="Maroon" title="c3">z</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <a href="zfmisc_1.html#K1" title="ZFMISC_1:func.1">K16</a>(<a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">K17</a>(<a href="ordinal1.html#K4" title="ORDINAL1:func.4">omega</a>,<span class="p1">(<span class="default"><a href="ordinal1.html#K6" title="ORDINAL1:func.6">Segm</a> <font color="Maroon" title="c1">t</font></span>)</span>)) &amp; <font color="Maroon" title="c3">z</font> is    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="ordinal1.html#K6" title="ORDINAL1:func.6">Segm</a> <font color="Maroon" title="c1">t</font> &amp;  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c3">z</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">t</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 &amp; (  for <font color="Olive" title="b1">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c3">z</font> holds <br/><font color="Maroon" title="c3">z</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="int_1.html#K1" title="INT_1:func.1">[\</a><span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="int_1.html#K3" title="INT_1:func.3">frac</a> <span class="p4">(<span class="default"><span class="p5">(<span class="default"><font color="Olive" title="b1">i</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> 1</span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c2">a</font></span>)</span></span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c1">t</font></span>)</span></span><a href="int_1.html#K1" title="INT_1:func.1">/]</a></span> ) )
