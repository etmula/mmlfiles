<span class="kw">let </span><font color="Maroon" title="c4">i</font>, <font color="Maroon" title="c5">j</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c4">i</font>,<font color="Maroon" title="c5">j</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="matrix_0.html#K2" title="MATRIX_0:func.2">Indices</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="matrixc1.html#K13" title="MATRIXC1:func.13">QuadraticForm</a> (<font color="Maroon" title="c1">x</font>,<font color="Maroon" title="c3">M</font>,<font color="Maroon" title="c2">y</font>)</span>)</span> <a href="matrixc1.html#K1" title="MATRIXC1:func.1">*'</a></span>)</span> implies <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="matrixc1.html#K13" title="MATRIXC1:func.13">QuadraticForm</a> (<font color="Maroon" title="c1">x</font>,<font color="Maroon" title="c3">M</font>,<font color="Maroon" title="c2">y</font>)</span>)</span> <a href="matrixc1.html#K1" title="MATRIXC1:func.1">*'</a></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c4">i</font>,<font color="Maroon" title="c5">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="matrixc1.html#K13" title="MATRIXC1:func.13">QuadraticForm</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c1">x</font> <a href="complsp2.html#K1" title="COMPLSP2:func.1">*'</a></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon