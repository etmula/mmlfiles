<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="18_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c1">k</font>, <font color="Maroon" title="c2">q</font>, <font color="Maroon" title="c3">d</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">q</font> is  <a href="int_2.html#V1" title="INT_2:attr.1">prime</a>  &amp; <font color="Maroon" title="c3">d</font> <a href="nat_d.html#R1" title="NAT_D:pred.1">divides</a> <font color="Maroon" title="c1">k</font> <a href="nat_1.html#K2" title="NAT_1:func.2">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">q</font> <a href="newton.html#K11" title="NEWTON:func.11">|^</a> <span class="p2">(<span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span> &amp;  not <font color="Maroon" title="c3">d</font> <a href="nat_d.html#R1" title="NAT_D:pred.1">divides</a> <font color="Maroon" title="c1">k</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">q</font> <a href="newton.html#K1" title="NEWTON:func.1">|^</a> <a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span>)</span> implies <font color="Maroon" title="c2">q</font> <a href="newton.html#K11" title="NEWTON:func.11">|^</a> <span class="p1">(<span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="nat_d.html#R1" title="NAT_D:pred.1">divides</a> <font color="Maroon" title="c3">d</font> )</span><br/>

<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:18_2"/><span class="lab"><font color="Green" title="E20">A13</font></span>: 
<font color="Maroon" title="c2">q</font> is  <a href="int_2.html#V1" title="INT_2:attr.1">prime</a> 
 <span class="kw">and </span><br/><a NAME="E2:18_2"/><span class="lab"><font color="Green" title="E21">A14</font></span>: 
<font color="Maroon" title="c3">d</font> <a href="nat_d.html#R1" title="NAT_D:pred.1">divides</a> <font color="Maroon" title="c1">k</font> <a href="nat_1.html#K2" title="NAT_1:func.2">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">q</font> <a href="newton.html#K11" title="NEWTON:func.11">|^</a> <span class="p2">(<span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span>
