<span class="kw">let </span><font color="Maroon" title="c10">X</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="group_10.html#K3" title="GROUP_10:func.3">the_subsets_of_card</a> (<font color="Maroon" title="c2">n</font>,<font color="Maroon" title="c1">E</font>); <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c9">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c10">X</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">LO</font> <a href="group_10.html#K1" title="GROUP_10:func.1">^</a> <font color="Maroon" title="c4">s</font></span>)</span> <a href="relset_1.html#K7" title="RELSET_1:func.7">.:</a> <font color="Maroon" title="c10">X</font></span><br/>

