<span class="kw">assume </span><a NAME="E1:60_2"/>
 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c4">G</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">F</font> <a href="finseq_1.html#K8" title="FINSEQ_1:func.8">^</a> <font color="Maroon" title="c4">G</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c2">F</font></span>)</span> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <span class="p1">(<span class="default"><a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c4">G</font></span>)</span></span><br/>

