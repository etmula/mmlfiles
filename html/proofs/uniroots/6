<span class="p1">(<span class="default"><a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c5">g</font></span>)</span> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <span class="p1">(<span class="default"><a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <span class="p2"><a href="pre_poly.html#K3" title="PRE_POLY:func.3">&lt;*</a><span class="default"><font color="Maroon" title="c6">c</font></span><a href="pre_poly.html#K3" title="PRE_POLY:func.3">*&gt;</a></span></span>)</span>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E5:6_1"><span class="lab"><font color="Green" title="E12">A6</font></span></a>, <a class="ref" href="rlvect_1.html#T41" onmouseover="rs('rlvect_1/T41')" onmouseout="rh()">RLVECT_1:41</a></span>
<br/>.= 
<font color="Maroon" title="c9">Sgi</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c10">ci</font>
<span class="kw">by</span> <span class="lab"><a class="ref" href="rlvect_1.html#T44" onmouseover="rs('rlvect_1/T44')" onmouseout="rh()">RLVECT_1:44</a></span>
;<br/>
<span class="kw">hence </span><a NAME="E14:6_1"/>
 <a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c4">f</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<a NAME="E3:6"/><span class="lab"><font color="Green" title="E9">A11</font></span>: 
 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c2">f</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> 
 
;<br/>
<a NAME="E4:6"/><span class="lab"><font color="Green" title="E10">A12</font></span>: 
<font color="Maroon">S<sub>1</sub></font>[ <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> ]
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="6_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c3">f</font> be   <a href="struct_0.html#NM7" title="STRUCT_0:NM.7">FinSequence</a> of <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c3">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; (  for <font color="Olive" title="b1">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c3">f</font> holds <br/><font color="Maroon" title="c3">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  ) implies  <a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c3">f</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:6_2"/>
 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c3">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  ex <font color="Olive" title="b1">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c3">f</font> &amp;  not <font color="Maroon" title="c3">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  ) or  <a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c3">f</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  )</span><br/>

<a NAME="E2:6_2"/><span class="kw">then </span>
<font color="Maroon" title="c3">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="pre_poly.html#K2" title="PRE_POLY:func.2">&lt;*&gt;</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>
 
;<br/>
<span class="kw">hence </span><a NAME="E3:6_2"/>
(  ex <font color="Olive" title="b1">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c3">f</font> &amp;  not <font color="Maroon" title="c3">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  ) or  <a href="rlvect_1.html#K4" title="RLVECT_1:func.4">Sum</a> <font color="Maroon" title="c3">f</font> is  <a href="int_1.html#V1" title="INT_1:attr.1">integer</a>  )
 <span class="kw">by</span> <span class="lab"><a class="ref" href="complfld.html#T7" onmouseover="rs('complfld/T7')" onmouseout="rh()">COMPLFLD:7</a>, <a class="ref" href="rlvect_1.html#T43" onmouseover="rs('rlvect_1/T43')" onmouseout="rh()">RLVECT_1:43</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<a NAME="E5:6"/>
 for <font color="Olive" title="b1">n</font> being   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a> holds  <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">n</font>]
 
