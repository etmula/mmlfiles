 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:65"><span class="lab"><font color="Green" title="E56">A0</font></span></a>, <a class="txt" href="#E2:65"><span class="lab"><font color="Green" title="E57">A1</font></span></a>, <a class="ref" href="matrix11.html#T45" target="_self" onmouseover="rs('matrix11/T45')" onmouseout="rh()">Th45</a></span>;<br/><span class="kw">hence </span><a NAME="E7:65_1_2"/>
 <a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">M</font> <a href="matrix11.html#K4" title="MATRIX11:func.4">*</a> <font color="Maroon" title="c5">Perm</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix_1.html#K11" title="MATRIX_1:func.11">-</a> (<span class="p1">(<span class="default"><a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <font color="Maroon" title="c3">M</font></span>)</span>,<font color="Maroon" title="c4">perm</font>)
