 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( ( <font color="Maroon" title="c7">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">l</font> &amp;  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c6">pD</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix_0.html#K1" title="MATRIX_0:func.1">width</a> <font color="Maroon" title="c5">M</font> implies  <a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<span class="p1">(<span class="default"><a href="matrix11.html#NK5" title="MATRIX11:NK.5">RLine</a> (<font color="Maroon" title="c5">M</font>,<font color="Maroon" title="c4">l</font>,<font color="Maroon" title="c6">pD</font>)</span>)</span>,<font color="Maroon" title="c7">i</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">pD</font> ) &amp; ( <font color="Maroon" title="c7">i</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> <font color="Maroon" title="c4">l</font> implies  <a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<span class="p1">(<span class="default"><a href="matrix11.html#NK5" title="MATRIX11:NK.5">RLine</a> (<font color="Maroon" title="c5">M</font>,<font color="Maroon" title="c4">l</font>,<font color="Maroon" title="c6">pD</font>)</span>)</span>,<font color="Maroon" title="c7">i</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<font color="Maroon" title="c5">M</font>,<font color="Maroon" title="c7">i</font>) ) )</span><br/>

<span class="kw">set </span><font color="Maroon" title="c8">R</font> =  <a href="matrix11.html#NK5" title="MATRIX11:NK.5">RLine</a> (<font color="Maroon" title="c5">M</font>,<font color="Maroon" title="c4">l</font>,<font color="Maroon" title="c6">pD</font>);<br/>
<span class="kw">set </span><font color="Maroon" title="c9">LR</font> =  <a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<span class="p1">(<span class="default"><a href="matrix11.html#NK5" title="MATRIX11:NK.5">RLine</a> (<font color="Maroon" title="c5">M</font>,<font color="Maroon" title="c4">l</font>,<font color="Maroon" title="c6">pD</font>)</span>)</span>,<font color="Maroon" title="c7">i</font>);<br/>
