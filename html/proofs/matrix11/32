<span class="kw">assume </span><a NAME="E1:32_2"/><span class="lab"><font color="Green" title="E31">A3</font></span>: 
 <a href="matrix11.html#K2" title="MATRIX11:func.2">sgn</a> (<font color="Maroon" title="c3">perm2</font>,<font color="Maroon" title="c2">K</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="group_1.html#K1" title="GROUP_1:func.1">1_</a> <font color="Maroon" title="c2">K</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c3">perm2</font> is  <a href="matrix_1.html#V6" title="MATRIX_1:attr.6">even</a> </span><br/>

<span class="kw">consider </span><font color="Maroon" title="c5">P</font> being   <a href="struct_0.html#NM7" title="STRUCT_0:NM.7">FinSequence</a> of <span class="p1">(<span class="default"><a href="matrix_1.html#K10" title="MATRIX_1:func.10">Group_of_Perm</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 2</span>)</span></span>)</span><span class="kw"> such that </span><br/><a NAME="E3:32_2"/><span class="lab"><font color="Green" title="E32">A4</font></span>: 
<font color="Maroon" title="c3">perm2</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="group_4.html#K3" title="GROUP_4:func.3">Product</a> <font color="Maroon" title="c5">P</font>
 <span class="kw">and </span><br/><a NAME="E4:32_2"/><span class="lab"><font color="Green" title="E33">A5</font></span>: 
 for <font color="Olive" title="b1">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c5">P</font> holds <br/> ex <font color="Olive" title="b2">trans</font> being    <a href="matrix_1.html#M1" title="MATRIX_1:mode.1">Element</a> of  <a href="matrix_1.html#K9" title="MATRIX_1:func.9">Permutations</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 2</span>)</span> st <br/>( <font color="Maroon" title="c5">P</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">trans</font> &amp; <font color="Olive" title="b2">trans</font> is  <a href="matrix_1.html#V5" title="MATRIX_1:attr.5">being_transposition</a>  )
 <span class="kw">by</span> <span class="lab"><a class="ref" href="matrix11.html#T21" target="_self" onmouseover="rs('matrix11/T21')" onmouseout="rh()">Th21</a></span>;<br/>
<span class="kw">assume </span><a NAME="E5:32_2"/>
