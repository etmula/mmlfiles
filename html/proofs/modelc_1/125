( <font color="Maroon" title="c3">s</font> <a href="modelc_1.html#R3" title="MODELC_1:pred.3">|=</a> <font color="Maroon" title="c5">f</font> <a href="modelc_1.html#K27" title="MODELC_1:func.27">EU</a> <font color="Maroon" title="c6">g</font> iff  ex <font color="Olive" title="b1">pai</font> being    <a href="modelc_1.html#M1" title="MODELC_1:mode.1">inf_path</a> of <font color="Maroon" title="c2">R</font> st <br/>( <font color="Olive" title="b1">pai</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">s</font> &amp;  ex <font color="Olive" title="b2">m</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( (  for <font color="Olive" title="b3">j</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b3">j</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Olive" title="b2">m</font> holds <br/><font color="Olive" title="b1">pai</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b3">j</font> <a href="modelc_1.html#R3" title="MODELC_1:pred.3">|=</a> <font color="Maroon" title="c5">f</font> ) &amp; <font color="Olive" title="b1">pai</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">m</font> <a href="modelc_1.html#R3" title="MODELC_1:pred.3">|=</a> <font color="Maroon" title="c6">g</font> ) ) )
