<span class="kw">assume </span><a NAME="E6:91"/>
 ex <font color="Olive" title="b1">V</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><font color="Maroon" title="c2">J</font> <a href="tops_5.html#K1" title="TOPS_5:func.1">.</a> <font color="Maroon" title="c3">i</font></span>)</span> st <br/>( <font color="Olive" title="b1">V</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">open</a>  &amp; <font color="Maroon" title="c4">P</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="card_3.html#K4" title="CARD_3:func.4">product</a> <span class="p1">(<span class="default"><span class="p2"><a href="domain_1.html#K6" title="DOMAIN_1:func.6">{</a><span class="default"><font color="Maroon" title="c3">i</font></span><a href="domain_1.html#K6" title="DOMAIN_1:func.6">}</a></span> <a href="funcop_1.html#K8" title="FUNCOP_1:func.8">--&gt;</a> <font color="Olive" title="b1">V</font></span>)</span> )
