<a NAME="E8:17_1_1"/>
 <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">V</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c3">X</font>
 
;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c4">X</font> = <font color="Maroon" title="c3">X</font> as   non  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">empty</a>  <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">V</font> ;<br/>
<span class="kw">consider </span><font color="Maroon" title="c5">W</font> being   <a href="vectsp_1.html#V7" title="VECTSP_1:attr.7">strict</a>   <a href="vectsp_4.html#M1" title="VECTSP_4:mode.1">Subspace</a> of <font color="Maroon" title="c1">V</font><span class="kw"> such that </span><br/><a NAME="E11:17_1_1"/><span class="lab"><font color="Green" title="E15">A7</font></span>: 
