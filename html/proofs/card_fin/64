 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  ex <font color="Olive" title="b1">y</font> being    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a>  st <br/>( <font color="Olive" title="b1">y</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="numbers.html#K4" title="NUMBERS:func.4">INT</a>  &amp; <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c4">x</font>,<font color="Olive" title="b1">y</font>] )</span><br/>

