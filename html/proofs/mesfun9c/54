<span class="kw">let </span><font color="Maroon" title="c7">x</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c7">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c4">E</font> implies <span class="p1">(<span class="default"><a href="mesfun7c.html#K1" title="MESFUN7C:func.1">R_EAL</a> <font color="Maroon" title="c5">F</font></span>)</span> <a href="mesfunc5.html#K3" title="MESFUNC5:func.3">#</a> <font color="Maroon" title="c7">x</font> is  <a href="mesfunc5.html#V10" title="MESFUNC5:attr.10">convergent</a>  )</span><br/>

