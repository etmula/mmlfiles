<span class="kw">hence </span><a NAME="E3:48"/>
 for <font color="Olive" title="b1">A</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="euclid.html#K1" title="EUCLID:func.1">REAL</a> <font color="Maroon" title="c2">d</font></span>)</span> holds <br/> ( <font color="Olive" title="b1">A</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="chain_1.html#K4" title="CHAIN_1:func.4">cells</a> (<font color="Maroon" title="c1">k</font>,<font color="Maroon" title="c3">G</font>) iff  ex <font color="Olive" title="b2">l</font>, <font color="Olive" title="b3">r</font> being    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of  <a href="euclid.html#K1" title="EUCLID:func.1">REAL</a> <font color="Maroon" title="c2">d</font> st <br/>( <font color="Olive" title="b1">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="chain_1.html#K3" title="CHAIN_1:func.3">cell</a> (<font color="Olive" title="b2">l</font>,<font color="Olive" title="b3">r</font>) &amp; (  ex <font color="Olive" title="b4">X</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c2">d</font></span>)</span> st <br/>(  <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <font color="Olive" title="b4">X</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">k</font> &amp; (  for <font color="Olive" title="b5">i</font> being    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c2">d</font>  holds <br/>( ( <font color="Olive" title="b5">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Olive" title="b4">X</font> &amp; <font color="Olive" title="b2">l</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="