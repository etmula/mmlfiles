( <span class="p1">(<span class="default"><a href="anproj_8.html#K4" title="ANPROJ_8:func.4">homography</a> <font color="Maroon" title="c8">N</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c1">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">q</font> &amp; <span class="p1">(<span class="default"><a href="anproj_8.html#K4" title="ANPROJ_8:func.4">homography</a> <font color="Maroon" title="c8">N</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c2">q</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">p</font> &amp; <span class="p1">(<span class="default"><a href="anproj_8.html#K4" title="ANPROJ_8:func.4">homography</a> <font color="Maroon" title="c8">N</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c3">r</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">s</font> )
