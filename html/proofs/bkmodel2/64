<span class="kw">let </span><font color="Maroon" title="c22">v</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="bkmodel1.html#K4" title="BKMODEL1:func.4">real_projective_plane</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c22">v</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1">(<span class="default"><a href="bkmodel2.html#K5" title="BKMODEL2:func.5">tangent</a> <font color="Maroon" title="c1">p</font></span>)</span> <a href="subset_1.html#K9" title="SUBSET_1:func.9">/\</a> <span class="p1">(<span class="default"><a href="bkmodel2.html#K5" title="BKMODEL2:func.5">tangent</a> <font color="Maroon" title="c2">q</font></span>)</span> implies <span class="p1">(<span class="default"><a href="anproj_8.html#K4" title="ANPROJ_8:func.4">homography</a> <font color="Maroon" title="c8">N</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c22">v</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c22">v</font> )</span><br/>

