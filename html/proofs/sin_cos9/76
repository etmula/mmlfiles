<span class="kw">by</span> <span class="lab"><a class="txt" href="#E6:76"><span class="lab"><font color="Green" title="E69">A8</font></span></a>, <a class="txt" href="#E4:76"><span class="lab"><font color="Green" title="E67">A4</font></span></a>, <a class="txt" href="#E5:76"><span class="lab"><font color="Green" title="E68">A5</font></span></a>, <a class="txt" href="#E12:76"><span class="lab"><font color="Green" title="E73">A12</font></span></a>, <a class="txt" href="#E11:76"><span class="lab"><font color="Green" title="E72">A11</font></span></a>, <a class="ref" href="sin_cos9.html#T23" target="_self" onmouseover="rs('sin_cos9/T23')" onmouseout="rh()">Th23</a>, <a class="ref" href="fdiff_2.html#T48" onmouseover="rs('fdiff_2/T48')" onmouseout="rh()">FDIFF_2:48</a></span>
<br/>.= 
1 <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">r</font> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1</span>)</span>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E16:76"><span class="lab"><font color="Green" title="E75">A14</font></span></a>, <a class="ref" href="xcmplx_1.html#T73" onmouseover="rs('xcmplx_1/T73')" onmouseout="rh()">XCMPLX_1:73</a></span>
;<br/>
<span class="kw">hence </span><a NAME="E20:76"/>
 <a href="fdiff_1.html#K1" title="FDIFF_1:func.1">diff</a> (<a href="sin_cos9.html#K1" title="SIN_COS9:func.1">arctan</a>,<font color="Maroon" title="c1">r</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p1">(<span class="default">1 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p2">(<span class="default"><font color="Maroon" title="c1">r</font> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span></span>)</span>
