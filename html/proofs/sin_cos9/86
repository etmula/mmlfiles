( <a href="sin_cos9.html#K1" title="SIN_COS9:func.1">arctan</a> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <font color="Maroon" title="c4">f</font> <a href="fdiff_1.html#R2" title="FDIFF_1:pred.2">is_differentiable_on</a> <font color="Maroon" title="c3">Z</font> &amp; (  for <font color="Olive" title="b1">x</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st <font color="Olive" title="b1">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c3">Z</font> holds <br/><span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="sin_cos9.html#K1" title="SIN_COS9:func.1">arctan</a> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <font color="Maroon" title="c4">f</font></span>)</span> <a href="fdiff_1.html#K2" title="FDIFF_1:func.2">`|</a> <font color="Maroon" title="c3">Z</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">r</font> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p1">(<span class="default">1 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><font color="Maroon" title="c1">r</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Olive" title="b1">x</font></span>)</span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">s</font></span>)</span> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span></span>)</span> ) )
