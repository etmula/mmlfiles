( <font color="Maroon" title="c4">f1</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Maroon" title="c5">f2</font> <a href="fdiff_1.html#R2" title="FDIFF_1:pred.2">is_differentiable_on</a> <font color="Maroon" title="c2">Z</font> &amp; (  for <font color="Olive" title="b1">x</font> being   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>  st <font color="Olive" title="b1">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c2">Z</font> holds <br/><span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c4">f1</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Maroon" title="c5">f2</font></span>)</span> <a href="fdiff_1.html#K2" title="FDIFF_1:func.2">`|</a> <font color="Maroon" title="c2">Z</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Olive" title="b1">x</font></span>)</span> <a href="xcmplx_0.html#K7" title="XCMPLX_0:func.7">/</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">r</font> <a href="square_1.html#K1" title="SQUARE_1:func.1">^2</a></span>)</span> ) )
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:106"><span class="lab"><font color="Green" title="E80">A1</font></span></a>, <a class="txt" href="#E8:106"><span class="lab"><font color="Green" title="E87">A8</font></span></a>, <a class="txt" href="#E12:106"><span class="lab"><font color="Green" title="E90">A12</font></span></a>, <a class="ref" href="fdiff_1.html#T18" onmouseover="rs('fdiff_1/T18')" onmouseout="rh()">FDIFF_1:18</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


