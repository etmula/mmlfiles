<span class="kw">let </span><font color="Maroon" title="c6">y</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <span class="p1">(<span class="default"><a href="fuznum_1.html#K4" title="FUZNUM_1:func.4">TriangularFS</a> (<font color="Maroon" title="c1">a</font>,<font color="Maroon" title="c2">b</font>,<font color="Maroon" title="c3">c</font>)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 implies <font color="Maroon" title="c6">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">bb</font> )</span><br/>

