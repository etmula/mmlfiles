<span class="kw">let </span><font color="Maroon" title="c10">k</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c10">k</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c7">s</font> implies  ex <font color="Olive" title="b1">b1</font>, <font color="Olive" title="b2">b2</font> being   <a href="pre_poly.html#NM2" title="PRE_POLY:NM.2">bag</a> of <font color="Maroon" title="c1">n</font> st <br/>( <span class="p1">(<span class="default"><a href="pre_poly.html#K21" title="PRE_POLY:func.21">decomp</a> <font color="Maroon" title="c6">b</font></span>)</span> <a href="pre_poly.html#K9" title="PRE_POLY:func.9">/.</a> <font color="Maroon" title="c10">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">&lt;*</a><span class="default"><font color="Olive" title="b1">b1</font>,<font color="Olive" title="b2">b2</font></span><a href="finseq_1.html#K10" title="FINSEQ_1:func.10">*&gt;</a></span> &amp; <font color="Maroon" title="c7">s</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Maroon" title="c10">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">p</font> <a href="polynom1.html#K4" title="POLYNOM1:func.4">.</a> <font color="Olive" title="b1">b1</font></span>)</span> <a href="algstr_0.html#K6" title="ALGSTR_0:func.6">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">q</font> <a href="polynom1.html#K4" title="POLYNOM1:func.4">.</a> <font c