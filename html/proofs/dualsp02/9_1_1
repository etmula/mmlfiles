<a NAME="E7:9_1_1"/><span class="kw">then </span>
<font color="Maroon" title="c3">f</font> is  <a href="dualsp01.html#V1" title="DUALSP01:attr.1">Lipschitzian</a> 
 
;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c4">f</font> = <font color="Maroon" title="c3">f</font> as   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="dualsp01.html#K17" title="DUALSP01:func.17">DualSp</a> <span class="p2">(<span class="default"><a href="dualsp01.html#K17" title="DUALSP01:func.17">DualSp</a> <font color="Maroon" title="c1">X</font></span>)</span></span>)</span> <span class="kw">by</span> <span class="lab"><a class="txt" href="#E4:9_1_1"><span class="lab"><font color="Green" title="E11">P2</font></span></a>, <a class="txt" href="#E5:9_1_1"><span class="lab"><font color="Green" title="E12">P3</font></span></a>, <a class="ref" href="dualsp01.html#D10" onmouseover="rs('dualsp01/D10')" onmouseout="rh()">DUALSP01:def 10</a></span>;<br/>
<span class="kw">take </span>
<font color="Maroon" title="c4">f</font>
; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">f</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="dualsp01.html#K17" title="DUALSP01:func.17">DualSp</a> <font color="Maroon" title="c1">X</font></span>)</span> holds  <font color="Maroon" title="c4">f</font> <a href="dualsp01.html#K18" title="DUALSP01:func.18">.</a> <font color="Olive" title="b1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">f</font> <a href="dualsp01.html#K18" title="DUALSP01:func.18">.</a> <font color="Maroon" title="c2">x</font></span><br/>

<span class="kw">thus </span><a NAME="E9:9_1_1"/>
 for <font color="Olive" title="b1">f</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1">(<span class="default"><a href="dualsp01.html#K17" title="DUALSP01:func.17">DualSp</a> <font color="Maroon" title="c1">X</font></span>)</span> holds  <font color="Maroon" title="c4">f</font> <a href="dualsp01.html#K18" title="DUALSP01:func.18">.</a> <font color="Olive" title="b1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">f</font> <a href="dualsp01.html#K18" title="DUALSP01:func.18">.</a> <font color="Maroon" title="c2">x</font>
