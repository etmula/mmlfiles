<span class="kw">hence </span><a NAME="E3:77"/>
( <font color="Maroon" title="c4">f</font> is   <a href="mod_4.html#NM6" title="MOD_4:NM.6">Homomorphism</a> of <font color="Maroon" title="c2">V</font>,<font color="Maroon" title="c3">W</font> iff ( (  for <font color="Olive" title="b1">x</font>, <font color="Olive" title="b2">y</font> being   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <font color="Maroon" title="c2">V</font> holds  <font color="Maroon" title="c4">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">x</font> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <font color="Olive" title="b2">y</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">x</font></span>)</span> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">y</font></span>)</span> ) &amp; (  for <font color="Olive" title="b1">a</font> being   <a href="vectsp_1.html#NM6" title="VECTSP_1:NM.6">Scalar</a> of <font color="Maroon" title="c1">K</font><br/>  for <font color="Olive" title="b2">x</font> being   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <font color="Maroon" title="c2">V</font> holds  <font color="Maroon" title="c4">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">a</font> <a href="vectsp_1.html#K3" title="VECTSP_1:func.3">*</a> <font color="Olive" title="b2">x</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">a</font> <a href="vectsp_1.html#K3" title="VECTSP_1:func.3">*</a> <span class="p1">(<span class="defau