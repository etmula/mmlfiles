<span class="kw">thus </span><a NAME="E5:82"/>
( <font color="Maroon" title="c6">E</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c4">f</font></span>)</span> <a href="subset_1.html#K9" title="SUBSET_1:func.9">/\</a> <span class="p1">(<span class="default"><a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c5">g</font></span>)</span> &amp;  <a href="mesfunc6.html#K1" title="MESFUNC6:func.1">Integral</a> (<font color="Maroon" title="c3">M</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="valued_1.html#K3" title="VALUED_1:func.3">+</a> <font color="Maroon" title="c5">g</font></span>)</span>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="mesfunc6.html#K1" title="MESFUNC6:func.1">Integral</a> (<font color="Maroon" title="c3">M</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c4">f</font> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <font color="Maroon" title="c6">E</font></span>)</span>)</span>)</span> <a href="supinf_2.html#K3" title="SUPINF_2:func.3">+</a> <span class="p1">(<span class="default"><a href="mesfunc6.html#K1" title="MESFUNC6:func.1">Integral</a> (<font color="Maroon" title="c3">M</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c5">g</font> <a href="partfun1.html#K2" title="PARTFUN1:func.2">|</a> <font color="Maroon" title="c6">E</font></span>)</span>)</span>)</span> )
