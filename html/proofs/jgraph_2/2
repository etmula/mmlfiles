 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">q4</font> where <font color="Olive" title="b1">q4</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="euclid.html#K14" title="EUCLID:func.14">Euclid</a> <font color="Maroon" title="c1">n</font></span>)</span> :  <a href="metric_1.html#K3" title="METRIC_1:func.3">dist</a> (<font color="Maroon" title="c2">q2</font>,<font color="Olive" title="b1">q4</font>) <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Maroon" title="c4">r</font> </span> } </span> </span><br/>

