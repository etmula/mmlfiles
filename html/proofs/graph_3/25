( <font color="Maroon" title="c9">v9</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the <a href="graph_1.html#U2" title="GRAPH_1:sel.2">Target</a> of <font color="Maroon" title="c1">G</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">e</font> or <font color="Maroon" title="c9">v9</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  the <a href="graph_1.html#U1" title="GRAPH_1:sel.1">Source</a> of <font color="Maroon" title="c1">G</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">e</font> )
