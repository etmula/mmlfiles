<span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><font color="Olive" title="b1">a</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Olive" title="b2">b</font></span>)</span> where <font color="Olive" title="b1">a</font>, <font color="Olive" title="b2">b</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">R</font> : ( <font color="Olive" title="b1">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">I</font> &amp; <font color="Olive" title="b2">b</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c3">J</font> ) </span> } </span>  is   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">R</font>
