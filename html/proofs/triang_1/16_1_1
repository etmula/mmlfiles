 <span class="kw">and </span><br/><a NAME="E3:16_1_1"/>
 <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c3">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font>
 <span class="kw">and </span><br/><a NAME="E4:16_1_1"/>
 <a href="xtuple_0.html#K10" title="XTUPLE_0:func.10">rng</a> <font color="Maroon" title="c3">x</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>
 <span class="kw">by</span> <span class="lab"><a class="ref" href="funct_2.html#D2" onmouseover="rs('funct_2/D2')" onmouseout="rh()">FUNCT_2:def 2</a></span>;<br/>
<span class="kw">reconsider </span><font color="Maroon" title="c4">x</font> = <font color="Maroon" title="c3">x</font> as    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:16_1_1"><span class="lab"><font color="Green" title="E6">A1</font></span></a>, <a class="ref" href="finseq_2.html#T25" onmouseover="rs('finseq_2/T25')" onmouseout="rh()">FINSEQ_2:25</a></span>;<br/>
<a NAME="E6:16_1_1"/>
 <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <span class="p1">(<span class="default"><font color="Maroon" title="c1">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> 
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="numbers.html#T19" onmouseover="rs('numbers/T19')" onmouseout="rh()">NUMBERS:19</a></span>;<br/>
<a NAME="E7:16_1_1"/><span class="kw">then </span>
<font color="Maroon" title="c4">x</font> is    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> 
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="finseq_2.html#T24" onmouseover="rs('finseq_2/T24')" onmouseout="rh()">FINSEQ_2:24</a></span>;<br/>
