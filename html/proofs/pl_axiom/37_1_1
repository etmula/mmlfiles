 ex <font color="Olive">b<sub>1</sub></font> being   <a href="funct_2.html#NM1" title="FUNCT_2:NM.1">Function</a> of <a href="pl_axiom.html#K1" title="PL_AXIOM:func.1">PL-WFF</a>,<a href="margrel1.html#K5" title="MARGREL1:func.5">BOOLEAN</a> st <br/>( <font color="Olive">b<sub>1</sub></font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <a href="pl_axiom.html#K2" title="PL_AXIOM:func.2">FaLSUM</a> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  &amp; (  for <font color="Olive" title="b2">k</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  holds <br/> ( <font color="Olive">b<sub>1</sub></font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><a href="pl_axiom.html#K4" title="PL_AXIOM:func.4">Prop</a> <font color="Olive" title="b2">k</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 iff  <a href="pl_axiom.html#K4" title="PL_AXIOM:func.4">Prop</a> <font color="Olive" title="b2">k</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c1">M</font> ) ) &amp; (  for <font color="Olive" title="b2">p</font>, <font color="Olive" title="b3">q</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="pl_axiom.html#K1" title="PL_AXIOM:func.1">PL-WFF</a>  holds  <font color="Olive">b<sub>1</sub></font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b2">p</font> <a href="pl_axiom.html#K3" title="PL_AXIOM:func.3">=&gt;</a> <font color="Olive" title="b3">q</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Olive">b<sub>1</sub></font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">p</font></span>)</