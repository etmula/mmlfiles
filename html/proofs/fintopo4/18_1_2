<span class="kw">let </span><font color="Maroon" title="c4">x</font> be    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c4">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> implies  <a href="relat_1.html#K9" title="RELAT_1:func.9">Im</a> (<font color="Maroon" title="c2">f1</font>,<font color="Maroon" title="c4">x</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="relat_1.html#K9" title="RELAT_1:func.9">Im</a> (<font color="Maroon" title="c3">f2</font>,<font color="Maroon" title="c4">x</font>) )</span><br/>

<span class="kw">assume </span><a NAME="E1:18_1_2_1"/><span class="lab"><font color="Green" title="E10">A26</font></span>: 
