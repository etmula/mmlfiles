 for <font color="Olive" title="b1">f1</font>, <font color="Olive" title="b2">f2</font> being   <a href="relset_1.html#NM2" title="RELSET_1:NM.2">Relation</a> of <span class="p1">(<span class="default"><a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font></span>)</span>  st (  for <font color="Olive" title="b3">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b3">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> holds <br/> <a href="relat_1.html#K9" title="RELAT_1:func.9">Im</a> (<font color="Olive" title="b1">f1</font>,<font color="Olive" title="b3">i</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="enumset1.html#K1" title="ENUMSET1:func.1">{</a><span class="default"><font color="Olive" title="b3">i</font>,<span class="p2">(<span class="default"><a href="nat_1.html#K5" title="NAT_1:func.5">max</a> (<span class="p3">(<span class="default"><font color="Olive" title="b3">i</font> <a href="nat_d.html#K7" title="NAT_D:func.7">-'</a> 1</span>)</span>,1)</span>)</span>,<span class="p2">(<span class="default"><a href="xxreal_0.html#K3" title="XXREAL_0:func.3">min</a> (<span class="p3">(<span class="default"><font color="Olive" title="b3">i</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>,<font color="Maroon" title="c1">n</font>)</span>)</span></span><a href="enumset1.html#K1" title="ENUMSET1:func.1">}</a></span> ) &amp; (  for <font color="Olive" title="b3">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b3">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINS