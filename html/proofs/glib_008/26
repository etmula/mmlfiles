<span class="kw">let </span><font color="Maroon" title="c9">x</font> be    <a href="hidden.html#M1" title="HIDDEN:mode.1">object</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="glib_000.html#K25" title="GLIB_000:func.25">the_Edges_of</a> <font color="Maroon" title="c6">G4</font> implies <font color="Maroon" title="c9">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <span class="p1">(<span class="default"><a href="glib_000.html#K25" title="GLIB_000:func.25">the_Edges_of</a> <font color="Maroon" title="c5">G3</font></span>)</span> <a href="subset_1.html#K7" title="SUBSET_1:func.7">\</a> <span class="p1"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><font color="Maroon" title="c3">e</font></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span> )</span><br/>

