span> ) iff (  not <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><span class="p3"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c1">a</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p3"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c2">b</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">a</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c2">b</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> or  not <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">a</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">b</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c1">a</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c2">b</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> ) )
