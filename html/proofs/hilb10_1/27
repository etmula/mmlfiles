<span class="kw">let </span><font color="Maroon" title="c3">k</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c3">k</font>] implies <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c3">k</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> 1] )</span><br/>

