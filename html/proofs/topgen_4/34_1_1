( (  for <font color="Olive" title="b1">P</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">T</font>  st <font color="Olive" title="b1">P</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> holds <br/><font color="Olive" title="b1">P</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">open</a>  ) &amp; (  for <font color="Olive" title="b1">P</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">T</font>  st <font color="Olive" title="b1">P</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> holds <br/><font color="Olive" title="b1">P</font> is  <a href="pre_topc.html#V4" title="PRE_TOPC:attr.4">closed</a>  ) )
 
;<br/>
