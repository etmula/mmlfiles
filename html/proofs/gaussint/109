<span class="kw">let </span><font color="Maroon" title="c3">x</font>, <font color="Maroon" title="c4">y</font> be   <a href="gaussint.html#NM1" title="GAUSSINT:NM.1">G_INTEG</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c3">x</font> <a href="gaussint.html#R2" title="GAUSSINT:pred.2">is_associated_to</a> <font color="Maroon" title="c4">y</font> implies <font color="Maroon" title="c3">x</font> <a href="gaussint.html#K15" title="GAUSSINT:func.15">*'</a>  <a href="gaussint.html#R2" title="GAUSSINT:pred.2">is_associated_to</a> <font color="Maroon" title="c4">y</font> <a href="gaussint.html#K15" title="GAUSSINT:func.15">*'</a>  )</span><br/>

