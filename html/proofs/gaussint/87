 for <font color="Olive" title="b1">v</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a>  st <font color="Olive" title="b1">v</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a> holds <br/><font color="Olive" title="b1">v</font> is  <a href="algstr_0.html#V23" title="ALGSTR_0:attr.23">left_invertible</a> 
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="87_10"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c4">v</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c4">v</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a> implies <font color="Maroon" title="c4">v</font> is  <a href="algstr_0.html#V23" title="ALGSTR_0:attr.23">left_invertible</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:87_10"/><span class="lab"><font color="Green" title="E44">A15</font></span>: 
<font color="Maroon" title="c4">v</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c4">v</font> is  <a href="algstr_0.html#V23" title="ALGSTR_0:attr.23">left_invertible</a> </span><br/>

<span class="kw">reconsider </span><font color="Maroon" title="c5">v1</font> = <font color="Maroon" title="c4">v</font> as   <a href="gaussint.html#NM3" title="GAUSSINT:NM.3">G_RAT</a> <span class="kw">by</span> <span class="lab"><a class="ref" href="gaussint.html#T10" target="_self" onmouseover="rs('gaussint/T10')" onmouseout="rh()">Th10</a></span>;<br/>
<span class="kw">reconsider </span><font color="Maroon" title="c6">w</font> = <font color="Maroon" title="c5">v1</font> <a href="xcmplx_0.html#K5" title="XCMPLX_0:func.5">"</a>  as   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a> <span class="kw">by</span> <span class="lab"><a class="ref" href="gaussint.html#T11" target="_self" onmouseover="rs('gaussint/T11')" onmouseout="rh()">Th11</a></span>;<br/>
<font color="Maroon" title="c6">w</font> <a href="algstr_0.html#K6" title="ALGSTR_0:func.6">*</a> <font color="Maroon" title="c4">v</font> = 
<span class="p1">(<span class="default"><font color="Maroon" title="c5">v1</font> <a href="xcmplx_0.html#K5" title="XCMPLX_0:func.5">"</a></span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c5">v1</font>
<span class="kw">by</span> <span class="lab"><a class="ref" href="gaussint.html#T29" target="_self" onmouseover="rs('gaussint/T29')" onmouseout="rh()">Th29</a></span>
<br/>.= 
 <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="gaussint.html#K14" title="GAUSSINT:func.14">Gauss_RAT_Ring</a>
