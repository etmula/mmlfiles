<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="42_1_1_7"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c10">R</font> be    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c10">R</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><a href="vectsp_6.html#K1" title="VECTSP_6:func.1">Carrier</a> <font color="Olive" title="b1">L</font></span>)</span> where <font color="Olive" title="b1">L</font> is    <a href="vectsp_6.html#M2" title="VECTSP_6:mode.2">Linear_Combination</a> of <font color="Maroon" title="c3">B</font> :  ex <font color="Olive" title="b2">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c5">p</font> &amp;  <a href="vectsp_6.html#K4" title="VECTSP_6:func.4">Sum</a> <font color="Olive" title="b1">L</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font> ) </span> } </span>  implies <font color="Maroon" title="c10">R</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:42_1_1_7"/>
<font color="Maroon" title="c10">R</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><a href="vectsp_6.html#K1" title="VECTSP_6:func.1">Carrier</a> <font color="Olive" title="b1">L</font></span>)</span> where <font color="Olive" title="b1">L</font> is    <a href="vectsp_6.html#M2" title="VECTSP_6:mode.2">Linear_Combination</a> of <font color="Maroon" title="c3">B</font> :  ex <font color="Olive" title="b2">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c5">p</font> &amp;  <a href="vectsp_6.html#K4" title="VECTSP_6:func.4">Sum</a> <font color="Olive" title="b1">L</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font> ) </span> } </span> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c10">R</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a> </span><br/>

<a NAME="E2:42_1_1_7"/><span class="kw">then </span>
 ex <font color="Olive" title="b1">L</font> being    <a href="vectsp_6.html#M2" title="VECTSP_6:mode.2">Linear_Combination</a> of <font color="Maroon" title="c3">B</font> st <br/>( <font color="Maroon" title="c10">R</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="vectsp_6.html#K1" title="VECTSP_6:func.1">Carrier</a> <font color="Olive" title="b1">L</font> &amp;  ex <font color="Olive" title="b2">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  st <br/>( <font color="Olive" title="b2">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c5">p</font> &amp;  <a href="vectsp_6.html#K4" title="VECTSP_6:func.4">Sum</a> <font color="Olive" title="b1">L</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">p</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">i</font> ) )
 
