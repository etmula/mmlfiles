<span class="kw">let </span><font color="Maroon" title="c5">m</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c5">m</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a>  <a href="basel_2.html#K3" title="BASEL_2:func.3">sieve</a> (<span class="p1">(<span class="default"><span class="p2"><a href="polynom5.html#K5" title="POLYNOM5:func.5">&lt;%</a><span class="default"><a href="hahnban1.html#K2" title="HAHNBAN1:func.2">i_FC</a>,<span class="p3">(<span class="default"><a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a></span>)</span></span><a href="polynom5.html#K5" title="POLYNOM5:func.5">%&gt;</a></span> <a href="polynom5.html#K3" title="POLYNOM5:func.3">`^</a> <span class="p2">(<span class="default"><span class="p3">(<span class="default">2 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c3">n</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span>,2) implies <font color="Maroon" title="c3">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c5">m</font> )</span><br/>

<span class="kw">assume </span><a NAME="E1:47_2"/><span class="lab"><font color="Green" title="E30">A3</font></span>: 
