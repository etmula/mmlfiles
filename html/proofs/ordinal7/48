<span class="kw">thus </span><a NAME="E4:48"/>
( (  for <font color="Olive" title="b1">a</font> being   <a href="ordinal1.html#NM3" title="ORDINAL1:NM.3">Ordinal</a>  st <font color="Olive" title="b1">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="afinsq_1.html#K2" title="AFINSQ_1:func.2">dom</a> <font color="Maroon" title="c1">A</font> holds <br/><font color="Maroon" title="c1">A</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">a</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="ordinal2.html#K12" title="ORDINAL2:func.12">exp</a> (<a href="ordinal1.html#K4" title="ORDINAL1:func.4">omega</a>,<font color="Maroon" title="c2">b</font>) ) implies  <a href="ordinal5.html#K4" title="ORDINAL5:func.4">Sum^</a> <font color="Maroon" title="c1">A</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="ordinal2.html#K12" title="ORDINAL2:func.12">exp</a> (<a href="ordinal1.html#K4" title="ORDINAL1:func.4">omega</a>,<font color="Maroon" title="c2">b</font>) )
