<span class="kw">let </span><font color="Maroon" title="c9">i</font>, <font color="Maroon" title="c10">j</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c9">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c4">M</font> &amp; <font color="Maroon" title="c10">j</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <span class="p1">(<span class="default"><a href="matrix_0.html#K1" title="MATRIX_0:func.1">width</a> <font color="Maroon" title="c4">M</font></span>)</span> implies ( ( <font color="Maroon" title="c10">j</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">l</font> implies <font color="Maroon" title="c8">M2</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c9">i</font>,<font color="Maroon" title="c10">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">M</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c9">i</font>,<font color="Maroon" title="c6">k</font>) ) &amp; ( <font color="Maroon" title="c10">j</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">k</font> implies <font color="Maroon" title="c8">M2</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c9">i</font>,<font color="Maroon" title="c10">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">M</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" titl