 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>  is  <a href="algstr_0.html#V33" title="ALGSTR_0:attr.33">almost_left_invertible</a>  &amp;  not  <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>  is  <a href="struct_0.html#V6" title="STRUCT_0:attr.6">degenerated</a>  )</span><br/>

<span class="kw">thus </span><a NAME="E10:6_1_1"/>
 <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>  is  <a href="algstr_0.html#V33" title="ALGSTR_0:attr.33">almost_left_invertible</a> 
  <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  not  <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>  is  <a href="struct_0.html#V6" title="STRUCT_0:attr.6">degenerated</a> </span><div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="6_1_1_3"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c1">x</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="algstr_0.html#D38" onmouseover="rs('algstr_0/D38')" onmouseout="rh()">ALGSTR_0:def 38</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a> or <font color="Maroon" title="c1">x</font> is  <a href="algstr_0.html#V23" title="ALGSTR_0:attr.23">left_invertible</a>  )</span><br/>

<span class="kw">reconsider </span><font color="Maroon" title="c2">x1</font> = <font color="Maroon" title="c1">x</font> as    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#K2" title="NUMBERS:func.2">COMPLEX</a>  <span class="kw">by</span> <span class="lab"><a class="ref" href="complfld.html#D1" target="_self" onmouseover="rs('complfld/D1')" onmouseout="rh()">Def1</a></span>;<br/>
<span class="kw">assume </span><a NAME="E2:6_1_1_3"/><span class="lab"><font color="Green" title="E4">A1</font></span>: 
<font color="Maroon" title="c1">x</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">x</font> is  <a href="algstr_0.html#V23" title="ALGSTR_0:attr.23">left_invertible</a> </span><br/>

<a NAME="E3:6_1_1_3"/>
<font color="Maroon" title="c2">x1</font> <a href="xcmplx_0.html#K5" title="XCMPLX_0:func.5">"</a>  <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="numbers.html#K2" title="NUMBERS:func.2">COMPLEX</a> 
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="xcmplx_0.html#D2" onmouseover="rs('xcmplx_0/D2')" onmouseout="rh()">XCMPLX_0:def 2</a></span>;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c3">y</font> = <font color="Maroon" title="c2">x1</font> <a href="xcmplx_0.html#K5" title="XCMPLX_0:func.5">"</a>  as   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a> <span class="kw">by</span> <span class="lab"><a class="ref" href="complfld.html#D1" target="_self" onmouseover="rs('complfld/D1')" onmouseout="rh()">Def1</a></span>;<br/>
<span class="kw">take </span>
<font color="Maroon" title="c3">y</font>
; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="algstr_0.html#D27" onmouseover="rs('algstr_0/D27')" onmouseout="rh()">ALGSTR_0:def 27</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c3">y</font> <a href="algstr_0.html#K6" title="ALGSTR_0:func.6">*</a> <font color="Maroon" title="c1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a></span><br/>

<a NAME="E5:6_1_1_3"/>
<font color="Maroon" title="c2">x1</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="complex1.html#K5" title="COMPLEX1:func.5">0c</a> 
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:6_1_1_3"><span class="lab"><font color="Green" title="E4">A1</font></span></a>, <a class="ref" href="complfld.html#D1" target="_self" onmouseover="rs('complfld/D1')" onmouseout="rh()">Def1</a></span>;<br/>
<span class="kw">hence </span><a NAME="E6:6_1_1_3"/>
<font color="Maroon" title="c3">y</font> <a href="algstr_0.html#K6" title="ALGSTR_0:func.6">*</a> <font color="Maroon" title="c1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="complfld.html#K1" title="COMPLFLD:func.1">F_Complex</a>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="complfld.html#E3"><span class="lab"><font color="Green" title="E3">Lm2</font></span></a>, <a class="ref" href="xcmplx_0.html#D7" onmouseover="rs('xcmplx_0/D7')" onmouseout="rh()">XCMPLX_0:def 7</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


