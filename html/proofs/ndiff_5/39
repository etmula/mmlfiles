<font color="Maroon" title="c5">x</font> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="ndiff_5.html#K4" title="NDIFF_5:func.4">reproj</a> (<font color="Maroon" title="c4">i</font>,<font color="Maroon" title="c5">x</font>)</span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c6">r</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="ndiff_5.html#K4" title="NDIFF_5:func.4">reproj</a> (<font color="Maroon" title="c4">i</font>,<span class="p2">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <span class="p3">(<span class="default"><a href="prvect_2.html#K14" title="PRVECT_2:func.14">product</a> <font color="Maroon" title="c3">G</font></span>)</span></span>)</span>)</span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="ndiff_5.html#K3" title="NDIFF_5:func.3">proj</a> <font color="Maroon" title="c4">i</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c5">x</font></span>)</span> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <font color="Maroon" title="c6">r</font></span>)</span>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E10:39"><span class="lab"><font color="Green" title="E32">A5</font></span></a>, <a class="ref" href="funct_1.html#T2" onmouseover="rs('funct_1/T2')" onmouseout="rh()">FUNCT_1:2</a>, <a class="ref" href="card_3.html#T9" onmouseover="rs('card_3/T9')" onmouseout="rh()">CARD_3:9</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


