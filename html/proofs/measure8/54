 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">Bseq</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Maroon" title="c7">n</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c5">Bseq</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Maroon" title="c8">m</font></span><br/>

