<span class="kw">let </span><font color="Maroon" title="c7">n</font>, <font color="Maroon" title="c8">m</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c7">n</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c8">m</font> implies <font color="Maroon" title="c5">Bseq</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Maroon" title="c7">n</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c5">Bseq</font> <a href="prob_2.html#K1" title="PROB_2:func.1">.</a> <font color="Maroon" title="c8">m</font> )</span><br/>

