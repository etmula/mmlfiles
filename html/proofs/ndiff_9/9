  <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="9_6"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c8">y</font> be   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <font color="Maroon" title="c2">F</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">L2</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c8">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c7">L</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">E</font></span>)</span>,<font color="Maroon" title="c8">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span></span><br/>

<a NAME="E1:9_6"/>
<span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">E</font></span>)</span>,<font color="Maroon" title="c8">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> is    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a> 
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="tarski.html#T1" onmouseover="rs('tarski/T1')" onmouseout="rh()">TARSKI:1</a></span>;<br/>
<a NAME="E2:9_6"/><span class="kw">then </span>
<span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">E</font></span>)</span>,<font color="Maroon" title="c8">y</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> is   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <span class="p1"><a href="prvect_3.html#K7" title="PRVECT_3:func.7">[:</a><span class="default"><font color="Maroon" title="c1">E</font>,<font color="Maroon" title="c2">F</font></span><a href="prvect_3.html#K7" title="PRVECT_3:func.7">:]</a></span>
 
