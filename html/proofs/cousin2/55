<span class="kw">then </span> <a href="cousin2.html#K9" title="COUSIN2:func.9">HK-integral</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">r</font> <a href="valued_1.html#K26" title="VALUED_1:func.26">(#)</a> <font color="Maroon" title="c2">f</font></span>)</span> = 
<font color="Maroon" title="c3">r</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c4">J</font>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E4:55"><span class="lab"><font color="Green" title="E47">A2</font></span></a>, <a class="ref" href="cousin2.html#D11" target="_self" onmouseover="rs('cousin2/D11')" onmouseout="rh()">DEFCC</a></span>
<br/>.= 
<font color="Maroon" title="c3">r</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><a href="cousin2.html#K9" title="COUSIN2:func.9">HK-integral</a> <font color="Maroon" title="c2">f</font></span>)</span>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:55"><span class="lab"><font color="Green" title="E46">A1</font></span></a>, <a class="ref" href="cousin2.html#D11" target="_self" onmouseover="rs('cousin2/D11')" onmouseout="rh()">DEFCC</a></span>
;<br/>
<span class="kw">hence </span><a NAME="E7:55"/>
( <font color="Maroon" title="c3">r</font> <a href="valued_1.html#K26" title="VALUED_1:func.26">(#)</a> <font color="Maroon" title="c2">f</font> is   <a href="cousin2.html#V1" title="COUSIN2:attr.1">HK-integrable</a>  <a href="funct_2.html#NM1" title="FUNCT_2:NM.1">Function</a> of <font color="Maroon" title="c1">I</font>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> &amp;  <a href="cousin2.html#K9" title="COUSIN2:func.9">HK-integral</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">r</font> <a href="valued_1.html#K26" title="VALUED_1:func.26">(#)</a> <font color="Maroon" title="c2">f</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">r</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><a href="cousin2.html#K9" title="COUSIN2:func.9">HK-integral</a> <font color="Maroon" title="c2">f</font></span>)</span> )
