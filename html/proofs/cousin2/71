<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:71"/><span class="lab"><font color="Green" title="E55">A1</font></span>: 
<font color="Maroon" title="c8">fc</font> <a href="funct_2.html#R1" title="FUNCT_2:pred.1">=</a>  <a href="funct_3.html#K5" title="FUNCT_3:func.5">chi</a> (<font color="Maroon" title="c1">I</font>,<font color="Maroon" title="c1">I</font>)
 <span class="kw">and </span><br/><a NAME="E2:71"/><span class="lab"><font color="Green" title="E56">A2</font></span>: 
