<span class="kw">let </span><font color="Maroon" title="c7">i</font>, <font color="Maroon" title="c8">j</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Maroon" title="c7">i</font>,<font color="Maroon" title="c8">j</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="matrix_0.html#K2" title="MATRIX_0:func.2">Indices</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">M1</font> <a href="matrix_3.html#K3" title="MATRIX_3:func.3">+</a> <font color="Maroon" title="c4">M2</font></span>)</span> implies <span class="p1">(<span class="default"><font color="Maroon" title="c3">M1</font> <a href="matrix_3.html#K3" title="MATRIX_3:func.3">+</a> <font color="Maroon" title="c4">M2</font></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c7">i</font>,<font color="Maroon" title="c8">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">p</font> <a href="fvsum_1.html#K3" title="FVSUM_1:func.3">+</a> <font color="Maroon" title="c6">q</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c8">j</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c7">i</font></span>)</span> <a href="int_1.html#K5" title="INT_1:func.5">mod</a> <span class="p3">(<span class="default"><a href="finseq_1.html#K3" title="FINSEQ_