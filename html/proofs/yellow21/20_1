 for <font color="Olive" title="b1">a</font>, <font color="Olive" title="b2">b</font> being   <a href="altcat_1.html#NM1" title="ALTCAT_1:NM.1">Object</a> of <font color="Maroon" title="c4">C</font><br/>  for <font color="Olive" title="b3">f</font> being   <a href="orders_3.html#V5" title="ORDERS_3:attr.5">monotone</a>  <a href="struct_0.html#NM6" title="STRUCT_0:NM.6">Function</a> of <span class="p1">(<span class="default"><a href="yellow21.html#K3" title="YELLOW21:func.3">latt</a> <font color="Olive" title="b1">a</font></span>)</span>,<span class="p1">(<span class="default"><a href="yellow21.html#K3" title="YELLOW21:func.3">latt</a> <font color="Olive" title="b2">b</font></span>)</span> holds <br/> ( <font color="Olive" title="b3">f</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">&lt;^</a><span class="default"><font color="Olive" title="b1">a</font>,<font color="Olive" title="b2">b</font></span><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">^&gt;</a></span> iff <font color="Maroon">P<sub>2</sub></font>[ <a href="yellow21.html#K3" title="YELLOW21:func.3">latt</a> <font color="Olive" title="b1">a</font>, <a href="yellow21.html#K3" title="YELLOW21:func.3">latt</a> <font color="Olive" title="b2">b</font>,<font color="Olive" title="b3">f</font>] )
