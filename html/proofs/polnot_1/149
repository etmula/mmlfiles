<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:149"/><span class="lab"><font color="Green" title="E94">A1</font></span>: 
<font color="Maroon" title="c6">J</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="polnot_1.html#K19" title="POLNOT_1:func.19">Polish-expression-hierarchy</a> (<font color="Maroon" title="c1">L</font>,<font color="Maroon" title="c2">E</font>,<font color="Maroon" title="c5">n</font>)
