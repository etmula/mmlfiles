<span class="kw">let </span><font color="Maroon" title="c11">F</font> be   <a href="polnot_1.html#NM6" title="POLNOT_1:NM.6">Polish-WFF</a> of <font color="Maroon" title="c1">L</font>,<font color="Maroon" title="c2">E</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c11">F</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c9">J1</font> implies <font color="Maroon" title="c10">H1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">g</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">L</font> <a href="polnot_1.html#K15" title="POLNOT_1:func.15">-head</a> <font color="Maroon" title="c11">F</font></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon" title="c7">H</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <span class="p3">(<span class="default"><a href="polnot_1.html#K33" title="POLNOT_1:func.33">Polish-WFF-args</a> <font color="Maroon" title="c11">F</font></span>)</span></span>)</span></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> )</span><br/>

<span class="kw">assume </span><a NAME="E1:147_2"/>
<font color="Maroon" title="c11">F</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c9">J1</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c10">H1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">g</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">L</font> <a href="polnot_1.html#K15" title="POLNOT_1:func.15">-head</a> <font color="Maroon" title="c11">F</font></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon" title="c7">H</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <span class="p3">(<span class="default"><a href="polnot_1.html#K33" title="POLNOT_1:func.33">Polish-WFF-args</a> <font color="Maroon" title="c11">F</font></span>)</span></span>)</span></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span></span><br/>

<span class="kw">then </span><span class="kw">consider </span><font color="Maroon" title="c12">G</font> being   <a href="polnot_1.html#NM6" title="POLNOT_1:NM.6">Polish-WFF</a> of <font color="Maroon" title="c1">L</font>,<font color="Maroon" title="c2">E</font><span class="kw"> such that </span><br/><a NAME="E3:147_2"/><span class="lab"><font color="Green" title="E95">A41</font></span>: 
<font color="Maroon" title="c12">G</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">F</font>
 <span class="kw">and </span><br/><a NAME="E4:147_2"/><span class="lab"><font color="Green" title="E96">A42</font></span>: 
<font color="Maroon" title="c10">H1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">g</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">L</font> <a href="polnot_1.html#K15" title="POLNOT_1:func.15">-head</a> <font color="Maroon" title="c12">G</font></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon" title="c7">H</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <span class="p3">(<span class="default"><a href="polnot_1.html#K33" title="POLNOT_1:func.33">Polish-WFF-args</a> <font color="Maroon" title="c12">G</font></span>)</span></span>)</span></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E6:147"><span class="lab"><font color="Green" title="E94">A21</font></span></a></span>;<br/>
<span class="kw">thus </span><a NAME="E5:147_2"/>
<font color="Maroon" title="c10">H1</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c11">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">g</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">L</font> <a href="polnot_1.html#K15" title="POLNOT_1:func.15">-head</a> <font color="Maroon" title="c11">F</font></span>)</span>,<span class="p2">(<span class="default"><font color="Maroon" title="c7">H</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <span class="p3">(<span class="default"><a href="polnot_1.html#K33" title="POLNOT_1:func.33">Polish-WFF-args</a> <font color="Maroon" title="c11">F</font></span>)</span></span>)</span></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:147_2"><span class="lab"><font color="Green" title="E95">A41</font></span></a>, <a class="txt" href="#E4:147_2"><span class="lab"><font color="Green" title="E96">A42</font></span></a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


