<span class="kw">let </span><font color="Maroon" title="c2">y</font> be    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <span class="p1"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,1</span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <a href="fuzimpl2.html#NK7" title="FUZIMPL2:NK.7">I_YG</a> <a href="binop_1.html#K1" title="BINOP_1:func.1">.</a> (1,<font color="Maroon" title="c2">y</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">y</font></span><br/>

