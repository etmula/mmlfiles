 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">L</font> is  <a href="lattices.html#V15" title="LATTICES:attr.15">bounded</a> </span><br/>

<a NAME="E2:30_1_1"/><span class="kw">then </span>
( <font color="Maroon" title="c1">L</font> is   <a href="lattices.html#NM4" title="LATTICES:NM.4">0_Lattice</a> &amp; <font color="Maroon" title="c1">L</font> is   <a href="lattices.html#NM5" title="LATTICES:NM.5">1_Lattice</a> )
 
