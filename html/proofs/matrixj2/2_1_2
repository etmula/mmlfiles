( ( <font color="Maroon" title="c4">M1</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">L</font> &amp; <font color="Maroon" title="c5">M2</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">L</font> ) or ( <font color="Maroon" title="c4">M1</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="group_1.html#K1" title="GROUP_1:func.1">1_</a> <font color="Maroon" title="c1">K</font> &amp; <font color="Maroon" title="c5">M2</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="group_1.html#K1" title="GROUP_1:func.1">1_</a> <font color="Maroon" title="c1">K</font> ) or ( <font color="Maroon" title="c4">M1</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">K</font> &amp; <font color="Maroon" title="c5">M2</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c6">i</font>,<font color="Maroon" title="c7">j</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">K</font> ) )
