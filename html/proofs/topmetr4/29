( <font color="Maroon" title="c6">G</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c3">F</font> &amp; <font color="Maroon" title="c6">G</font> is    <a href="setfam_1.html#M1" title="SETFAM_1:mode.1">Cover</a> of <font color="Maroon" title="c1">N</font> &amp; <font color="Maroon" title="c6">G</font> is  <a href="finset_1.html#V1" title="FINSET_1:attr.1">finite</a>  )
