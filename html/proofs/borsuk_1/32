<font color="Maroon" title="c9">Y1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="funct_3.html#K10" title="FUNCT_3:func.10">pr2</a> ( the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">X</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">Y</font>)</span>)</span> <a href="relset_1.html#K7" title="RELSET_1:func.7">.:</a> <font color="Maroon" title="c3">D</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c9">Y1</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">open</a> </span><br/>

<a NAME="E11:32"/><span class="kw">then </span>
<font color="Maroon" title="c9">Y1</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="setfam_1.html#K5" title="SETFAM_1:func.5">union</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="funct_3.html#K2" title="FUNCT_3:func.2">.:</a> <span class="p3">(<span class="default"><a href="funct_3.html#K10" title="FUNCT_3:func.10">pr2</a> ( the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">X</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c2">Y</font>)</span>)</span></span>)</span> <a href="relset_1.html#K7" title="RELSET_1:func.7">.:</a> <font color="Maroon" title="c7">K</font></span>)</span>
 
