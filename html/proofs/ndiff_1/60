<span class="kw">hence </span><a NAME="E6:60"/>
( <font color="Maroon" title="c5">N</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c3">f</font> &amp;  ex <font color="Olive" title="b1">R</font> being   <a href="ndiff_1.html#NM1" title="NDIFF_1:NM.1">RestFunc</a> of <font color="Maroon" title="c1">S</font>,<font color="Maroon" title="c2">T</font> st <br/>( <font color="Olive" title="b1">R</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <span class="p1">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">S</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c2">T</font> &amp; <font color="Olive" title="b1">R</font> <a href="nfcont_1.html#R1" title="NFCONT_1:pred.1">is_continuous_in</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">S</font> &amp; (  for <font color="Olive" title="b2">x</font> being   <a href="pre_topc.html#NM2" title="PRE_TOPC:NM.2">Point</a> of <font color="Maroon" title="c1">S</font>  st <font color="Olive" title="b2">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c5">N</font> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b2">x</font></span>)</span> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Maroon" title="c4">x0</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="ndiff_1.html#K3" title="NDIFF_1:func.3">diff</a> (<font color="Maroon" title="c3">f</font>,<font color="Maroon" title="c4">x0</font>)</span>)</span> <a href="lopban_1.html#K17" title="LOPBAN_1:func.17">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b2">x</font> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <font color="Maroon" title="c4">x0</font></span>)</span></span>)</span> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">R</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <span class="p2">(<span class="default"><font color="Olive" title="b2">x</font> <a href="algstr_0.html#K5" title="ALGSTR_0:func.5">-</a> <font color="Maroon" title="c4">x0</font></span>)</span></span>)</span> ) ) )
