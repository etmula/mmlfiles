<span class="kw">let </span><font color="Maroon" title="c1">n</font> be   <a href="ordinal1.html#NM6" title="ORDINAL1:NM.6">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">K</font> being   <a href="vectsp_1.html#NM5" title="VECTSP_1:NM.5">Field</a><br/>  for <font color="Olive" title="b2">M1</font>, <font color="Olive" title="b3">M2</font>, <font color="Olive" title="b4">M3</font> being   <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of <font color="Maroon" title="c1">n</font>,<font color="Olive" title="b1">K</font>  st <font color="Olive" title="b2">M1</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  &amp; <font color="Olive" title="b3">M2</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  &amp; <font color="Olive" title="b4">M3</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  holds <br/>( <span class="p1">(<span class="default"><font color="Olive" title="b2">M1</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Olive" title="b3">M2</font></span>)</span> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Olive" title="b4">M3</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  &amp; <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b2">M1</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Olive" title="b3">M2</font></span>)</span> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Olive" title="b4">M3</font></span>)</span> <a href="matrix_6.html#K1" title="MATRIX_6:func.1">~</a>  <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">