<span class="kw">hence </span><a NAME="E37:41_1_1"/>
 ex <font color="Olive">b<sub>1</sub></font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> st <br/>( <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default">1,<font color="Olive">b<sub>1</sub></font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="matrix_0.html#K2" title="MATRIX_0:func.2">Indices</a> <span class="p1">(<span class="default"><a href="goboard2.html#K2" title="GOBOARD2:func.2">GoB</a> <font color="Maroon" title="c1">g</font></span>)</span> &amp; <span class="p1">(<span class="default"><a href="goboard2.html#K2" title="GOBOARD2:func.2">GoB</a> <font color="Maroon" title="c1">g</font></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (1,<font color="Olive">b<sub>1</sub></font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="pscomp_1.html#K18" title="PSCOMP_1:func.18">W-min</a> <span class="p1">(<span class="default"><a href="topreal1.html#K3" title="TOPREAL1:func.3">L~</a> <font color="Maroon" title="c1">g</font></span>)</span> )
