<span class="kw">hence </span><a NAME="E4:115"/>
(  for <font color="Olive" title="b1">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">i</font> <a href="xxreal_0.html#NR2" title="XXREAL_0:NR.2">&gt;=</a> 1 holds <br/>( <font color="Maroon" title="c1">H</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  the <a href="modelc_3.html#U1" title="MODELC_3:sel.1">LTLold</a> of <span class="p1">(<span class="default"><a href="modelc_3.html#K10" title="MODELC_3:func.10">CastNode</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c3">q</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Olive" title="b1">i</font></span>)</span>,<font color="Maroon" title="c2">v</font>)</span>)</span> &amp;  <a href="modelc_2.html#K11" title="MODELC_2:func.11">the_left_argument_of</a> <font color="Maroon" title="c1">H</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  the <a href="modelc_3.html#U1" title="MODELC_3:sel.1">LTLold</a> of <span class="p1">(<span class="default"><a href="modelc_3.html#K10" title="MODELC_3:func.10">CastNode</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c3">q</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Olive" title="b1">i</font></span>)</span>,<font color="Maroon" title="c2">v</font>)</span>)</span> &amp;  not  <a href="modelc_2.html#K12" title="MODELC_2:func.12">the_right_argument_of</a> <font color="Maroon" title="c1">H</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  the <a href="modelc_3.html#U1" title="MODELC_3:sel.1">LTLold</a> of <span class="p1">(<span class="default"><a href="modelc_3.html#K10" title="MODELC_3:func.10">CastNode</a> (<span class="p2">(<span class="default"><font color="Maroon" title="c3">q</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Olive" title="b1">i</font></span>)</span>,<font color="Maroon" title="c2">v</font>)</span>)</span> ) or  ex <font color="Olive" title="b1">j</font> bein