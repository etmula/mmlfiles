<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c6">f</font> = <font color="Maroon" title="c5">f</font> as    <a href="prob_1.html#M2" title="PROB_1:mode.2">Probability</a> of <font color="Maroon" title="c2">Sigma</font> <span class="kw">by</span> <span class="lab"><a class="txt" href="#E7:31_1_1"><span class="lab"><font color="Green" title="E22">A9</font></span></a>, <a class="txt" href="#E5:31_1_1"><span class="lab"><font color="Green" title="E20">A6</font></span></a>, <a class="txt" href="#E6:31_1_1"><span class="lab"><font color="Green" title="E21">A7</font></span></a>, <a class="ref" href="prob_1.html#D8" onmouseover="rs('prob_1/D8')" onmouseout="rh()">PROB_1:def 8</a></span>;<br/>
<span class="kw">take </span>
<font color="Maroon" title="c6">f</font>
; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">A</font> being    <a href="prob_1.html#M1" title="PROB_1:mode.1">Event</a> of <font color="Maroon" title="c2">Sigma</font> holds  <font color="Maroon" title="c6">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b1">A</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">P</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p2">(<span class="default"><font color="Olive" title="b1">A</font> <a href="prob_1.html#K5" title="PROB_1:func.5">/\</a> <font color="Maroon" title="c4">B</font></span>)</span></span>)</span> <a href="real_1.html#K6" title="REAL_1:func.6">/</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">P</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">B</font></span>)</span></span><br/>

