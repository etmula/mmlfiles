( <font color="Maroon" title="c11">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="prob_1.html#K3" title="PROB_1:func.3">Intersection</a> <font color="Maroon" title="c7">A10</font> iff  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c11">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c5">A1</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Olive" title="b1">n</font> )
 
