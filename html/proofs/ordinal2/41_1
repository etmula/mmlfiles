<span class="kw">let </span><font color="Maroon" title="c4">A</font> be   <a href="ordinal1.html#NM3" title="ORDINAL1:NM.3">Ordinal</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c4">A</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon">F<sub>1</sub></font>() implies <font color="Maroon" title="c3">fi</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><a href="ordinal1.html#K1" title="ORDINAL1:func.1">succ</a> <font color="Maroon" title="c4">A</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon">F<sub>3</sub></font>(<font color="Maroon" title="c4">A</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c3">fi</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c4">A</font></span>)</span>) )</span><br/>

