(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <a href="pcs_0.html#G2" title="PCS_0:aggr.2">pcs-Str</a>(# <span class="p1">(<span class="default"><a href="card_3.html#K3" title="CARD_3:func.3">Union</a> <span class="p2">(<span class="default"><a href="pralg_1.html#K12" title="PRALG_1:func.12">Carrier</a> <font color="Maroon" title="c4">C</font></span>)</span></span>)</span>,<font color="Maroon" title="c11">IR</font>,<font color="Maroon" title="c12">TR</font> #) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="card_3.html#K3" title="CARD_3:func.3">Union</a> <span class="p1">(<span class="default"><a href="pralg_1.html#K12" title="PRALG_1:func.12">Carrier</a> <font color="Maroon" title="c4">C</font></span>)</span> &amp;  the <a href="orders_2.html#U1" title="ORDERS_2:sel.1">InternalRel</a> of <a href="pcs_0.html#G2" title="PCS_0:aggr.2">pcs-Str</a>(# <span class="p1">(<span class="default"><a href="card_3.html#K3" title="CARD_3:func.3">Union</a> <span class="p2">(<span class="default"><a href="pralg_1.html#K12" title="PRALG_1:func.12">Carrier</a> <font color="Maroon" title="c4">C</font></span>)</span></span>)</span>,<font color="Maroon" title="c11">IR</font>,<font color="Maroon" title="c12">TR</font> #) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="card_3.html#K3" title="CARD_3:func.3">Union</a> <span class="p1">(<span class="default"><a href="pcs_0.html#K4" title="PCS_0:func.4">pcs-InternalRels</a> <font color="Maroon" title="c4">C</font></span>)</span> &amp;  the <a href="pcs_0.html#U1" title="PCS_0:sel.1">ToleranceRel</a> of <a href="pcs_0.html#G2" title="PCS_0:aggr.2">pcs-Str</a>(# <span class="p1">(<span class="default"><a href="card_3.html#K3" title="CARD_3:func.3">Union</a> <span class="p2">(<span class="default"><a href="pralg_1.html#K12" title="PRALG_1:func.12">Carrier</a> <font color="Maroon" title="c4">C</font></span>)</span></span>)</span>,<font color="Maroon" title="c11">IR</font>,<font color="Maroon