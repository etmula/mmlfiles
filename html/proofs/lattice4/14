 for <font color="Olive" title="b1">p</font>, <font color="Olive" title="b2">q</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font>  st <font color="Olive" title="b1">p</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> &amp; <font color="Olive" title="b2">q</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> holds <br/><font color="Olive" title="b1">p</font> <a href="lattices.html#K4" title="LATTICES:func.4">"/\"</a> <font color="Olive" title="b2">q</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font>
 
;<br/>
<a NAME="E4:14"/>
 for <font color="Olive" title="b1">p</font>, <font color="Olive" title="b2">q</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font>  st <font color="Olive" title="b1">p</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> &amp; <font color="Olive" title="b2">q</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font> holds <br/><font color="Olive" title="b1">p</font> <a href="lattices.html#K3" title="LATTICES:func.3">"\/"</a> <font color="Olive" title="b2">q</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c2">F</font>
 
