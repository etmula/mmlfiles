<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="46_1_1_2_1"><span class="kw">proof </span></a><div class="add">

<span class="kw">set </span><font color="Maroon" title="c10">Q</font> =  the    <a href="lattice5.html#M1" title="LATTICE5:mode.1">QuadrSeq</a> of <font color="Maroon" title="c9">dq</font>;<br/>
<span class="kw">set </span><font color="Maroon" title="c11">AQ</font> =  <a href="lattice8.html#K7" title="LATTICE8:func.7">NextSet2</a> <font color="Maroon" title="c9">dq</font>;<br/>
