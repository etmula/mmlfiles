<a NAME="E2:78"/>
 <a href="hahnban1.html#K16" title="HAHNBAN1:func.16">prodReIm</a> <font color="Maroon" title="c2">l</font> is  <a href="vectsp_1.html#V13" title="VECTSP_1:attr.13">additive</a> 
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="78_2"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c3">x</font>, <font color="Maroon" title="c4">y</font> be   <a href="vectsp_1.html#NM8" title="VECTSP_1:NM.8">Vector</a> of <font color="Maroon" title="c1">V</font>; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="vectsp_1.html#D19" onmouseover="rs('vectsp_1/D19')" onmouseout="rh()">VECTSP_1:def 19</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <span class="p1">(<span class="default"><a href="hahnban1.html#K16" title="HAHNBAN1:func.16">prodReIm</a> <font color="Maroon" title="c2">l</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">x</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c4">y</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="hahnban1.html#K16" title="HAHNBAN1:func.16">prodReIm</a> <font color="Maroon" title="c2">l</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c3">x</font></span>)</span> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="hahnban1.html#K16" title="HAHNBAN1:func.16">prodReIm</a> <font color="Maroon" title="c2">l</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">y</font></span>)</span></span><br/>

<span class="kw">thus </span><span class="p1">(<span class="default"><a href="hahnban1.html#K16" title="HAHNBAN1:func.16">prodReIm</a> <font color="Maroon" title="c2">l</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">x</font> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <font color="Maroon" title="c4">y</font></span>)</span> = 
<span class="p1"><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">[**</a><span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="hahnban1.html#K13" title="HAHNBAN1:func.13">RtoC</a> <font color="Maroon" title="c2">l</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p3">(<span class="default"><font color="Maroon" title="c3">x</font> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <font color="Maroon" title="c4">y</font></span>)</span></span>)</span>,<span class="p2">(<span class="default"><a href="real_1.html#K1" title="REAL_1:func.1">-</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="hahnban1.html#K15" title="HAHNBAN1:func.15">i-shift</a> <span class="p5">(<span class="default"><a href="hahnban1.html#K13" title="HAHNBAN1:func.13">RtoC</a> <font color="Maroon" title="c2">l</font></span>)</span></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p4">(<span class="default"><font color="Maroon" title="c3">x</font> <a href="rlvect_1.html#K3" title="RLVECT_1:func.3">+</a> <font color="Maroon" title="c4">y</font></span>)</span></span>)</span></span>)</span></span><a href="hahnban1.html#K1" title="HAHNBAN1:func.1">**]</a></span>
