( <font color="Maroon" title="c2">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c3">A</font> <a href="fin_topo.html#K8" title="FIN_TOPO:func.8">^i</a>  iff  for <font color="Olive" title="b1">y</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">FT</font>  st  <a href="fintopo2.html#K3" title="FINTOPO2:func.3">P_0</a> (<font color="Maroon" title="c2">x</font>,<font color="Olive" title="b1">y</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  holds <br/> <a href="fintopo2.html#K1" title="FINTOPO2:func.1">P_1</a> (<font color="Maroon" title="c2">x</font>,<font color="Olive" title="b1">y</font>,<font color="Maroon" title="c3">A</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="margrel1.html#K7" title="MARGREL1:func.7">TRUE</a>  )
