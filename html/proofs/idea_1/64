 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <span class="p1">(<span class="default"><a href="idea_1.html#K18" title="IDEA_1:func.18">IDEA_Q_F</a> (<font color="Maroon" title="c3">Key</font>,<font color="Maroon" title="c1">n</font>,<span class="p2">(<span class="default"><font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <span class="p1">(<span class="default"><span class="p2"><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">&lt;*</a><span class="default"><span class="p3">(<span class="default"><a href="idea_1.html#K16" title="IDEA_1:func.16">IDEA_Q</a> (<span class="p4">(<span class="default"><a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<font color="Maroon" title="c3">Key</font>,<span class="p5">(<span class="default"><font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>)</span>)</span>,<font color="Maroon" title="c1">n</font>)</span>)</span></span><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">*&gt;</a></span> <a href="finseq_1.html#K7" title="FINSEQ_1:func.7">^</a> <span class="p2">(<span class="default"><a href="idea_1.html#K18" title="IDEA_1:func.18">IDEA_Q_F</a> (<font color="Maroon" title="c3">Key</font>,<font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c4">k</font>)</span>)</span></span>)</span>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="idea_1.html#D18" target="_self" onmouseover="rs('idea_1/D18')" onmouseout="rh()">Def18</a></span>;<br/>
<span class="kw">hence </span><a NAME="E4:64"/>
 <a href="idea_1.html#K18" title="IDEA_1:func.18">IDEA_Q_F</a> (<font color="Maroon" title="c3">Key</font>,<font color="Maroon" title="c1">n</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">&lt;*</a><span class="default"><span class="p2">(<span class="default"><a href="idea_1.html#K16" title="IDEA_1:func.16">IDEA_Q</a> (<span class="p3">(<span class="default"><a href="matrix_0.html#K7" title="MATRIX_0:func.7">Line</a> (<font color="Maroon" title="c3">Key</font>,<span class="p4">(<span class="default"><font color="Maroon" title="c4">k</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span>)</span>)</span>,<font color="Maroon" title="c1">n</font>)</span>)</span></span><a href="finseq_1.html#K9" title="FINSEQ_1:func.9">*&gt;</a></span> <a href="finseq_1.html#K7" title="FINSEQ_1:func.7">^</a> <span class="p1">(<span class="default"><a href="idea_1.html#K18" title="IDEA_1:func.18">IDEA_Q_F</a> (<font color="Maroon" title="c3">Key</font>,<font color="Maroon" title="c1">n</font>,<font color="Maroon" title="c4">k</font>)</span>)</span>
