<span class="kw">thus </span><a NAME="E8:60_1_1"/>
( <font color="Maroon" title="c5">p</font> is  <a href="qc_lang1.html#V4" title="QC_LANG1:attr.4">conjunctive</a>  implies <font color="Maroon" title="c4">F</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c5">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">F</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p2">(<span class="default"><a href="qc_lang1.html#K19" title="QC_LANG1:func.19">the_left_argument_of</a> <font color="Maroon" title="c5">p</font></span>)</span></span>)</span> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">F</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p2">(<span class="default"><a href="qc_lang1.html#K20" title="QC_LANG1:func.20">the_right_argument_of</a> <font color="Maroon" title="c5">p</font></span>)</span></span>)</span> )
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:60_1_1"><span class="lab"><font color="Green" title="E38">A1</font></span></a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c5">p</font> is  <a href="qc_lang1.html#V5" title="QC_LANG1:attr.5">universal</a>  implies <font color="Maroon" title="c4">F</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c5">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">F</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p2">(<span class="default"><a href="qc_lang1.html#K22" title="QC_LANG1:func.22">the_scope_of</a> <font color="Maroon" title="c5">p</font></span>)</span></span>)</span> <a href="subset_1.html#K7" title="SUBSET_1:func.7">\</a> <span class="p1"><a href="tarski.html#K1" title="TARSKI:func.1">{</a><span class="default"><span class="p2">(<span class="default"><a href="qc_lang1.html#K21" title="QC_LANG1:func.21">bound_in</a> <font color="Maroon" title="c5">p</font></span>)</span></span><a href="tarski.html#K1" title="TARSKI:func.1">}</a></span> )</span><br/>

<span class="kw">assume </span><a NAME="E9:60_1_1"/>
