 for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds <br/> (  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">g</font> <a href="mesfunc5.html#K4" title="MESFUNC5:func.4">.</a> <font color="Olive" title="b1">n</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfunc5.html#K4" title="MESFUNC5:func.4">.</a> <font color="Olive" title="b1">n</font></span>)</span> &amp; (  for <font color="Olive" title="b2">x</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c1">X</font>  st <font color="Olive" title="b2">x</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">g</font> <a href="mesfunc5.html#K4" title="MESFUNC5:func.4">.</a> <font color="Olive" title="b1">n</font></span>)</span> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c4">g</font> <a href="mesfunc5.html#K4" title="MESFUNC5:func.4">.</a> <font color="Olive" title="b1">n</font></span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b2">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="comseq_3.html#K7" title="COMSEQ_3:func.7">Re</a> <span class="p2">(<span class="default"><font color="Maroon" title="c2">f</font> <a href="mesfun7c.html#K9" title="MESFUN7C:func.9">#</a> <font color="Olive" title="b2">x</font></span>)</span></span>)</span> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">n</font> ) )
