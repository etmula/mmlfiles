<span class="kw">let </span><font color="Maroon" title="c1">f</font> be   <a href="funct_1.html#NM1" title="FUNCT_1:NM.1">Function</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c1">f</font> is   <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a> iff (  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c1">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">n</font> is  <a href="xreal_0.html#V1" title="XREAL_0:attr.1">real</a>  ) ) )</span><br/>

<span class="kw">thus </span><a NAME="E1:3"/>
( <font color="Maroon" title="c1">f</font> is   <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a> implies (  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c1">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">n</font> is  <a href="xreal_0.html#V1" title="XREAL_0:attr.1">real</a>  ) ) )
 <span class="kw">by</span> <span class="lab"><a class="ref" href="seq_1.html#T1" target="_self" onmouseover="rs('seq_1/T1')" onmouseout="rh()">Th1</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> (  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a> holds  <font color="Maroon" title="c1">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">n</font> is  <a href="xreal_0.html#V1" title="XREAL_0:attr.1">real</a>  ) implies <font color="Maroon" title="c1">f</font> is   <a href="seq_1.html#NM1" title="SEQ_1:NM.1">Real_Sequence</a> )</span><br/>

<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E2:3"/><span class="lab"><font color="Green" title="E3">A1</font></span>: 
 <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> 
 <span class="kw">and </span><br/><a NAME="E3:3"/><span class="lab"><font color="Green" title="E4">A2</font></span>: 
