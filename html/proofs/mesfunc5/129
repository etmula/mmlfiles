<span class="kw">by</span> <span class="lab"><a class="txt" href="#E12:129"><span class="lab"><font color="Green" title="E116">A9</font></span></a>, <a class="txt" href="#E14:129"><span class="lab"><font color="Green" title="E118">A11</font></span></a>, <a class="ref" href="mesfunc3.html#T10" onmouseover="rs('mesfunc3/T10')" onmouseout="rh()">MESFUNC3:10</a></span>
<br/>.= 
<font color="Maroon" title="c10">rr</font> <a href="extreal1.html#K1" title="EXTREAL1:func.1">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">M</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <span class="p2">(<span class="default"><a href="tarski.html#K3" title="TARSKI:func.3">union</a> <span class="p3">(<span class="default"><a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Maroon" title="c7">F</font></span>)</span></span>)</span></span>)</span>
<span class="kw">by</span> <span class="lab"><a class="ref" href="mesfunc3.html#T9" onmouseover="rs('mesfunc3/T9')" onmouseout="rh()">MESFUNC3:9</a></span>
;<br/>
<span class="kw">hence </span><a NAME="E18:129"/>
 <a href="mesfunc3.html#K1" title="MESFUNC3:func.1">integral</a> (<font color="Maroon" title="c4">M</font>,<font color="Maroon" title="c5">f</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c6">r</font> <a href="xxreal_3.html#K4" title="XXREAL_3:func.4">*</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">M</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <span class="p2">(<span class="default"><a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c5">f</font></span>)</span></span>)</span>
