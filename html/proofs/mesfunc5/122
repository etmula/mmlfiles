<span class="p1">(<span class="default"><a href="mesfunc5.html#K6" title="MESFUNC5:func.6">integral+</a> (<font color="Maroon" title="c4">M</font>,<span class="p2">(<span class="default"><a href="mesfunc2.html#K1" title="MESFUNC2:func.1">max+</a> <font color="Maroon" title="c5">f</font></span>)</span>)</span>)</span> <a href="supinf_2.html#K4" title="SUPINF_2:func.4">-</a> <span class="p1">(<span class="default"><a href="mesfunc5.html#K6" title="MESFUNC5:func.6">integral+</a> (<font color="Maroon" title="c4">M</font>,<span class="p2">(<span class="default"><a href="mesfunc2.html#K2" title="MESFUNC2:func.2">max-</a> <font color="Maroon" title="c5">f</font></span>)</span>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c7">maxf1</font> <a href="real_1.html#K5" title="REAL_1:func.5">-</a> <font color="Maroon" title="c8">maxf2</font>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="supinf_2.html#T3" onmouseover="rs('supinf_2/T3')" onmouseout="rh()">SUPINF_2:3</a></span>;<br/>
<span class="kw">hence </span><a NAME="E17:122"/>
(  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="mesfunc5.html#K6" title="MESFUNC5:func.6">integral+</a> (<font color="Maroon" title="c4">M</font>,<span class="p1">(<span class="default"><a href="mesfunc2.html#K1" title="MESFUNC2:func.1">max+</a> <font color="Maroon" title="c5">f</font></span>)</span>) &amp;  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="mesfunc5.html#K6" title="MESFUNC5:func.6">integral+</a> (<font color="Maroon" title="c4">M</font>,<span class="p1">(<span class="default"><a href="mesfunc2.html#K2" title="MESFUNC2:func.2">max-</a> <font color="Maroon" title="c5">f</font></span>)</span>) &amp;  <a href="supinf_1.html#K2" title="SUPINF_1:func.2">-infty</a>  <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a>  <a href="mesfunc5.html#K7" title="MESFUNC5:func.7">Integral</a> (<font color="Maroon" title="c4">M</font>,<font color="Maroon" title="c5">f</font>) &amp;  <a href="mesfunc5.html#K7" title="MESFUNC5:func.7">Integral</a> (<font color="Maroon" title="c4">M</font>,<font color="Maroon" title="c5">f</font>) <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a>  <a href="supinf_1.html#K1" title="SUPINF_1:func.1">+infty</a>  )
