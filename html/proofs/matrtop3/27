<span class="kw">let </span><font color="Maroon" title="c1">r</font> be   <a href="xreal_0.html#NM1" title="XREAL_0:NM.1">Real</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">i</font>, <font color="Olive" title="b2">j</font>, <font color="Olive" title="b3">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">i</font> &amp; <font color="Olive" title="b1">i</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Olive" title="b2">j</font> &amp; <font color="Olive" title="b2">j</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b3">n</font> holds <br/> <a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <span class="p1">(<span class="default"><a href="matrtop3.html#K2" title="MATRTOP3:func.2">Rotation</a> (<font color="Olive" title="b1">i</font>,<font color="Olive" title="b2">j</font>,<font color="Olive" title="b3">n</font>,<font color="Maroon" title="c1">r</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a></span><br/><span class="kw">let </span><font color="Maroon" title="c2">i</font>, <font color="Maroon" title="c3">j</font>, <font color="Maroon" title="c4">n</font> be   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c2">