<span class="kw">assume </span><a NAME="E1:27"/><span class="lab"><font color="Green" title="E20">A1</font></span>: 
( 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c2">i</font> &amp; <font color="Maroon" title="c2">i</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> <font color="Maroon" title="c3">j</font> &amp; <font color="Maroon" title="c3">j</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c4">n</font> )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <span class="p1">(<span class="default"><a href="matrtop3.html#K2" title="MATRTOP3:func.2">Rotation</a> (<font color="Maroon" title="c2">i</font>,<font color="Maroon" title="c3">j</font>,<font color="Maroon" title="c4">n</font>,<font color="Maroon" title="c1">r</font>)</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a></span><br/>

<span class="kw">then </span><span class="kw">consider </span><font color="Maroon" title="c5">A</font> being   <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of <font color="Maroon" title="c4">n</font>,<a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a><span class="kw"> such that </span><br/><a NAME="E3:27"/><span class="lab"><font color="Green" title="E21">A2</font></span>: 
