<span class="kw">consider </span><font color="Maroon" title="c3">M</font> being   <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of <font color="Maroon" title="c1">n</font>,<a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a><span class="kw"> such that </span><br/><a NAME="E2:14_1_1"/><span class="lab"><font color="Green" title="E7">A2</font></span>: 
 <a href="matrix_3.html#K12" title="MATRIX_3:func.12">Det</a> <font color="Maroon" title="c3">M</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="algstr_0.html#K4" title="ALGSTR_0:func.4">-</a> <span class="p1">(<span class="default"><a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a></span>)</span>
 <span class="kw">and </span><br/><a NAME="E3:14_1_1"/><span class="lab"><font color="Green" title="E8">A3</font></span>: 
( <font color="Maroon" title="c3">M</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Maroon" title="c2">i</font>,<font color="Maroon" title="c2">i</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="algstr_0.html#K4" title="ALGSTR_0:func.4">-</a> <span class="p1">(<span class="default"><a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a></span>)</span> &amp; (  for <font color="Olive" title="b1">k</font>, <font color="Olive" title="b2">m</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <span class="p1"><a href="tarski.html#K4" title="TARSKI:func.4">[</a><span class="default"><font color="Olive" title="b1">k</font>,<font color="Olive" title="b2">m</font></span><a href="tarski.html#K4" title="TARSKI:func.4">]</a></span> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="matrix_0.html#K2" title="MATRIX_0:func.2">Indices</a> <font color="Maroon" title="c3">M</font> holds <br/>( ( <font color="Olive" title="b1">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">m</font> &amp; <font color="Olive" title="b1">k</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> <font color="Maroon" title="c2">i</font> implies <font color="Maroon" title="c3">M</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">k</font>,<font color="Olive" title="b1">k</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K5" title="STRUCT_0:func.5">1.</a> <a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a> ) &amp; ( <font color="Olive" title="b1">k</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a> <font color="Olive" title="b2">m</font> implies <font color="Maroon" title="c3">M</font> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (<font color="Olive" title="b1">k</font>,<font color="Olive" titl