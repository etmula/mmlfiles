<span class="kw">let </span><font color="Maroon" title="c3">x</font>, <font color="Maroon" title="c4">y</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">f1</font> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<font color="Maroon" title="c3">x</font>,<font color="Maroon" title="c4">y</font>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">f2</font> <a href="binop_1.html#K2" title="BINOP_1:func.2">.</a> (<font color="Maroon" title="c3">x</font>,<font color="Maroon" title="c4">y</font>)</span><br/>

<span class="kw">consider </span><font color="Maroon" title="c5">x1</font>, <font color="Maroon" title="c6">x2</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> <span class="kw"> such that </span><br/><a NAME="E2:44_1_2_1"/><span class="lab"><font color="Green" title="E32">A4</font></span>: 
<font color="Maroon" title="c3">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Maroon" title="c5">x1</font>,<font color="Maroon" title="c6">x2</font></span><a href="domain_1.html#K1" title="DOMAIN_1:func.1">]</a></span>
