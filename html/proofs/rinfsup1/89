 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c2">r</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <font color="Maroon" title="c3">p</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="rinfsup1.html#K1" title="RINFSUP1:func.1">upper_bound</a> <span class="p1">(<span class="default"><a href="rinfsup1.html#K3" title="RINFSUP1:func.3">inferior_realsequence</a> <font color="Maroon" title="c1">seq</font></span>)</span> &amp;  <a href="rinfsup1.html#K2" title="RINFSUP1:func.2">lower_bound</a> <span class="p1">(<span class="default"><a href="rinfsup1.html#K4" title="RINFSUP1:func.4">superior_realsequence</a> <font color="Maroon" title="c1">seq</font></span>)</span> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c2">r</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c3">p</font> )</span><br/>

