<span class="kw">thus </span><a NAME="E1:79_2"/>
( <font color="Maroon" title="c9">C</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">independent</a>  &amp; <font color="Maroon" title="c9">C</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c2">A</font> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <font color="Maroon" title="c3">B</font> )
 <span class="kw">by</span> <span class="lab"><a class="ref" href="xboole_1.html#T36" onmouseover="rs('xboole_1/T36')" onmouseout="rh()">XBOOLE_1:36</a></span>; <span class="comment"><font color="firebrick">:: according to </font></span><a class="ref" href="matroid0.html#D10" target="_self" onmouseover="rs('matroid0/D10')" onmouseout="rh()">MATROID0:def 10</a> <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">B</font> being   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">M</font>  st <font color="Olive" title="b1">B</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">independent</a>  &amp; <font color="Olive" title="b1">B</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c2">A</font> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <font color="Maroon" title="c3">B</font> &amp; <font color="Maroon" title="c9">C</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Olive" title="b1">B</font> holds <br/><font color="Maroon" title="c9">C</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">B</font></span><br/>

<span class="kw">let </span><font color="Maroon" title="c12">D</font> be   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <font color="Maroon" title="c1">M</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c12">D</font> is  <a href="pre_topc.html#V3" title="PRE_TOPC:attr.3">independent</a>  &amp; <font color="Maroon" title="c12">D</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c2">A</font> <a href="subset_1.html#K4" title="SUBSET_1:func.4">\/</a> <font color="Maroon" title="c3">B</font> &amp; <font color="Maroon" title="c9">C</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c12">D</font> implies <font color="Maroon" title="c9">C</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c12">D</font> )</span><br/>

