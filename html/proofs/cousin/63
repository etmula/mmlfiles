<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:63"/><span class="lab"><font color="Green" title="E42">A1</font></span>: 
<font color="Maroon" title="c1">r</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c2">s</font>
 <span class="kw">and </span><br/><a NAME="E2:63"/><span class="lab"><font color="Green" title="E43">A2</font></span>: 
<font color="Maroon" title="c4">S</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><span class="p3"><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">].</a><span class="default"><span class="p4">(<span class="default"><font color="Olive" title="b1">x</font> <a href="xcmplx_0.html#K6" title="XCMPLX_0:func.6">-</a> <span class="p5">(<span class="default"><font color="Maroon" title="c3">jauge</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">x</font></span>)</span></span>)</span>,<span class="p4">(<span class="default"><font color="Olive" title="b1">x</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p5">(<span class="default"><font color="Maroon" title="c3">jauge</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">x</font></span>)</span></span>)</span></span><a href="rcomp_1.html#K2" title="RCOMP_1:func.2">.[</a></span> <a href="subset_1.html#K9" title="SUBSET_1:func.9">/\</a> <span class="p3"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font></span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span></span>)</span> where <font color="Olive" title="b1">x</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <span class="p1"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font></span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span> : verum </span> } </span> 
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  for <font color="Olive" title="b1">IC</font> being    <a href="rcomp_3.html#M1" title="RCOMP_3:mode.1">IntervalCover</a> of <font color="Maroon" title="c4">S</font> holds <br/> ( <font color="Olive" title="b1">IC</font> is    <a href="finseq_1.html#M2" title="FINSEQ_1:mode.2">FinSequence</a> of  <a href="zfmisc_1.html#K1" title="ZFMISC_1:func.1">bool</a> <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> &amp;  <a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Olive" title="b1">IC</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c4">S</font> &amp;  <a href="tarski.html#K3" title="TARSKI:func.3">union</a> <span class="p1">(<span class="default"><a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Olive" title="b1">IC</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">[.</a><span class="default"><font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font></span><a href="rcomp_1.html#K1" title="RCOMP_1:func.1">.]</a></span> &amp; (  for <font color="Olive" title="b2">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st 1 <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b2">n</font> holds <br/>( ( <font color="Olive" title="b2">n</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a>  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Olive" title="b1">IC</font> implies  not <font color="Olive" title="b1">IC</font> <a href="partfun1.html#K7" title="PARTFUN1:func.7">/.</a> <font color="Olive" title="b2">n</font> is  <a href="xboole_0.html#V1" title="XBOOLE_0:attr.1">e