( <font color="Maroon" title="c4">S</font> is   <a href="struct_0.html#NM3" title="STRUCT_0:NM.3">Subset-Family</a> of <span class="p1">(<span class="default"><a href="topmetr.html#K4" title="TOPMETR:func.4">Closed-Interval-TSpace</a> (<font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font>)</span>)</span> &amp; <font color="Maroon" title="c4">S</font> is   <a href="struct_0.html#NM13" title="STRUCT_0:NM.13">Cover</a> of <span class="p1">(<span class="default"><a href="topmetr.html#K4" title="TOPMETR:func.4">Closed-Interval-TSpace</a> (<font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font>)</span>)</span> &amp; <font color="Maroon" title="c4">S</font> is  <a href="tops_2.html#V1" title="TOPS_2:attr.1">open</a>  &amp; <font color="Maroon" title="c4">S</font> is  <a href="rcomp_3.html#V1" title="RCOMP_3:attr.1">connected</a>  &amp; <font color="Maroon" title="c1">r</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Maroon" title="c2">s</font> )
 
