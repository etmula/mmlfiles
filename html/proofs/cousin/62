<span class="kw">let </span><font color="Maroon" title="c5">X</font> be   <a href="struct_0.html#NM2" title="STRUCT_0:NM.2">Subset</a> of <span class="p1">(<span class="default"><a href="topmetr.html#K4" title="TOPMETR:func.4">Closed-Interval-TSpace</a> (<font color="Maroon" title="c1">r</font>,<font color="Maroon" title="c2">s</font>)</span>)</span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c5">X</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c4">S</font> implies <font color="Maroon" title="c5">X</font> is  <a href="connsp_1.html#V2" title="CONNSP_1:attr.2">connected</a>  )</span><br/>

