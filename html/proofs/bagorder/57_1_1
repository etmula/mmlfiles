<span class="kw">thus </span><a NAME="E9:57_1_1"/>
(  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c6">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>  &amp; <font color="Maroon" title="c6">f</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <a href="numbers.html#K5" title="NUMBERS:func.5">0</a> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <span class="p2"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><font color="Olive" title="b1">x</font>,<font color="Olive" title="b2">y</font></span><a href="domain_1.html#K1" title="DOMAIN_1:func.1">]</a></span> where <font color="Olive" title="b1">x</font>, <font color="Olive" title="b2">y</font> is    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="finsub_1.html#K5" title="FINSUB_1:func.5">Fin</a>  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">R</font> : ( <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  or ( <font color="Olive" title="b1">x</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  &amp; <font color="Olive" title="b2">y</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  &amp;  <a href="bagorder.html#K11" title="BAGORDER:func.11">PosetMax</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="bagorder.html#K11" title="BAGORDER:func.11">PosetMax</a> <font color="Olive" title="b2">y</font> &amp; <span class="p1"><a href="domain_1.html#K1" title="DOMAIN_1:func.1">[</a><span class="default"><span class="p2">(<span class="default"><a href="bagorder.html#K11" title="BAGORDER:func.11">Pos