<a NAME="E1:3"/><span class="lab"><font color="Green" title="E4">A1</font></span>: 
 <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c5">F</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Maroon" title="c5">F</font>
 
;<br/>
<span class="kw">assume </span><a NAME="E2:3"/>
( <font color="Maroon" title="c3">f</font> <a href="mesfunc2.html#R1" title="MESFUNC2:pred.1">is_simple_func_in</a> <font color="Maroon" title="c2">S</font> &amp;  <a href="relat_1.html#NK1" title="RELAT_1:NK.1">dom</a> <font color="Maroon" title="c3">f</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  &amp; <font color="Maroon" title="c3">f</font> is  <a href="supinf_2.html#V6" title="SUPINF_2:attr.6">nonnegative</a>  &amp; <font color="Maroon" title="c5">F</font>,<font color="Maroon" title="c6">a</font> <a href="mesfunc3.html#R1" title="MESFUNC3:pred.1">are_Re-presentation_of</a> <font color="Maroon" title="c3">f</font> &amp;  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c7">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c5">F</font> &amp; (  for <font color="Olive" title="b1">n</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">n</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c7">x</font> holds <br/><font color="Maroon" title="c7">x</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c6">a</font> <a href="supinf_2.html#K12" title="SUPINF_2:func.12">.</a> <font color="Olive" title="b1">n</font></span>)</span> <a href="extreal1.html#K1" title="EXTREAL1:func.1">*</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c4">M</font> <a href="partfun1.html#K1" title="PARTFUN1:func.1">*</a> <font color="Maroon" title="c5">F</font></span>)</span> <a href="supinf_2.html#K12" tit