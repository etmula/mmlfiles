 ex <font color="Olive" title="b1">f</font> being   <a href="group_6.html#NM2" title="GROUP_6:NM.2">Homomorphism</a> of <span class="p1">(<span class="default"><a href="group_7.html#K2" title="GROUP_7:func.2">product</a> <font color="Maroon" title="c3">F</font></span>)</span>,<span class="p1">(<span class="default"><a href="group_7.html#K2" title="GROUP_7:func.2">product</a> <font color="Maroon" title="c2">G</font></span>)</span> st <br/>( <font color="Olive" title="b1">f</font> is  <a href="funct_2.html#V3" title="FUNCT_2:attr.3">bijective</a>  &amp; (  for <font color="Olive" title="b2">x</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="group_7.html#K2" title="GROUP_7:func.2">product</a> <font color="Maroon" title="c3">F</font></span>)</span>  ex <font color="Olive" title="b3">s</font> being   <a href="struct_0.html#NM7" title="STRUCT_0:NM.7">FinSequence</a> of <span class="p1">(<span class="default"><a href="group_7.html#K2" title="GROUP_7:func.2">product</a> <font color="Maroon" title="c2">G</font></span>)</span> st <br/>(  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Olive" title="b3">s</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c1">n</font> &amp; (  for <font color="Olive" title="b4">k</font> being    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of  <a href="finseq_1.html#K2" title="FINSEQ_1:func.2">Seg</a> <font color="Maroon" title="c1">n</font> holds  <font color="Olive" title="b3">s</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b4">k</font> <a href="struct_0.html#R1" title="STRUCT_0:pred.1">in</a> <font color="Maroon" title="c3">F</font> <a href="group_7.html#K1" title="GROUP_7:func.1">.</a> <font color="Olive" title="b4">k</font> ) &amp; <font color="Olive" title="b3">s</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b2">x</font> &amp; <font color="Olive" title="b1">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Olive" title="b2">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="group_4.html#K3" title="GROUP_4:func.3">Product</a> <font color="Olive" title="b3">s</font> ) ) )
