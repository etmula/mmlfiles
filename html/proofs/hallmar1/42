 for <font color="Olive" title="b1">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a>   st <font color="Olive" title="b1">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c2">A</font> holds <br/> <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">f9</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b1">i</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1
 
<div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="42_3"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c6">i</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c6">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c2">A</font> implies  <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">f9</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">i</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 )</span><br/>

<span class="kw">assume </span><a NAME="E1:42_3"/><span class="lab"><font color="Green" title="E33">A5</font></span>: 
<font color="Maroon" title="c6">i</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a> <font color="Maroon" title="c2">A</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="card_1.html#K4" title="CARD_1:func.4">card</a> <span class="p1">(<span class="default"><font color="Maroon" title="c5">f9</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">i</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1</span><br/>

<a NAME="E2:42_3"/><span class="kw">then </span>
( <font color="Maroon" title="c4">f</font> is    <a href="hallmar1.html#M3" title="HALLMAR1:mode.3">Singlification</a> of <font color="Maroon" title="c2">A</font>,<font color="Maroon" title="c6">i</font> &amp; <font color="Maroon" title="c2">A</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c6">i</font> <a href="hidden.html#NR2" title="HIDDEN:NR.2">&lt;&gt;</a>  <a href="xboole_0.html#K1" title="XBOOLE_0:func.1">{}</a>  )
 
