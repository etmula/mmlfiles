<font color="Maroon" title="c5">z</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c2">a</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c6">x</font>
 <span class="kw">and </span><br/><a NAME="E6:6_2"/><span class="lab"><font color="Green" title="E12">A10</font></span>: 
<font color="Maroon" title="c6">x</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a> <font color="Maroon" title="c1">X</font>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:6_2"><span class="lab"><font color="Green" title="E10">A8</font></span></a>, <a class="ref" href="member_1.html#T195" onmouseover="rs('member_1/T195')" onmouseout="rh()">MEMBER_1:195</a></span>;<br/>
<span class="kw">reconsider </span><font color="Maroon" title="c7">x</font> = <font color="Maroon" title="c6">x</font> as    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>  <span class="kw">by</span> <span class="lab"><a class="txt" href="#E6:6_2"><span class="lab"><font color="Green" title="E12">A10</font></span></a>, <a class="ref" href="xreal_0.html#D1" onmouseover="rs('xreal_0/D1')" onmouseout="rh()">XREAL_0:def 1</a></span>;<br/>
<a NAME="E8:6_2"/>
<font color="Maroon" title="c3">f</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c7">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c5">z</font>
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E2:6"><span class="lab"><font color="Green" title="E7">A1</font></span></a>, <a class="txt" href="#E6:6_2"><span class="lab"><font color="Green" title="E12">A10</font></span></a>, <a class="txt" href="#E5:6_2"><span class="lab"><font color="Green" title="E11">A9</font></span></a></span>;<br/>
<span class="kw">hence </span><a NAME="E9:6_2"/>
<font color="Maroon" title="c4">z</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="xtuple_0.html#K10" title="XTUPLE_0:func.10">rng</a> <font color="Maroon" title="c3">f</font>
