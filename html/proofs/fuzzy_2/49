<font color="Maroon" title="c2">C</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="fuzzy_2.html#K3" title="FUZZY_2:func.3">++</a> <span class="p2">(<span class="default"><a href="fuzzy_1.html#K1" title="FUZZY_1:func.1">min</a> (<font color="Maroon" title="c5">g</font>,<font color="Maroon" title="c4">h</font>)</span>)</span></span>)</span>
 
<span class="kw">by</span> <span class="lab"><a class="ref" href="funct_2.html#D1" onmouseover="rs('funct_2/D1')" onmouseout="rh()">FUNCT_2:def 1</a></span>;<br/>
<span class="kw">hence </span><a NAME="E4:49"/>
<font color="Maroon" title="c3">f</font> <a href="fuzzy_2.html#K3" title="FUZZY_2:func.3">++</a> <span class="p1">(<span class="default"><a href="fuzzy_1.html#K1" title="FUZZY_1:func.1">min</a> (<font color="Maroon" title="c5">g</font>,<font color="Maroon" title="c4">h</font>)</span>)</span> <a href="funct_2.html#R2" title="FUNCT_2:pred.2">=</a>  <a href="fuzzy_1.html#K1" title="FUZZY_1:func.1">min</a> (<span class="p1">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="fuzzy_2.html#K3" title="FUZZY_2:func.3">++</a> <font color="Maroon" title="c5">g</font></span>)</span>,<span class="p1">(<span class="default"><font color="Maroon" title="c3">f</font> <a href="fuzzy_2.html#K3" title="FUZZY_2:func.3">++</a> <font color="Maroon" title="c4">h</font></span>)</span>)
