<span class="kw">let </span><font color="Maroon" title="c1">f</font> be   <a href="funct_2.html#NM1" title="FUNCT_2:NM.1">Function</a> of <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>,<a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c1">f</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="partfun1.html#K6" title="PARTFUN1:func.6">id</a> <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> implies <font color="Maroon" title="c1">f</font> is  <a href="fdiff_1.html#V4" title="FDIFF_1:attr.4">differentiable</a>  )</span><br/>

<span class="kw">assume </span><a NAME="E1:17_1_1"/>
<font color="Maroon" title="c1">f</font> <a href="funct_2.html#R2" title="FUNCT_2:pred.2">=</a>  <a href="partfun1.html#K6" title="PARTFUN1:func.6">id</a> <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c1">f</font> is  <a href="fdiff_1.html#V4" title="FDIFF_1:attr.4">differentiable</a> </span><br/>

<a NAME="E2:17_1_1"/><span class="kw">then </span>
<font color="Maroon" title="c1">f</font> <a href="fdiff_1.html#R2" title="FDIFF_1:pred.2">is_differentiable_on</a>  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> 
 
<span class="kw">by</span> <span class="lab"><a class="txt" href="polydiff.html#E13"><span class="lab"><font color="Green" title="E9">Lm1</font></span></a>, <a class="ref" href="fdiff_1.html#T17" onmouseover="rs('fdiff_1/T17')" onmouseout="rh()">FDIFF_1:17</a></span>;<br/>
<span class="kw">hence </span><a NAME="E3:17_1_1"/>
<font color="Maroon" title="c1">f</font> is  <a href="fdiff_1.html#V4" title="FDIFF_1:attr.4">differentiable</a> 
