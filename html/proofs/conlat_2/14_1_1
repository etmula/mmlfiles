<span class="kw">let </span><font color="Maroon" title="c3">o</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">C</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  ex <font color="Olive" title="b1">O</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">C</font> ex <font color="Olive" title="b2">A</font> being   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of  the <a href="struct_0.html#U4" title="STRUCT_0:sel.4">carrier'</a> of <font color="Maroon" title="c1">C</font> st <br/>( <font color="Maroon" title="c2">f</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c3">o</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="conlat_1.html#G2" title="CONLAT_1:aggr.2">ConceptStr</a>(# <font color="Olive" title="b1">O</font>,<font color="Olive" title="b2">A</font> #) &amp; <font color="Olive" title="b1">O</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="conlat_1.html#K2" title="CONLAT_1:func.2">AttributeDerivation</a> <font color="Maroon" title="c1">C</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="conlat_1.html#K1" title="CONLAT_1:func.1">ObjectDerivation</a> <font color="Maroon" title="c1">C</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p2"><a href="domain_1.html#K6" title="DOMAIN_1:func.6">{</a><span class="default"><font color="Maroon" title="c3">o</font></span><a href="domain_1.html#K6" title="DOMAIN_1:func.6">}</a></span></s